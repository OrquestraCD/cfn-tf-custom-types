{
    "typeName": "Terraform::VSphere::VappContainer",
    "description": "CloudFormation equivalent of vsphere_vapp_container",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CpuExpandable": {
            "type": "boolean"
        },
        "CpuLimit": {
            "type": "number"
        },
        "CpuReservation": {
            "type": "number"
        },
        "CpuShareLevel": {
            "type": "string"
        },
        "CpuShares": {
            "type": "number"
        },
        "CustomAttributes": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "Id": {
            "type": "string"
        },
        "MemoryExpandable": {
            "type": "boolean"
        },
        "MemoryLimit": {
            "type": "number"
        },
        "MemoryReservation": {
            "type": "number"
        },
        "MemoryShareLevel": {
            "type": "string"
        },
        "MemoryShares": {
            "type": "number"
        },
        "Name": {
            "type": "string"
        },
        "ParentFolderId": {
            "type": "string"
        },
        "ParentResourcePoolId": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "ParentResourcePoolId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}