{
    "typeName": "Terraform::VSphere::ContentLibrary",
    "description": "The `vsphere_content_library` resource can be used to manage Content Libraries.\n\n~> **NOTE:** This resource requires vCenter and is not available on direct ESXi\nconnections.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/vsphere/Terraform-VSphere-ContentLibrary/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "A description of the Content Library."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name of the Content Library."
        },
        "StorageBacking": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
