{
    "typeName": "Terraform::FlexibleEngine::DwsClusterV1",
    "description": "Manages a DWS cluster resource within FlexibleEngine",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AvailabilityZone": {
            "type": "string",
            "description": "AZ in a cluster."
        },
        "Created": {
            "type": "string"
        },
        "Endpoints": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "ConnectInfo": {
                        "type": "string"
                    },
                    "JdbcUrl": {
                        "type": "string"
                    }
                }
            }
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "Cluster name, which must be unique and contains 4 to 64\ncharacters, which consist of letters, digits, hyphens (-), or underscores\n(_) only and must start with a letter."
        },
        "NodeType": {
            "type": "string",
            "description": "Node type."
        },
        "NumberOfNode": {
            "type": "number",
            "description": "Number of nodes in a cluster. The value ranges\nfrom 3 to 32."
        },
        "Port": {
            "type": "number",
            "description": "Service port of a cluster (8000 to 10000). The default\nvalue is 8000."
        },
        "PublicEndpoints": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "JdbcUrl": {
                        "type": "string"
                    },
                    "PublicConnectInfo": {
                        "type": "string"
                    }
                }
            }
        },
        "Region": {
            "type": "string"
        },
        "SecurityGroupId": {
            "type": "string",
            "description": "ID of a security group. The ID is used for\nconfiguring cluster network."
        },
        "Status": {
            "type": "string"
        },
        "SubStatus": {
            "type": "string"
        },
        "SubnetId": {
            "type": "string",
            "description": "Subnet ID, which is used for configuring cluster\nnetwork."
        },
        "TaskStatus": {
            "type": "string"
        },
        "Updated": {
            "type": "string"
        },
        "UserName": {
            "type": "string",
            "description": "Administrator username for logging in to a data\nwarehouse cluster The administrator username must:."
        },
        "UserPwd": {
            "type": "string",
            "description": "Administrator password for logging in to a data\nwarehouse cluster."
        },
        "Version": {
            "type": "string"
        },
        "VpcId": {
            "type": "string",
            "description": "VPC ID, which is used for configuring cluster network."
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "NodeType",
        "NumberOfNode",
        "SecurityGroupId",
        "SubnetId",
        "UserName",
        "UserPwd",
        "VpcId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Created",
        "/properties/Endpoints",
        "/properties/Id",
        "/properties/PublicEndpoints",
        "/properties/Status",
        "/properties/SubStatus",
        "/properties/TaskStatus",
        "/properties/Updated",
        "/properties/Version"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}