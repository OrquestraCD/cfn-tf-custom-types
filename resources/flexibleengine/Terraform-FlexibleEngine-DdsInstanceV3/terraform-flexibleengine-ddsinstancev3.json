{
    "typeName": "Terraform::FlexibleEngine::DdsInstanceV3",
    "description": "CloudFormation equivalent of flexibleengine_dds_instance_v3",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "BackupStrategy": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "KeepDays": {
                    "type": "number"
                },
                "StartTime": {
                    "type": "string"
                }
            },
            "required": [
                "KeepDays",
                "StartTime"
            ]
        },
        "Datastore": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "StorageEngine": {
                    "type": "string"
                },
                "Type": {
                    "type": "string"
                },
                "Version": {
                    "type": "string"
                }
            },
            "required": [
                "Type",
                "Version"
            ]
        },
        "Flavor": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Num": {
                    "type": "number"
                },
                "Size": {
                    "type": "number"
                },
                "SpecCode": {
                    "type": "string"
                },
                "Storage": {
                    "type": "string"
                },
                "Type": {
                    "type": "string"
                }
            },
            "required": [
                "Num",
                "SpecCode",
                "Type"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AvailabilityZone": {
            "type": "string"
        },
        "DbUsername": {
            "type": "string"
        },
        "DiskEncryptionId": {
            "type": "string"
        },
        "Mode": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "Password": {
            "type": "string"
        },
        "Region": {
            "type": "string"
        },
        "SecurityGroupId": {
            "type": "string"
        },
        "SubnetId": {
            "type": "string"
        },
        "VpcId": {
            "type": "string"
        },
        "BackupStrategy": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/BackupStrategy"
            },
            "maxItems": 1
        },
        "Datastore": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Datastore"
            },
            "maxItems": 1,
            "minItems": 1
        },
        "Flavor": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Flavor"
            },
            "minItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "AvailabilityZone",
        "Mode",
        "Name",
        "Password",
        "SecurityGroupId",
        "SubnetId",
        "VpcId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/DbUsername"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}