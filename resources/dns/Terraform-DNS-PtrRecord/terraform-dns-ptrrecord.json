{
    "typeName": "Terraform::DNS::PtrRecord",
    "description": "Creates a PTR type DNS record.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name of the record. The `zone` argument will be appended to this value to create the full record path."
        },
        "Ptr": {
            "type": "string",
            "description": "The canonical name this record will point to."
        },
        "Ttl": {
            "type": "number",
            "description": "The TTL of the record set. Defaults to `3600`."
        },
        "Zone": {
            "type": "string",
            "description": "DNS zone the record belongs to. It must be an FQDN, that is, include the trailing dot."
        }
    },
    "additionalProperties": false,
    "required": [
        "Ptr",
        "Zone"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}