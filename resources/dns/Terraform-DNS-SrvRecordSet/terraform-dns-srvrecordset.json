{
    "typeName": "Terraform::DNS::SrvRecordSet",
    "description": "CloudFormation equivalent of dns_srv_record_set",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Srv": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Port": {
                    "type": "number"
                },
                "Priority": {
                    "type": "number"
                },
                "Target": {
                    "type": "string"
                },
                "Weight": {
                    "type": "number"
                }
            },
            "required": [
                "Port",
                "Priority",
                "Target",
                "Weight"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "Ttl": {
            "type": "number"
        },
        "Zone": {
            "type": "string"
        },
        "Srv": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Srv"
            },
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "Zone"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}