{
    "typeName": "Terraform::Brightbox::LoadBalancer",
    "description": "Provides a Brightbox Load Balancer resource. This can be used to create,\nmodify, and delete Load Balancers.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Healthcheck": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Interval": {
                    "type": "number"
                },
                "Port": {
                    "type": "number"
                },
                "Request": {
                    "type": "string"
                },
                "ThresholdDown": {
                    "type": "number"
                },
                "ThresholdUp": {
                    "type": "number"
                },
                "Timeout": {
                    "type": "number"
                },
                "Type": {
                    "type": "string"
                }
            },
            "required": [
                "Port",
                "Type"
            ]
        },
        "Listener": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "In": {
                    "type": "number"
                },
                "Out": {
                    "type": "number"
                },
                "Protocol": {
                    "type": "string"
                },
                "Timeout": {
                    "type": "number"
                }
            },
            "required": [
                "In",
                "Out",
                "Protocol"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "BufferSize": {
            "type": "number",
            "description": "Buffer size in bytes."
        },
        "CertificatePem": {
            "type": "string",
            "description": "A X509 SSL certificate in PEM format. Must be included along with `certificate_key`. If intermediate certificates are required they should be concatenated after the main certificate."
        },
        "CertificatePrivateKey": {
            "type": "string",
            "description": "The RSA private key used to sign the certificate in PEM format. Must be included along with `certificate_pem`."
        },
        "Id": {
            "type": "string"
        },
        "Locked": {
            "type": "boolean"
        },
        "Name": {
            "type": "string",
            "description": "A label assigned to the Load Balancer."
        },
        "Nodes": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "An array of Server IDs."
        },
        "Policy": {
            "type": "string",
            "description": "Method of load balancing to use, either `least-connections` or `round-robin`."
        },
        "Sslv3": {
            "type": "boolean",
            "description": "Allow SSL v3 to be used. Default is `false`."
        },
        "Status": {
            "type": "string"
        },
        "Healthcheck": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Healthcheck"
            },
            "maxItems": 1,
            "minItems": 1
        },
        "Listener": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Listener"
            },
            "minItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/Locked",
        "/properties/Status"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}