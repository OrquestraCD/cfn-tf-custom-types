        },
        "BytesUsed": {
            "type": "number"
        },
        "ContainerRead": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "ContainerSyncKey": {
            "type": "string"
        },
        "ContainerSyncTo": {
            "type": "string"
        },
        "ContainerWrite": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "CreatedAt": {
            "type": "string"
        },
        "HistoryLocation": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Metadata": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
