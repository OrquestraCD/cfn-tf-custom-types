{
    "typeName": "Terraform::Brightbox::Server",
    "description": "Provides a Brightbox Server resource. This can be used to create,\nmodify, and delete Servers. Servers also support\n[provisioning](/docs/provisioners/index.html).",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Fqdn": {
            "type": "string"
        },
        "Hostname": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Image": {
            "type": "string",
            "description": "The Server image ID."
        },
        "Interface": {
            "type": "string"
        },
        "Ipv4Address": {
            "type": "string"
        },
        "Ipv4AddressPrivate": {
            "type": "string"
        },
        "Ipv6Address": {
            "type": "string"
        },
        "Ipv6Hostname": {
            "type": "string"
        },
        "Locked": {
            "type": "boolean"
        },
        "Name": {
            "type": "string",
            "description": "The Server name."
        },
        "PublicHostname": {
            "type": "string"
        },
        "ServerGroups": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Status": {
            "type": "string"
        },
        "Type": {
            "type": "string",
            "description": "The handle of the server type required (`1gb.ssd`, etc)."
        },
        "UserData": {
            "type": "string"
        },
        "UserDataBase64": {
            "type": "string"
        },
        "Username": {
            "type": "string"
        },
        "Zone": {
            "type": "string",
            "description": "The handle of the zone required (`gb1-a`, `gb1-b`)."
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "Image",
        "ServerGroups"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Fqdn",
        "/properties/Hostname",
        "/properties/Id",
        "/properties/Interface",
        "/properties/Ipv4Address",
        "/properties/Ipv4AddressPrivate",
        "/properties/Ipv6Address",
        "/properties/Ipv6Hostname",
        "/properties/Locked",
        "/properties/PublicHostname",
        "/properties/Status",
        "/properties/Username"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}