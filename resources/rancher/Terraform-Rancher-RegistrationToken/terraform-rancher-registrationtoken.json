{
    "typeName": "Terraform::Rancher::RegistrationToken",
    "description": "Provides a Rancher Registration Token resource. This can be used to create registration tokens for rancher environments and retrieve their information.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/rancher/Terraform-Rancher-RegistrationToken/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AgentIp": {
            "type": "string",
            "description": "A string containing the CATTLE_AGENT_IP to add to the registration command."
        },
        "Command": {
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "A registration token description."
        },
        "EnvironmentId": {
            "type": "string",
            "description": "The ID of the environment to create the token for."
        },
        "HostLabels": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "A map of host labels to add to the registration command."
        },
        "Id": {
            "type": "string"
        },
        "Image": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name of the registration token."
        },
        "RegistrationUrl": {
            "type": "string"
        },
        "Token": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "EnvironmentId",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Command",
        "/properties/Id",
        "/properties/Image",
        "/properties/RegistrationUrl",
        "/properties/Token"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}