{
    "typeName": "Terraform::Rancher::RegistryCredential",
    "description": "Provides a Rancher Registy Credential resource. This can be used to create registry credentials for rancher environments and retrieve their information.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "A registry credential description."
        },
        "Email": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name of the registry credential."
        },
        "PublicValue": {
            "type": "string",
            "description": "The public value (user name) of the account."
        },
        "RegistryId": {
            "type": "string",
            "description": "The ID of the registry to create the credential for."
        },
        "SecretValue": {
            "type": "string",
            "description": "The secret value (password) of the account."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "PublicValue",
        "RegistryId",
        "SecretValue"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}