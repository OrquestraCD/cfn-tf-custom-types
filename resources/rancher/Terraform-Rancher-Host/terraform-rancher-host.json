{
    "typeName": "Terraform::Rancher::Host",
    "description": "Provides a Rancher Host resource. This can be used to manage and delete hosts on Rancher.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "A host description."
        },
        "EnvironmentId": {
            "type": "string",
            "description": "The ID of the environment the host is associated to."
        },
        "Hostname": {
            "type": "string",
            "description": "The host name. Used as the primary key to detect the host ID."
        },
        "Labels": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            },
            "description": "A dictionary of labels to apply to the host. Computed internal labels are excluded from that list."
        },
        "Name": {
            "type": "string",
            "description": "The name of the host."
        }
    },
    "additionalProperties": false,
    "required": [
        "EnvironmentId",
        "Hostname",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}