{
    "typeName": "Terraform::Rancher::Certificate",
    "description": "Provides a Rancher Certificate resource. This can be used to create certificates for rancher environments and retrieve their information.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Algorithm": {
            "type": "string"
        },
        "Cert": {
            "type": "string",
            "description": "The certificate content."
        },
        "CertChain": {
            "type": "string",
            "description": "The certificate chain."
        },
        "CertFingerprint": {
            "type": "string"
        },
        "Cn": {
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "A certificate description."
        },
        "EnvironmentId": {
            "type": "string",
            "description": "The ID of the environment to create the certificate for."
        },
        "ExpiresAt": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "IssuedAt": {
            "type": "string"
        },
        "Issuer": {
            "type": "string"
        },
        "Key": {
            "type": "string",
            "description": "The certificate key."
        },
        "KeySize": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name of the certificate."
        },
        "SerialNumber": {
            "type": "string"
        },
        "SubjectAlternativeNames": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Version": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "Cert",
        "EnvironmentId",
        "Key",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Algorithm",
        "/properties/CertFingerprint",
        "/properties/Cn",
        "/properties/ExpiresAt",
        "/properties/Id",
        "/properties/IssuedAt",
        "/properties/Issuer",
        "/properties/KeySize",
        "/properties/SerialNumber",
        "/properties/SubjectAlternativeNames",
        "/properties/Version"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}