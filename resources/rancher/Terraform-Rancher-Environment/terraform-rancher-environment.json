{
    "typeName": "Terraform::Rancher::Environment",
    "description": "Provides a Rancher Environment resource. This can be used to create and manage environments on rancher.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Member": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "ExternalId": {
                    "type": "string"
                },
                "ExternalIdType": {
                    "type": "string"
                },
                "Role": {
                    "type": "string"
                }
            },
            "required": [
                "ExternalId",
                "ExternalIdType",
                "Role"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "An environment description."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name of the environment."
        },
        "Orchestration": {
            "type": "string",
            "description": "Must be one of **cattle**, **swarm**, **mesos**, **windows** or **kubernetes**. This is a helper for setting the project_template_ids for the included Rancher templates. This will conflict with project_template_id setting. Changing this forces a new resource to be created."
        },
        "ProjectTemplateId": {
            "type": "string",
            "description": "This can be any valid project template ID. If this is set, then orchestration can not be. Changing this forces a new resource to be created."
        },
        "Member": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Member"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}