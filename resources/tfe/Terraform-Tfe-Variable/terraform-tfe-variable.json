{
    "typeName": "Terraform::Tfe::Variable",
    "description": "Creates, updates and destroys variables.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Category": {
            "type": "string",
            "description": "Whether this is a Terraform or environment variable.\nValid values are `terraform` or `env`."
        },
        "Description": {
            "type": "string",
            "description": "Description of the variable."
        },
        "Hcl": {
            "type": "boolean",
            "description": "Whether to evaluate the value of the variable as a string\nof HCL code. Has no effect for environment variables. Defaults to `false`."
        },
        "Key": {
            "type": "string",
            "description": "Name of the variable."
        },
        "Sensitive": {
            "type": "boolean",
            "description": "Whether the value is sensitive. If true then the\nvariable is written once and not visible thereafter. Defaults to `false`."
        },
        "Value": {
            "type": "string",
            "description": "Value of the variable."
        },
        "WorkspaceId": {
            "type": "string",
            "description": "The workspace that owns the variable, specified as\na human-readable ID (`<ORGANIZATION>/<WORKSPACE>`)."
        }
    },
    "additionalProperties": false,
    "required": [
        "Category",
        "Key",
        "WorkspaceId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}