{
    "typeName": "Terraform::Tfe::OrganizationToken",
    "description": "Generates a new organization token, replacing any existing token. This token\ncan be used to act as the organization service account.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ForceRegenerate": {
            "type": "boolean",
            "description": "If set to `true`, a new token will be\ngenerated even if a token already exists. This will invalidate the existing\ntoken!."
        },
        "Organization": {
            "type": "string",
            "description": "Name of the organization."
        },
        "Token": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "Organization"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Token"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}