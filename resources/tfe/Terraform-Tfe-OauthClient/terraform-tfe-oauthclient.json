{
    "typeName": "Terraform::Tfe::OauthClient",
    "description": "CloudFormation equivalent of tfe_oauth_client",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ApiUrl": {
            "type": "string"
        },
        "HttpUrl": {
            "type": "string"
        },
        "OauthToken": {
            "type": "string"
        },
        "OauthTokenId": {
            "type": "string"
        },
        "Organization": {
            "type": "string"
        },
        "PrivateKey": {
            "type": "string"
        },
        "ServiceProvider": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "ApiUrl",
        "HttpUrl",
        "OauthToken",
        "Organization",
        "ServiceProvider"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/OauthTokenId"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}