{
    "typeName": "Terraform::Heroku::Addon",
    "description": "CloudFormation equivalent of heroku_addon",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "App": {
            "type": "string"
        },
        "Config": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "ConfigVars": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "Plan": {
            "type": "string"
        },
        "ProviderId": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "App",
        "Plan"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/ConfigVars",
        "/properties/ProviderId"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}