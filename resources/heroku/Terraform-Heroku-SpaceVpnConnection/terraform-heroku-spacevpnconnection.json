{
    "typeName": "Terraform::Heroku::SpaceVpnConnection",
    "description": "Provides a resource for creating a VPN connection between a network and a Heroku Private Space. For more information, see [Private Spaces VPN Connection](https://devcenter.heroku.com/articles/private-space-vpn-connection?preview=1) in the Heroku DevCenter.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Tunnels": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Ip": {
                    "type": "string"
                },
                "PreSharedKey": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "IkeVersion": {
            "type": "number"
        },
        "Name": {
            "type": "string",
            "description": "The name of the VPN connection."
        },
        "PublicIp": {
            "type": "string",
            "description": "The public IP address of the VPN endpoint on the network where the VPN connection will be established."
        },
        "RoutableCidrs": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "A list of IPv4 CIDR blocks used by the network where the VPN connection will be established."
        },
        "Space": {
            "type": "string",
            "description": "The name of the Heroku Private Space where the VPN connection will be established."
        },
        "SpaceCidrBlock": {
            "type": "string"
        },
        "Tunnels": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Tunnels"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "PublicIp",
        "RoutableCidrs",
        "Space"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/IkeVersion",
        "/properties/SpaceCidrBlock"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}