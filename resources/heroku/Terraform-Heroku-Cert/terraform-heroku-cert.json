{
    "typeName": "Terraform::Heroku::Cert",
    "description": "Provides a Heroku SSL certificate resource. It allows to set a given certificate for a Heroku app.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "App": {
            "type": "string",
            "description": "The Heroku app to add to."
        },
        "CertificateChain": {
            "type": "string",
            "description": "The certificate chain to add."
        },
        "Cname": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "PrivateKey": {
            "type": "string",
            "description": "The private key for a given certificate chain."
        }
    },
    "additionalProperties": false,
    "required": [
        "App",
        "CertificateChain",
        "PrivateKey"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Cname",
        "/properties/Name"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}