{
    "typeName": "Terraform::Heroku::Build",
    "description": "CloudFormation equivalent of heroku_build",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "App": {
            "type": "string"
        },
        "Buildpacks": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "Id": {
            "type": "string"
        },
        "LocalChecksum": {
            "type": "string"
        },
        "OutputStreamUrl": {
            "type": "string"
        },
        "ReleaseId": {
            "type": "string"
        },
        "SlugId": {
            "type": "string"
        },
        "Source": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "Stack": {
            "type": "string"
        },
        "Status": {
            "type": "string"
        },
        "User": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "Uuid": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "App",
        "Source"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/LocalChecksum",
        "/properties/OutputStreamUrl",
        "/properties/ReleaseId",
        "/properties/SlugId",
        "/properties/Stack",
        "/properties/Status",
        "/properties/User",
        "/properties/Uuid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}