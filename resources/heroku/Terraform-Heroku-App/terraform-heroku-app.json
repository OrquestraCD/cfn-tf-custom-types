{
    "typeName": "Terraform::Heroku::App",
    "description": "CloudFormation equivalent of heroku_app",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Organization": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Locked": {
                    "type": "boolean"
                },
                "Name": {
                    "type": "string"
                },
                "Personal": {
                    "type": "boolean"
                }
            },
            "required": [
                "Name"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Acm": {
            "type": "boolean"
        },
        "AllConfigVars": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "Buildpacks": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "ConfigVars": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "GitUrl": {
            "type": "string"
        },
        "HerokuHostname": {
            "type": "string"
        },
        "InternalRouting": {
            "type": "boolean"
        },
        "Name": {
            "type": "string"
        },
        "Region": {
            "type": "string"
        },
        "SensitiveConfigVars": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "Space": {
            "type": "string"
        },
        "Stack": {
            "type": "string"
        },
        "Uuid": {
            "type": "string"
        },
        "WebUrl": {
            "type": "string"
        },
        "Organization": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Organization"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "Region"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/AllConfigVars",
        "/properties/GitUrl",
        "/properties/HerokuHostname",
        "/properties/WebUrl"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}