{
    "typeName": "Terraform::Heroku::Drain",
    "description": "Provides a Heroku Drain resource. This can be used to\ncreate and manage Log Drains on Heroku.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "App": {
            "type": "string",
            "description": "The Heroku app to link to."
        },
        "Id": {
            "type": "string"
        },
        "Token": {
            "type": "string"
        },
        "Url": {
            "type": "string",
            "description": "The URL for Heroku to drain your logs to."
        }
    },
    "additionalProperties": false,
    "required": [
        "App",
        "Url"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/Token"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}