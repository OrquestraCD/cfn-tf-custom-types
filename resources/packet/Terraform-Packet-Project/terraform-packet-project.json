{
    "typeName": "Terraform::Packet::Project",
    "description": "CloudFormation equivalent of packet_project",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "BgpConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Asn": {
                    "type": "number"
                },
                "DeploymentType": {
                    "type": "string"
                },
                "Md5": {
                    "type": "string"
                }
            },
            "required": [
                "Asn",
                "DeploymentType"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "BackendTransfer": {
            "type": "boolean"
        },
        "Created": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "OrganizationId": {
            "type": "string"
        },
        "PaymentMethodId": {
            "type": "string"
        },
        "Updated": {
            "type": "string"
        },
        "BgpConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/BgpConfig"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Created",
        "/properties/Updated"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}