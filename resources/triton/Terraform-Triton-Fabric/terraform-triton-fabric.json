{
    "typeName": "Terraform::Triton::Fabric",
    "description": "CloudFormation equivalent of triton_fabric",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string"
        },
        "Fabric": {
            "type": "boolean"
        },
        "Gateway": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "InternetNat": {
            "type": "boolean"
        },
        "Name": {
            "type": "string"
        },
        "ProvisionEndIp": {
            "type": "string"
        },
        "ProvisionStartIp": {
            "type": "string"
        },
        "Public": {
            "type": "boolean"
        },
        "Resolvers": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "Routes": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "Subnet": {
            "type": "string"
        },
        "VlanId": {
            "type": "number"
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "ProvisionEndIp",
        "ProvisionStartIp",
        "Subnet",
        "VlanId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Fabric",
        "/properties/Public"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}