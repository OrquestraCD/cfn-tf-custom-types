{
    "typeName": "Terraform::Mailgun::Route",
    "description": "Provides a Mailgun Route resource. This can be used to create and manage routes on Mailgun.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Actions": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "Description": {
            "type": "string",
            "description": null
        },
        "Expression": {
            "type": "string",
            "description": "A filter expression like `match_recipient('.*@gmail.com')`."
        },
        "Id": {
            "type": "string"
        },
        "Priority": {
            "type": "number",
            "description": "Smaller number indicates higher priority. Higher priority routes are handled first."
        },
        "Region": {
            "type": "string",
            "description": "The region where domain will be created. Default value is `us`."
        }
    },
    "additionalProperties": false,
    "required": [
        "Actions",
        "Expression",
        "Priority"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}