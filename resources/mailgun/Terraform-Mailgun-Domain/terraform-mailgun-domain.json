{
    "typeName": "Terraform::Mailgun::Domain",
    "description": "Provides a Mailgun App resource. This can be used to\ncreate and manage applications on Mailgun.\n\nAfter DNS records are set, domain verification should be triggered manually using [PUT /domains/\\<domain\\>/verify](https://documentation.mailgun.com/en/latest/api-domains.html#domains)",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The domain to add to Mailgun."
        },
        "ReceivingRecords": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Priority": {
                        "type": "string"
                    },
                    "RecordType": {
                        "type": "string"
                    },
                    "Valid": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                }
            }
        },
        "Region": {
            "type": "string",
            "description": "The region where domain will be created. Default value is `us`."
        },
        "SendingRecords": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Name": {
                        "type": "string"
                    },
                    "RecordType": {
                        "type": "string"
                    },
                    "Valid": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                }
            }
        },
        "SmtpLogin": {
            "type": "string"
        },
        "SmtpPassword": {
            "type": "string"
        },
        "SpamAction": {
            "type": "string",
            "description": "`disabled` or `tag` Disable, no spam\nfiltering will occur for inbound messages. Tag, messages\nwill be tagged with a spam header."
        },
        "Wildcard": {
            "type": "boolean",
            "description": "Boolean that determines whether\nthe domain will accept email for sub-domains."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/ReceivingRecords",
        "/properties/SendingRecords",
        "/properties/SmtpLogin",
        "/properties/SmtpPassword"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}