{
    "typeName": "Terraform::RunScope::Environment",
    "description": "An [environment](https://www.runscope.com/docs/api/environments) resource.\nAn [environment](https://www.runscope.com/docs/api-testing/environments)\nis is a group of configuration settings (initial variables, locations,\nnotifications, integrations, etc.) used when running a test.\nEvery test has at least one environment, but you can create additional\nenvironments as needed. For common settings (base URLs, API keys)\nthat you'd like to use across all tests within a bucket,\nuse a [Shared Environment](https://www.runscope.com/docs/api-testing/environments#shared).",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/runscope/Terraform-RunScope-Environment/docs/README.md",
    "definitions": {
        "Emails": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "NotifyAll": {
                    "type": "boolean",
                    "description": "Send an email to all team members according to the `notify_on` rules."
                },
                "NotifyOn": {
                    "type": "string",
                    "description": "Upon completion of a test run Runscope will send email notifications, allowed values: `all`, `failures`, `threshold` or `switch`."
                },
                "NotifyThreshold": {
                    "type": "number"
                },
                "Recipients": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "$ref": "#/definitions/Recipients"
                    },
                    "minItems": 1
                }
            },
            "required": [
                "NotifyAll",
                "NotifyOn",
                "NotifyThreshold"
            ]
        },
        "RemoteAgents": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Name": {
                    "type": "string",
                    "description": "The name of the remote agent."
                },
                "Uuid": {
                    "type": "string",
                    "description": "The uuid of the remote agent."
                }
            },
            "required": [
                "Name",
                "Uuid"
            ]
        },
        "Recipients": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Email": {
                    "type": "string",
                    "description": "The email address for this account."
                },
                "Id": {
                    "type": "string",
                    "description": "The unique identifier for this person's account."
                },
                "Name": {
                    "type": "string",
                    "description": "The name of the person."
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "BucketId": {
            "type": "string",
            "description": "The id of the bucket to associate this environment with."
        },
        "Id": {
            "type": "string"
        },
        "InitialVariables": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "Map of keys and values being used for variables when the test begins."
        },
        "Integrations": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "A list of integration ids to enable for test runs using this environment."
        },
        "Name": {
            "type": "string",
            "description": "The name of environment."
        },
        "PreserveCookies": {
            "type": "boolean",
            "description": "If this is set to true, tests using this enviornment will manage cookies between steps."
        },
        "Regions": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "A list of [Runscope regions](https://www.runscope.com/docs/regions) to execute test runs in when using this environment."
        },
        "RetryOnFailure": {
            "type": "boolean"
        },
        "Script": {
            "type": "string",
            "description": "The [script](https://www.runscope.com/docs/api-testing/scripts#initial-script)\nto to run to setup the environment."
        },
        "TestId": {
            "type": "string",
            "description": "The id of the test to associate this environment with.\nIf given, creates a test specific environment, otherwise creates a shared environment."
        },
        "VerifySsl": {
            "type": "boolean"
        },
        "Webhooks": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Emails": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Emails"
            },
            "maxItems": 1
        },
        "RemoteAgents": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/RemoteAgents"
            }
        },
        "Recipients": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Recipients"
            },
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "BucketId",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}