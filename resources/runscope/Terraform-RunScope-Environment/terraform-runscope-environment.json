{
    "typeName": "Terraform::RunScope::Environment",
    "description": "CloudFormation equivalent of runscope_environment",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Emails": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "NotifyAll": {
                    "type": "boolean"
                },
                "NotifyOn": {
                    "type": "string"
                },
                "NotifyThreshold": {
                    "type": "number"
                },
                "Recipients": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "$ref": "#/definitions/Recipients"
                    }
                }
            },
            "required": [
                "NotifyAll",
                "NotifyOn",
                "NotifyThreshold"
            ]
        },
        "RemoteAgents": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Name": {
                    "type": "string"
                },
                "Uuid": {
                    "type": "string"
                }
            },
            "required": [
                "Name",
                "Uuid"
            ]
        },
        "Recipients": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Email": {
                    "type": "string"
                },
                "Id": {
                    "type": "string"
                },
                "Name": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "BucketId": {
            "type": "string"
        },
        "InitialVariables": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "Integrations": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Name": {
            "type": "string"
        },
        "PreserveCookies": {
            "type": "boolean"
        },
        "Regions": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "RetryOnFailure": {
            "type": "boolean"
        },
        "Script": {
            "type": "string"
        },
        "TestId": {
            "type": "string"
        },
        "VerifySsl": {
            "type": "boolean"
        },
        "Webhooks": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Emails": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Emails"
            },
            "maxItems": 1
        },
        "RemoteAgents": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/RemoteAgents"
            }
        },
        "Recipients": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Recipients"
            },
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "BucketId",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}