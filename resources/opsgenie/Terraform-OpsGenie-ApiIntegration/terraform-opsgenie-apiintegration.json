{
    "typeName": "Terraform::OpsGenie::ApiIntegration",
    "description": "Manages an API Integration within Opsgenie.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/opsgenie/Terraform-OpsGenie-ApiIntegration/docs/README.md",
    "definitions": {
        "Responders": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Id": {
                    "type": "string",
                    "description": "The id of the responder."
                },
                "Type": {
                    "type": "string",
                    "description": "The responder type."
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AllowWriteAccess": {
            "type": "boolean",
            "description": "This parameter is for configuring the write access of integration. If write access is restricted, the integration will not be authorized to write within any domain. Defaults to true."
        },
        "ApiKey": {
            "type": "string"
        },
        "Enabled": {
            "type": "boolean",
            "description": "This parameter is for specifying whether the integration will be enabled or not. Defaults to true."
        },
        "Id": {
            "type": "string"
        },
        "IgnoreRespondersFromPayload": {
            "type": "boolean",
            "description": "If enabled, the integration will ignore recipients sent in request payloads. Defaults to false."
        },
        "Name": {
            "type": "string",
            "description": "Name of the integration. Name must be unique for each integration."
        },
        "OwnerTeamId": {
            "type": "string",
            "description": "Owner team id of the integration."
        },
        "SuppressNotifications": {
            "type": "boolean",
            "description": "If enabled, notifications that come from alerts will be suppressed. Defaults to false."
        },
        "Type": {
            "type": "string",
            "description": "Type of the integration (API, Marid, Prometheus, etc). The full list of options can be found [here](https://docs.opsgenie.com/docs/integration-types-to-use-with-api)."
        },
        "Responders": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Responders"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/ApiKey",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": []
        }
    },
    "writeOnlyProperties": [
        "/properties/ApiKey"
    ]
}