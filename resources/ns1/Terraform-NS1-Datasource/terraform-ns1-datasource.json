{
    "typeName": "Terraform::NS1::Datasource",
    "description": "Provides a NS1 Data Source resource. This can be used to create, modify, and delete data sources.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Config": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            },
            "description": "The data source configuration, determined by its type,\nmatching the specification in `config` from /data/sourcetypes."
        },
        "Name": {
            "type": "string",
            "description": "The free form name of the data source."
        },
        "Sourcetype": {
            "type": "string",
            "description": "The data sources type, listed in API endpoint https://api.nsone.net/v1/data/sourcetypes."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "Sourcetype"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}