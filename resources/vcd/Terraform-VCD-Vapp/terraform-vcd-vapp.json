{
    "typeName": "Terraform::VCD::Vapp",
    "description": "Provides a vCloud Director vApp resource. This can be used to create, modify, and delete vApps.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/vcd/Terraform-VCD-Vapp/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AcceptAllEulas": {
            "type": "boolean"
        },
        "CatalogName": {
            "type": "string"
        },
        "Cpus": {
            "type": "number"
        },
        "Description": {
            "type": "string"
        },
        "GuestProperties": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "Key value map of vApp guest properties."
        },
        "Href": {
            "type": "string",
            "description": "The vApp Hyper Reference."
        },
        "Id": {
            "type": "string"
        },
        "Initscript": {
            "type": "string"
        },
        "Ip": {
            "type": "string"
        },
        "Memory": {
            "type": "number"
        },
        "Metadata": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "Key value map of metadata to assign to this vApp. Key and value can be any string. (Since *v2.2+* metadata is added directly to vApp instead of first VM in vApp)."
        },
        "Name": {
            "type": "string",
            "description": "A unique name for the vApp."
        },
        "NetworkName": {
            "type": "string"
        },
        "Org": {
            "type": "string",
            "description": "The name of organization to use, optional if defined at provider level. Useful when connected as sysadmin working across different organisations."
        },
        "Ovf": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            }
        },
        "PowerOn": {
            "type": "boolean",
            "description": "A boolean value stating if this vApp should be powered on. Default is `true`."
        },
        "Status": {
            "type": "number",
            "description": "The vApp status as a numeric code."
        },
        "StatusText": {
            "type": "string",
            "description": "The vApp status as text."
        },
        "StorageProfile": {
            "type": "string"
        },
        "TemplateName": {
            "type": "string"
        },
        "Vdc": {
            "type": "string",
            "description": "The name of VDC to use, optional if defined at provider level."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Href",
        "/properties/Id",
        "/properties/Status",
        "/properties/StatusText"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}