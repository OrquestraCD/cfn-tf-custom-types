{
    "typeName": "Terraform::Arukas::Container",
    "description": "Provides container resource. This allows container to be created, updated and deleted.\n\nFor additional details please refer to [API documentation](https://arukas.io/en/documents-en/arukas-api-reference-en/).",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Environments": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Key": {
                    "type": "string"
                },
                "Value": {
                    "type": "string"
                }
            },
            "required": [
                "Key",
                "Value"
            ]
        },
        "Ports": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Number": {
                    "type": "number"
                },
                "Protocol": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Cmd": {
            "type": "string",
            "description": "The command of the container."
        },
        "Endpoint": {
            "type": "string",
            "description": "The subdomain part of the endpoint assigned by Arukas. If it is not set, Arukas will do automatic assignment."
        },
        "EndpointFullHostname": {
            "type": "string"
        },
        "EndpointFullUrl": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Image": {
            "type": "string",
            "description": "The ID of the image to back this container.It must be a public image on DockerHub."
        },
        "Instances": {
            "type": "number",
            "description": "The count of the instance. It must be between `1` and `10`."
        },
        "Memory": {
            "type": "number"
        },
        "Name": {
            "type": "string",
            "description": "The name of the container."
        },
        "Plan": {
            "type": "string",
            "description": "The plan of the Arukas. It must be `free` or `hobby` or `standard-1` or `standard-2`."
        },
        "PortMappings": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "ContainerPort": {
                        "type": "number"
                    },
                    "Host": {
                        "type": "string"
                    },
                    "Ipaddress": {
                        "type": "string"
                    },
                    "ServicePort": {
                        "type": "number"
                    }
                }
            }
        },
        "Region": {
            "type": "string"
        },
        "ServiceId": {
            "type": "string"
        },
        "Environments": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Environments"
            },
            "maxItems": 20
        },
        "Ports": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Ports"
            },
            "maxItems": 20,
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Image",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/EndpointFullHostname",
        "/properties/EndpointFullUrl",
        "/properties/Id",
        "/properties/PortMappings",
        "/properties/Region",
        "/properties/ServiceId"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}