{
    "typeName": "Terraform::HCloud::Server",
    "description": "CloudFormation equivalent of hcloud_server",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "BackupWindow": {
            "type": "string"
        },
        "Backups": {
            "type": "boolean"
        },
        "Datacenter": {
            "type": "string"
        },
        "Image": {
            "type": "string"
        },
        "Ipv4Address": {
            "type": "string"
        },
        "Ipv6Address": {
            "type": "string"
        },
        "Ipv6Network": {
            "type": "string"
        },
        "Iso": {
            "type": "string"
        },
        "KeepDisk": {
            "type": "boolean"
        },
        "Labels": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "Location": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "Rescue": {
            "type": "string"
        },
        "ServerType": {
            "type": "string"
        },
        "SshKeys": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "Status": {
            "type": "string"
        },
        "UserData": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "Image",
        "Name",
        "ServerType"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/BackupWindow",
        "/properties/Ipv4Address",
        "/properties/Ipv6Address",
        "/properties/Ipv6Network",
        "/properties/Status"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}