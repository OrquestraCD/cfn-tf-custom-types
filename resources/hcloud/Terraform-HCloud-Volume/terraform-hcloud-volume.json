            "type": "string"
        },
        "Automount": {
            "type": "boolean"
        },
        "Format": {
            "type": "string"
        },
        "Labels": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "LinuxDevice": {
            "type": "string"
        },
        "Location": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "ServerId": {
            "type": "number"
        },
        "Size": {
            "type": "number"
        }
    },
    "additionalProperties": false,
