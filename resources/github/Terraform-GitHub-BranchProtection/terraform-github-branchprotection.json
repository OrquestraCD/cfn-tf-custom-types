{
    "typeName": "Terraform::GitHub::BranchProtection",
    "description": "CloudFormation equivalent of github_branch_protection",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "RequiredPullRequestReviews": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "DismissStaleReviews": {
                    "type": "boolean"
                },
                "DismissalTeams": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                },
                "DismissalUsers": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                },
                "IncludeAdmins": {
                    "type": "boolean"
                },
                "RequireCodeOwnerReviews": {
                    "type": "boolean"
                },
                "RequiredApprovingReviewCount": {
                    "type": "number"
                }
            },
            "required": []
        },
        "RequiredStatusChecks": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Contexts": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                },
                "IncludeAdmins": {
                    "type": "boolean"
                },
                "Strict": {
                    "type": "boolean"
                }
            },
            "required": []
        },
        "Restrictions": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Teams": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                },
                "Users": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Branch": {
            "type": "string"
        },
        "EnforceAdmins": {
            "type": "boolean"
        },
        "Etag": {
            "type": "string"
        },
        "Repository": {
            "type": "string"
        },
        "RequireSignedCommits": {
            "type": "boolean"
        },
        "RequiredPullRequestReviews": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/RequiredPullRequestReviews"
            },
            "maxItems": 1
        },
        "RequiredStatusChecks": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/RequiredStatusChecks"
            },
            "maxItems": 1
        },
        "Restrictions": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Restrictions"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Branch",
        "Repository"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Etag"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}