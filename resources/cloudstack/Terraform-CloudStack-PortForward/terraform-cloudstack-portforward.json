{
    "typeName": "Terraform::CloudStack::PortForward",
    "description": "Creates port forwards.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Forward": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "PrivatePort": {
                    "type": "number",
                    "description": "The private port to forward to."
                },
                "Protocol": {
                    "type": "string",
                    "description": "The name of the protocol to allow. Valid options are:\n`tcp` and `udp`."
                },
                "PublicPort": {
                    "type": "number",
                    "description": "The public port to forward from."
                },
                "VirtualMachineId": {
                    "type": "string",
                    "description": "The ID of the virtual machine to forward to."
                },
                "VmGuestIp": {
                    "type": "string",
                    "description": "The virtual machine IP address for the port\nforwarding rule (useful when the virtual machine has secondairy NICs\nor IP addresses)."
                }
            },
            "required": [
                "PrivatePort",
                "Protocol",
                "PublicPort",
                "VirtualMachineId"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "IpAddressId": {
            "type": "string",
            "description": "The IP address ID for which to create the port\nforwards. Changing this forces a new resource to be created."
        },
        "Managed": {
            "type": "boolean",
            "description": "USE WITH CAUTION! If enabled all the port forwards for\nthis IP address will be managed by this resource. This means it will delete\nall port forwards that are not in your config! (defaults false)."
        },
        "Project": {
            "type": "string",
            "description": "The name or ID of the project to create this port forward\nin. Changing this forces a new resource to be created."
        },
        "Forward": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Forward"
            },
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "IpAddressId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}