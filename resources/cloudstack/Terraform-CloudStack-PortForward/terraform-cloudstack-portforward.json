{
    "typeName": "Terraform::CloudStack::PortForward",
    "description": "CloudFormation equivalent of cloudstack_port_forward",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Forward": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "PrivatePort": {
                    "type": "number"
                },
                "Protocol": {
                    "type": "string"
                },
                "PublicPort": {
                    "type": "number"
                },
                "VirtualMachineId": {
                    "type": "string"
                },
                "VmGuestIp": {
                    "type": "string"
                }
            },
            "required": [
                "PrivatePort",
                "Protocol",
                "PublicPort",
                "VirtualMachineId"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "IpAddressId": {
            "type": "string"
        },
        "Managed": {
            "type": "boolean"
        },
        "Project": {
            "type": "string"
        },
        "Forward": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Forward"
            },
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "IpAddressId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}