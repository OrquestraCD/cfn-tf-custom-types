{
    "typeName": "Terraform::CloudStack::Nic",
    "description": "Creates an additional NIC to add a VM to the specified network.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/cloudstack/Terraform-CloudStack-Nic/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "IpAddress": {
            "type": "string",
            "description": "The IP address to assign to the NIC. Changing this\nforces a new resource to be created."
        },
        "NetworkId": {
            "type": "string",
            "description": "The ID of the network to plug the NIC into. Changing\nthis forces a new resource to be created."
        },
        "VirtualMachineId": {
            "type": "string",
            "description": "The ID of the virtual machine to which to\nattach the NIC. Changing this forces a new resource to be created."
        }
    },
    "additionalProperties": false,
    "required": [
        "NetworkId",
        "VirtualMachineId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}