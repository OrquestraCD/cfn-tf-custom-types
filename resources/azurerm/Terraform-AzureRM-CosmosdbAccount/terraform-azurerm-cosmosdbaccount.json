{
    "typeName": "Terraform::AzureRM::CosmosdbAccount",
    "description": "CloudFormation equivalent of azurerm_cosmosdb_account",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Capabilities": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Name": {
                    "type": "string"
                }
            },
            "required": [
                "Name"
            ]
        },
        "ConsistencyPolicy": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "ConsistencyLevel": {
                    "type": "string"
                },
                "MaxIntervalInSeconds": {
                    "type": "number"
                },
                "MaxStalenessPrefix": {
                    "type": "number"
                }
            },
            "required": [
                "ConsistencyLevel"
            ]
        },
        "GeoLocation": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "FailoverPriority": {
                    "type": "number"
                },
                "Location": {
                    "type": "string"
                },
                "Prefix": {
                    "type": "string"
                }
            },
            "required": [
                "FailoverPriority",
                "Location"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Read": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        },
        "VirtualNetworkRule": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Id": {
                    "type": "string"
                }
            },
            "required": [
                "Id"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ConnectionStrings": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "EnableAutomaticFailover": {
            "type": "boolean"
        },
        "EnableMultipleWriteLocations": {
            "type": "boolean"
        },
        "Endpoint": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "IpRangeFilter": {
            "type": "string"
        },
        "IsVirtualNetworkFilterEnabled": {
            "type": "boolean"
        },
        "Kind": {
            "type": "string"
        },
        "Location": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "OfferType": {
            "type": "string"
        },
        "PrimaryMasterKey": {
            "type": "string"
        },
        "PrimaryReadonlyMasterKey": {
            "type": "string"
        },
        "ReadEndpoints": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "ResourceGroupName": {
            "type": "string"
        },
        "SecondaryMasterKey": {
            "type": "string"
        },
        "SecondaryReadonlyMasterKey": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "WriteEndpoints": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "Capabilities": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Capabilities"
            }
        },
        "ConsistencyPolicy": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/ConsistencyPolicy"
            },
            "maxItems": 1,
            "minItems": 1
        },
        "GeoLocation": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/GeoLocation"
            },
            "minItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        },
        "VirtualNetworkRule": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/VirtualNetworkRule"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Location",
        "Name",
        "OfferType",
        "ResourceGroupName"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/ConnectionStrings",
        "/properties/Endpoint",
        "/properties/PrimaryMasterKey",
        "/properties/PrimaryReadonlyMasterKey",
        "/properties/ReadEndpoints",
        "/properties/SecondaryMasterKey",
        "/properties/SecondaryReadonlyMasterKey",
        "/properties/WriteEndpoints"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}