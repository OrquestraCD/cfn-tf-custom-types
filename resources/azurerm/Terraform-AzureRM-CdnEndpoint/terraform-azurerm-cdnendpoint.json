{
    "typeName": "Terraform::AzureRM::CdnEndpoint",
    "description": "CloudFormation equivalent of azurerm_cdn_endpoint",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "GeoFilter": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Action": {
                    "type": "string"
                },
                "CountryCodes": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                },
                "RelativePath": {
                    "type": "string"
                }
            },
            "required": [
                "Action",
                "CountryCodes",
                "RelativePath"
            ]
        },
        "Origin": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "HostName": {
                    "type": "string"
                },
                "HttpPort": {
                    "type": "number"
                },
                "HttpsPort": {
                    "type": "number"
                },
                "Name": {
                    "type": "string"
                }
            },
            "required": [
                "HostName",
                "Name"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Read": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ContentTypesToCompress": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "HostName": {
            "type": "string"
        },
        "IsCompressionEnabled": {
            "type": "boolean"
        },
        "IsHttpAllowed": {
            "type": "boolean"
        },
        "IsHttpsAllowed": {
            "type": "boolean"
        },
        "Location": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "OptimizationType": {
            "type": "string"
        },
        "OriginHostHeader": {
            "type": "string"
        },
        "OriginPath": {
            "type": "string"
        },
        "ProbePath": {
            "type": "string"
        },
        "ProfileName": {
            "type": "string"
        },
        "QuerystringCachingBehaviour": {
            "type": "string"
        },
        "ResourceGroupName": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "GeoFilter": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/GeoFilter"
            }
        },
        "Origin": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Origin"
            },
            "minItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "Location",
        "Name",
        "ProfileName",
        "ResourceGroupName"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/HostName"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}