            "type": "string"
        },
        "AdministratorLogin": {
            "type": "string"
        },
        "AdministratorLoginPassword": {
            "type": "string"
        },
        "Fqdn": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Location": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "ResourceGroupName": {
            "type": "string"
        },
        "SkuName": {
            "type": "string"
        },
        "SslEnforcement": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
