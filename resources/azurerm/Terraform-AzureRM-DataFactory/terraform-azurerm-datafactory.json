            "properties": {
                "AccountName": {
                    "type": "string"
                },
                "BranchName": {
                    "type": "string"
                },
                "ProjectName": {
                    "type": "string"
                },
                "RepositoryName": {
                    "type": "string"
                },
                "RootFolder": {
                    "type": "string"
