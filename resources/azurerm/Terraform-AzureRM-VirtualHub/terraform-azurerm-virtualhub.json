{
    "typeName": "Terraform::AzureRM::VirtualHub",
    "description": "Manages a Virtual Hub within a Virtual WAN.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Route": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AddressPrefixes": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    },
                    "description": "A list of Address Prefixes."
                },
                "NextHopIpAddress": {
                    "type": "string",
                    "description": "The IP Address that Packets should be forwarded to as the Next Hop."
                }
            },
            "required": [
                "AddressPrefixes",
                "NextHopIpAddress"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Read": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AddressPrefix": {
            "type": "string",
            "description": "The Address Prefix which should be used for this Virtual Hub."
        },
        "Location": {
            "type": "string",
            "description": "Specifies the supported Azure location where the Virtual Hub should exist. Changing this forces a new resource to be created."
        },
        "Name": {
            "type": "string",
            "description": "The name of the Virtual Hub. Changing this forces a new resource to be created."
        },
        "ResourceGroupName": {
            "type": "string",
            "description": "Specifies the name of the Resource Group where the Virtual Hub should exist. Changing this forces a new resource to be created."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            },
            "description": "A mapping of tags to assign to the Virtual Hub."
        },
        "VirtualWanId": {
            "type": "string",
            "description": "The ID of a Virtual WAN within which the Virtual Hub should be created."
        },
        "Route": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Route"
            }
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "AddressPrefix",
        "Location",
        "Name",
        "ResourceGroupName",
        "VirtualWanId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}