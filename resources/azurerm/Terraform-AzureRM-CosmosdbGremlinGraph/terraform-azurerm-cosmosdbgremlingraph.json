{
    "typeName": "Terraform::AzureRM::CosmosdbGremlinGraph",
    "description": "CloudFormation equivalent of azurerm_cosmosdb_gremlin_graph",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "ConflictResolutionPolicy": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "ConflictResolutionPath": {
                    "type": "string"
                },
                "ConflictResolutionProcedure": {
                    "type": "string"
                },
                "Mode": {
                    "type": "string"
                }
            },
            "required": [
                "Mode"
            ]
        },
        "IndexPolicy": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Automatic": {
                    "type": "boolean"
                },
                "ExcludedPaths": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                },
                "IncludedPaths": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                },
                "IndexingMode": {
                    "type": "string"
                }
            },
            "required": [
                "IndexingMode"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Read": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        },
        "UniqueKey": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Paths": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                }
            },
            "required": [
                "Paths"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AccountName": {
            "type": "string"
        },
        "DatabaseName": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "PartitionKeyPath": {
            "type": "string"
        },
        "ResourceGroupName": {
            "type": "string"
        },
        "Throughput": {
            "type": "number"
        },
        "ConflictResolutionPolicy": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/ConflictResolutionPolicy"
            },
            "minItems": 1
        },
        "IndexPolicy": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/IndexPolicy"
            },
            "minItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        },
        "UniqueKey": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/UniqueKey"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "AccountName",
        "DatabaseName",
        "Name",
        "ResourceGroupName"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
