{
    "typeName": "Terraform::AzureRM::Snapshot",
    "description": "Manages a Disk Snapshot.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "EncryptionSettings": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Enabled": {
                    "type": "boolean"
                },
                "DiskEncryptionKey": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/DiskEncryptionKey"
                    }
                },
                "KeyEncryptionKey": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/KeyEncryptionKey"
                    }
                }
            },
            "required": [
                "Enabled"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Read": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        },
        "DiskEncryptionKey": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "SecretUrl": {
                    "type": "string"
                },
                "SourceVaultId": {
                    "type": "string"
                }
            },
            "required": [
                "SecretUrl",
                "SourceVaultId"
            ]
        },
        "KeyEncryptionKey": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "KeyUrl": {
                    "type": "string"
                },
                "SourceVaultId": {
                    "type": "string"
                }
            },
            "required": [
                "KeyUrl",
                "SourceVaultId"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CreateOption": {
            "type": "string",
            "description": "Indicates how the snapshot is to be created. Possible values are `Copy` or `Import`. Changing this forces a new resource to be created."
        },
        "DiskSizeGb": {
            "type": "number",
            "description": "The size of the Snapshotted Disk in GB."
        },
        "Location": {
            "type": "string",
            "description": "Specifies the supported Azure location where the resource exists. Changing this forces a new resource to be created."
        },
        "Name": {
            "type": "string",
            "description": "Specifies the name of the Snapshot resource. Changing this forces a new resource to be created."
        },
        "ResourceGroupName": {
            "type": "string",
            "description": "The name of the resource group in which to create the Snapshot. Changing this forces a new resource to be created."
        },
        "SourceResourceId": {
            "type": "string",
            "description": "Specifies a reference to an existing snapshot, when `create_option` is `Copy`. Changing this forces a new resource to be created."
        },
        "SourceUri": {
            "type": "string",
            "description": "Specifies the URI to a Managed or Unmanaged Disk. Changing this forces a new resource to be created."
        },
        "StorageAccountId": {
            "type": "string",
            "description": "Specifies the ID of an storage account. Used with `source_uri` to allow authorization during import of unmanaged blobs from a different subscription. Changing this forces a new resource to be created."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            },
            "description": "A mapping of tags to assign to the resource."
        },
        "EncryptionSettings": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/EncryptionSettings"
            },
            "maxItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        },
        "DiskEncryptionKey": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/DiskEncryptionKey"
            },
            "maxItems": 1
        },
        "KeyEncryptionKey": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/KeyEncryptionKey"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "CreateOption",
        "Location",
        "Name",
        "ResourceGroupName"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}