{
    "typeName": "Terraform::AzureRM::NetappVolume",
    "description": "CloudFormation equivalent of azurerm_netapp_volume",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "ExportPolicyRule": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AllowedClients": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                },
                "CifsEnabled": {
                    "type": "boolean"
                },
                "Nfsv3Enabled": {
                    "type": "boolean"
                },
                "Nfsv4Enabled": {
                    "type": "boolean"
                },
                "ProtocolsEnabled": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                },
                "RuleIndex": {
                    "type": "number"
                },
                "UnixReadOnly": {
                    "type": "boolean"
                },
                "UnixReadWrite": {
                    "type": "boolean"
                }
            },
            "required": [
                "AllowedClients",
                "RuleIndex"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Read": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AccountName": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Location": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "PoolName": {
            "type": "string"
        },
        "Protocols": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "ResourceGroupName": {
            "type": "string"
        },
        "ServiceLevel": {
            "type": "string"
        },
        "StorageQuotaInGb": {
            "type": "number"
        },
        "SubnetId": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "VolumePath": {
            "type": "string"
        },
        "ExportPolicyRule": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/ExportPolicyRule"
            },
            "maxItems": 5
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "AccountName",
        "Location",
        "Name",
        "PoolName",
        "ResourceGroupName",
        "ServiceLevel",
        "StorageQuotaInGb",
        "SubnetId",
        "VolumePath"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}