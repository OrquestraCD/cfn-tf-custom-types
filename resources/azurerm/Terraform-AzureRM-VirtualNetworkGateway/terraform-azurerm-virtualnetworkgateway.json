    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ActiveActive": {
            "type": "boolean"
        },
        "DefaultLocalNetworkGatewayId": {
            "type": "string"
        },
        "EnableBgp": {
            "type": "boolean"
        },
        "Generation": {
            "type": "string"
        },
        "Location": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "ResourceGroupName": {
            "type": "string"
        },
        "Sku": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "Type": {
            "type": "string"
        },
        "VpnType": {
            "type": "string"
        },
        "BgpSettings": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/BgpSettings"
            },
            "maxItems": 1
        },
        "IpConfiguration": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/IpConfiguration"
            },
            "maxItems": 2,
            "minItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        },
        "VpnClientConfiguration": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/VpnClientConfiguration"
            },
            "maxItems": 1
        },
        "RevokedCertificate": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/RevokedCertificate"
            }
        },
        "RootCertificate": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/RootCertificate"
            }
        }
