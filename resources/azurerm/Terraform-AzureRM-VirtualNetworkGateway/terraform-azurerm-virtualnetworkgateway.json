{
    "typeName": "Terraform::AzureRM::VirtualNetworkGateway",
    "description": "CloudFormation equivalent of azurerm_virtual_network_gateway",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "BgpSettings": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Asn": {
                    "type": "number"
                },
                "PeerWeight": {
                    "type": "number"
                },
                "PeeringAddress": {
                    "type": "string"
                }
            },
            "required": []
        },
        "IpConfiguration": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Name": {
                    "type": "string"
                },
                "PrivateIpAddressAllocation": {
                    "type": "string"
                },
                "PublicIpAddressId": {
                    "type": "string"
                },
                "SubnetId": {
                    "type": "string"
                }
            },
            "required": [
                "SubnetId"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Read": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        },
        "VpnClientConfiguration": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AddressSpace": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                },
                "RadiusServerAddress": {
                    "type": "string"
                },
                "RadiusServerSecret": {
                    "type": "string"
                },
                "VpnClientProtocols": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                },
                "RevokedCertificate": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "$ref": "#/definitions/RevokedCertificate"
                    }
                },
                "RootCertificate": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "$ref": "#/definitions/RootCertificate"
                    }
                }
            },
            "required": [
                "AddressSpace"
            ]
        },
        "RevokedCertificate": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Name": {
                    "type": "string"
                },
                "Thumbprint": {
                    "type": "string"
                }
            },
            "required": [
                "Name",
                "Thumbprint"
            ]
        },
        "RootCertificate": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Name": {
                    "type": "string"
                },
                "PublicCertData": {
                    "type": "string"
                }
            },
            "required": [
                "Name",
                "PublicCertData"
            ]
        }
