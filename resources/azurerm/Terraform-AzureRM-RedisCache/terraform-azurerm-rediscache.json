{
    "typeName": "Terraform::AzureRM::RedisCache",
    "description": "CloudFormation equivalent of azurerm_redis_cache",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "PatchSchedule": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "DayOfWeek": {
                    "type": "string"
                },
                "StartHourUtc": {
                    "type": "number"
                }
            },
            "required": [
                "DayOfWeek"
            ]
        },
        "RedisConfiguration": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AofBackupEnabled": {
                    "type": "boolean"
                },
                "AofStorageConnectionString0": {
                    "type": "string"
                },
                "AofStorageConnectionString1": {
                    "type": "string"
                },
                "EnableAuthentication": {
                    "type": "boolean"
                },
                "MaxfragmentationmemoryReserved": {
                    "type": "number"
                },
                "MaxmemoryDelta": {
                    "type": "number"
                },
                "MaxmemoryPolicy": {
                    "type": "string"
                },
                "MaxmemoryReserved": {
                    "type": "number"
                },
                "NotifyKeyspaceEvents": {
                    "type": "string"
                },
                "RdbBackupEnabled": {
                    "type": "boolean"
                },
                "RdbBackupFrequency": {
                    "type": "number"
                },
                "RdbBackupMaxSnapshotCount": {
                    "type": "number"
                },
                "RdbStorageConnectionString": {
                    "type": "string"
                }
            },
            "required": []
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Read": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Capacity": {
            "type": "number"
        },
        "EnableNonSslPort": {
            "type": "boolean"
        },
        "Family": {
            "type": "string"
        },
        "Hostname": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Location": {
            "type": "string"
        },
        "MinimumTlsVersion": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "Port": {
            "type": "number"
        },
        "PrimaryAccessKey": {
            "type": "string"
        },
        "PrimaryConnectionString": {
            "type": "string"
        },
        "PrivateStaticIpAddress": {
            "type": "string"
        },
        "ResourceGroupName": {
            "type": "string"
        },
        "SecondaryAccessKey": {
            "type": "string"
        },
        "SecondaryConnectionString": {
            "type": "string"
        },
        "ShardCount": {
            "type": "number"
        },
        "SkuName": {
            "type": "string"
        },
        "SslPort": {
            "type": "number"
        },
        "SubnetId": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "Zones": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "PatchSchedule": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/PatchSchedule"
            }
        },
        "RedisConfiguration": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/RedisConfiguration"
            },
            "maxItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "Capacity",
        "Family",
        "Location",
        "Name",
        "ResourceGroupName",
        "SkuName"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Hostname",
        "/properties/Port",
        "/properties/PrimaryAccessKey",
        "/properties/PrimaryConnectionString",
        "/properties/SecondaryAccessKey",
        "/properties/SecondaryConnectionString",
        "/properties/SslPort"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}