{
    "typeName": "Terraform::AzureRM::ServiceFabricCluster",
    "description": "Manages a Service Fabric Cluster.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "AzureActiveDirectory": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "ClientApplicationId": {
                    "type": "string",
                    "description": "The Azure Active Directory Client ID which should be used for the Client Application."
                },
                "ClusterApplicationId": {
                    "type": "string",
                    "description": "The Azure Active Directory Cluster Application ID."
                },
                "TenantId": {
                    "type": "string",
                    "description": "The Azure Active Directory Tenant ID."
                }
            },
            "required": [
                "ClientApplicationId",
                "ClusterApplicationId",
                "TenantId"
            ]
        },
        "Certificate": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Thumbprint": {
                    "type": "string",
                    "description": "The Thumbprint of the Certificate."
                },
                "ThumbprintSecondary": {
                    "type": "string",
                    "description": "The Secondary Thumbprint of the Certificate."
                },
                "X509StoreName": {
                    "type": "string",
                    "description": "The X509 Store where the Certificate Exists, such as `My`."
                }
            },
            "required": [
                "Thumbprint",
                "X509StoreName"
            ]
        },
        "CertificateCommonNames": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "X509StoreName": {
                    "type": "string",
                    "description": "The X509 Store where the Certificate Exists, such as `My`."
                },
                "CommonNames": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "$ref": "#/definitions/CommonNames"
                    },
                    "minItems": 1
                }
            },
            "required": [
                "X509StoreName"
            ]
        },
        "ClientCertificateThumbprint": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "IsAdmin": {
                    "type": "boolean",
                    "description": "Does the Client Certificate have Admin Access to the cluster? Non-admin clients can only perform read only operations on the cluster."
                },
                "Thumbprint": {
                    "type": "string",
                    "description": "The Thumbprint associated with the Client Certificate."
                }
            },
            "required": [
                "IsAdmin",
                "Thumbprint"
            ]
        },
        "DiagnosticsConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "BlobEndpoint": {
                    "type": "string",
                    "description": "The Blob Endpoint of the Storage Account."
                },
                "ProtectedAccountKeyName": {
                    "type": "string",
                    "description": "The protected diagnostics storage key name, such as `StorageAccountKey1`."
                },
                "QueueEndpoint": {
                    "type": "string",
                    "description": "The Queue Endpoint of the Storage Account."
                },
                "StorageAccountName": {
                    "type": "string",
                    "description": "The name of the Storage Account where the Diagnostics should be sent to."
                },
                "TableEndpoint": {
                    "type": "string",
                    "description": "The Table Endpoint of the Storage Account."
                }
            },
            "required": [
                "BlobEndpoint",
                "ProtectedAccountKeyName",
                "QueueEndpoint",
                "StorageAccountName",
                "TableEndpoint"
            ]
        },
        "FabricSettings": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Name": {
                    "type": "string",
                    "description": "The name of the Fabric Setting, such as `Security` or `Federation`."
                },
                "Parameters": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "properties": {
                            "MapKey": {
                                "type": "string"
                            },
                            "MapValue": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "MapKey",
                            "MapValue"
                        ]
                    },
                    "description": "A map containing settings for the specified Fabric Setting."
                }
            },
            "required": [
                "Name"
            ]
        },
        "NodeType": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Capacities": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "properties": {
                            "MapKey": {
                                "type": "string"
                            },
                            "MapValue": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "MapKey",
                            "MapValue"
                        ]
                    },
                    "description": "The capacity tags applied to the nodes in the node type, the cluster resource manager uses these tags to understand how much resource a node has."
                },
                "ClientEndpointPort": {
                    "type": "number",
                    "description": "The Port used for the Client Endpoint for this Node Type. Changing this forces a new resource to be created."
                },
                "DurabilityLevel": {
                    "type": "string",
                    "description": "The Durability Level for this Node Type. Possible values include `Bronze`, `Gold` and `Silver`. Defaults to `Bronze`. Changing this forces a new resource to be created."
                },
                "HttpEndpointPort": {
                    "type": "number",
                    "description": "The Port used for the HTTP Endpoint for this Node Type. Changing this forces a new resource to be created."
                },
                "InstanceCount": {
                    "type": "number",
                    "description": "The number of nodes for this Node Type."
                },
                "IsPrimary": {
                    "type": "boolean",
                    "description": "Is this the Primary Node Type? Changing this forces a new resource to be created."
                },
                "Name": {
                    "type": "string",
                    "description": "The name of the Node Type. Changing this forces a new resource to be created."
                },
                "PlacementProperties": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "properties": {
                            "MapKey": {
                                "type": "string"
                            },
                            "MapValue": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "MapKey",
                            "MapValue"
                        ]
                    },
                    "description": "The placement tags applied to nodes in the node type, which can be used to indicate where certain services (workload) should run."
                },
                "ReverseProxyEndpointPort": {
                    "type": "number",
                    "description": "The Port used for the Reverse Proxy Endpoint  for this Node Type. Changing this will upgrade the cluster."
                },
                "ApplicationPorts": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/ApplicationPorts"
                    },
                    "maxItems": 1
                },
                "EphemeralPorts": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/EphemeralPorts"
                    },
                    "maxItems": 1
                }
            },
            "required": [
                "ClientEndpointPort",
                "HttpEndpointPort",
                "InstanceCount",
                "IsPrimary",
                "Name"
            ]
        },
        "ReverseProxyCertificate": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Thumbprint": {
                    "type": "string",
                    "description": "The Thumbprint of the Certificate."
                },
                "ThumbprintSecondary": {
                    "type": "string",
                    "description": "The Secondary Thumbprint of the Certificate."
                },
                "X509StoreName": {
                    "type": "string",
                    "description": "The X509 Store where the Certificate Exists, such as `My`."
                }
            },
            "required": [
                "Thumbprint",
                "X509StoreName"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Read": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        },
        "CommonNames": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "CertificateCommonName": {
                    "type": "string",
                    "description": "The common or subject name of the certificate."
                },
                "CertificateIssuerThumbprint": {
                    "type": "string",
                    "description": "The Issuer Thumbprint of the Certificate."
                }
            },
            "required": [
                "CertificateCommonName"
            ]
        },
        "ApplicationPorts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "EndPort": {
                    "type": "number",
                    "description": "The end of the Application Port Range on this Node Type."
                },
                "StartPort": {
                    "type": "number",
                    "description": "The start of the Application Port Range on this Node Type."
                }
            },
            "required": [
                "EndPort",
                "StartPort"
            ]
        },
        "EphemeralPorts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "EndPort": {
                    "type": "number",
                    "description": "The end of the Ephemeral Port Range on this Node Type."
                },
                "StartPort": {
                    "type": "number",
                    "description": "The start of the Ephemeral Port Range on this Node Type."
                }
            },
            "required": [
                "EndPort",
                "StartPort"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AddOnFeatures": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "A List of one or more features which should be enabled, such as `DnsService`."
        },
        "ClusterCodeVersion": {
            "type": "string",
            "description": "Required if Upgrade Mode set to `Manual`, Specifies the Version of the Cluster Code of the cluster."
        },
        "ClusterEndpoint": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Location": {
            "type": "string",
            "description": "Specifies the Azure Region where the Service Fabric Cluster should exist. Changing this forces a new resource to be created."
        },
        "ManagementEndpoint": {
            "type": "string",
            "description": "Specifies the Management Endpoint of the cluster such as `http://example.com`. Changing this forces a new resource to be created."
        },
        "Name": {
            "type": "string",
            "description": "The name of the Service Fabric Cluster. Changing this forces a new resource to be created."
        },
        "ReliabilityLevel": {
            "type": "string",
            "description": "Specifies the Reliability Level of the Cluster. Possible values include `None`, `Bronze`, `Silver`, `Gold` and `Platinum`."
        },
        "ResourceGroupName": {
            "type": "string",
            "description": "The name of the Resource Group in which the Service Fabric Cluster exists. Changing this forces a new resource to be created."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "A mapping of tags to assign to the resource."
        },
        "UpgradeMode": {
            "type": "string",
            "description": "Specifies the Upgrade Mode of the cluster. Possible values are `Automatic` or `Manual`."
        },
        "VmImage": {
            "type": "string",
            "description": "Specifies the Image expected for the Service Fabric Cluster, such as `Windows`. Changing this forces a new resource to be created."
        },
        "AzureActiveDirectory": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/AzureActiveDirectory"
            },
            "maxItems": 1
        },
        "Certificate": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Certificate"
            },
            "maxItems": 1
        },
        "CertificateCommonNames": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/CertificateCommonNames"
            },
            "maxItems": 1
        },
        "ClientCertificateThumbprint": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/ClientCertificateThumbprint"
            },
            "maxItems": 2
        },
        "DiagnosticsConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/DiagnosticsConfig"
            },
            "maxItems": 1
        },
        "FabricSettings": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/FabricSettings"
            }
        },
        "NodeType": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/NodeType"
            },
            "minItems": 1
        },
        "ReverseProxyCertificate": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/ReverseProxyCertificate"
            },
            "maxItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        },
        "CommonNames": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/CommonNames"
            },
            "minItems": 1
        },
        "ApplicationPorts": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/ApplicationPorts"
            },
            "maxItems": 1
        },
        "EphemeralPorts": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/EphemeralPorts"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Location",
        "ManagementEndpoint",
        "Name",
        "ReliabilityLevel",
        "ResourceGroupName",
        "UpgradeMode",
        "VmImage"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/ClusterEndpoint",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}