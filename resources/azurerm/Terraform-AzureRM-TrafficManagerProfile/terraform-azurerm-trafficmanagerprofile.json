{
    "typeName": "Terraform::AzureRM::TrafficManagerProfile",
    "description": "CloudFormation equivalent of azurerm_traffic_manager_profile",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "DnsConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "RelativeName": {
                    "type": "string"
                },
                "Ttl": {
                    "type": "number"
                }
            },
            "required": [
                "RelativeName",
                "Ttl"
            ]
        },
        "MonitorConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "ExpectedStatusCodeRanges": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                },
                "IntervalInSeconds": {
                    "type": "number"
                },
                "Path": {
                    "type": "string"
                },
                "Port": {
                    "type": "number"
                },
                "Protocol": {
                    "type": "string"
                },
                "TimeoutInSeconds": {
                    "type": "number"
                },
                "ToleratedNumberOfFailures": {
                    "type": "number"
                }
            },
            "required": [
                "Port",
                "Protocol"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Read": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Fqdn": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "ProfileStatus": {
            "type": "string"
        },
        "ResourceGroupName": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "TrafficRoutingMethod": {
            "type": "string"
        },
        "DnsConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/DnsConfig"
            },
            "maxItems": 1,
            "minItems": 1
        },
        "MonitorConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/MonitorConfig"
            },
            "maxItems": 1,
            "minItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "ResourceGroupName",
        "TrafficRoutingMethod"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Fqdn"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
