{
    "typeName": "Terraform::PagerDuty::User",
    "description": "CloudFormation equivalent of pagerduty_user",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AvatarUrl": {
            "type": "string"
        },
        "Color": {
            "type": "string"
        },
        "Description": {
            "type": "string"
        },
        "Email": {
            "type": "string"
        },
        "HtmlUrl": {
            "type": "string"
        },
        "InvitationSent": {
            "type": "boolean"
        },
        "JobTitle": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "Role": {
            "type": "string"
        },
        "Teams": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "TimeZone": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "Email",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/AvatarUrl",
        "/properties/HtmlUrl",
        "/properties/InvitationSent"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}