{
    "typeName": "Terraform::DigitalOcean::Volume",
    "description": "CloudFormation equivalent of digitalocean_volume",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string"
        },
        "DropletIds": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "number"
            }
        },
        "FilesystemLabel": {
            "type": "string"
        },
        "FilesystemType": {
            "type": "string"
        },
        "InitialFilesystemLabel": {
            "type": "string"
        },
        "InitialFilesystemType": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "Region": {
            "type": "string"
        },
        "Size": {
            "type": "number"
        },
        "SnapshotId": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Urn": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "Region",
        "Size"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/DropletIds",
        "/properties/FilesystemLabel",
        "/properties/Urn"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}