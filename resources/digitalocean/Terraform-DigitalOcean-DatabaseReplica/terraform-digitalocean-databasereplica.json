{
    "typeName": "Terraform::DigitalOcean::DatabaseReplica",
    "description": "Provides a DigitalOcean database replica resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ClusterId": {
            "type": "string",
            "description": "The ID of the original source database cluster."
        },
        "Database": {
            "type": "string"
        },
        "Host": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name for the database replica."
        },
        "Password": {
            "type": "string"
        },
        "Port": {
            "type": "number"
        },
        "PrivateHost": {
            "type": "string"
        },
        "PrivateUri": {
            "type": "string"
        },
        "Region": {
            "type": "string",
            "description": "DigitalOcean region where the replica will reside."
        },
        "Size": {
            "type": "string",
            "description": "Database Droplet size associated with the replica (ex. `db-s-1vcpu-1gb`)."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Uri": {
            "type": "string"
        },
        "User": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "ClusterId",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Database",
        "/properties/Host",
        "/properties/Id",
        "/properties/Password",
        "/properties/Port",
        "/properties/PrivateHost",
        "/properties/PrivateUri",
        "/properties/Uri",
        "/properties/User"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}