{
    "typeName": "Terraform::DigitalOcean::Droplet",
    "description": "CloudFormation equivalent of digitalocean_droplet",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Backups": {
            "type": "boolean"
        },
        "CreatedAt": {
            "type": "string"
        },
        "Disk": {
            "type": "number"
        },
        "Id": {
            "type": "string"
        },
        "Image": {
            "type": "string"
        },
        "Ipv4Address": {
            "type": "string"
        },
        "Ipv4AddressPrivate": {
            "type": "string"
        },
        "Ipv6": {
            "type": "boolean"
        },
        "Ipv6Address": {
            "type": "string"
        },
        "Ipv6AddressPrivate": {
            "type": "string"
        },
        "Locked": {
            "type": "boolean"
        },
        "Memory": {
            "type": "number"
        },
        "Monitoring": {
            "type": "boolean"
        },
        "Name": {
            "type": "string"
        },
        "PriceHourly": {
            "type": "number"
        },
        "PriceMonthly": {
            "type": "number"
        },
        "PrivateNetworking": {
            "type": "boolean"
        },
        "Region": {
            "type": "string"
        },
        "ResizeDisk": {
            "type": "boolean"
        },
        "Size": {
            "type": "string"
        },
        "SshKeys": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Status": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Urn": {
            "type": "string"
        },
        "UserData": {
            "type": "string"
        },
        "Vcpus": {
            "type": "number"
        },
        "VolumeIds": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Image",
        "Name",
        "Region",
        "Size"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/CreatedAt",
        "/properties/Disk",
        "/properties/Ipv4Address",
        "/properties/Ipv4AddressPrivate",
        "/properties/Ipv6Address",
        "/properties/Ipv6AddressPrivate",
        "/properties/Locked",
        "/properties/Memory",
        "/properties/PriceHourly",
        "/properties/PriceMonthly",
        "/properties/Status",
        "/properties/Urn",
        "/properties/Vcpus"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}