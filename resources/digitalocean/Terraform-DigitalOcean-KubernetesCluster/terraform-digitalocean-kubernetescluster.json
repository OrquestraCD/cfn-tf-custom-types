{
    "typeName": "Terraform::DigitalOcean::KubernetesCluster",
    "description": "Provides a DigitalOcean Kubernetes cluster resource. This can be used to create, delete, and modify clusters. For more information see the [official documentation](https://www.digitalocean.com/docs/kubernetes/).",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "NodePool": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AutoScale": {
                    "type": "boolean"
                },
                "Labels": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "properties": {
                            "Key": {
                                "type": "string"
                            },
                            "Value": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "Key",
                            "Value"
                        ]
                    }
                },
                "MaxNodes": {
                    "type": "number"
                },
                "MinNodes": {
                    "type": "number"
                },
                "Name": {
                    "type": "string"
                },
                "NodeCount": {
                    "type": "number"
                },
                "Size": {
                    "type": "string"
                },
                "Tags": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                }
            },
            "required": [
                "Name",
                "Size"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ClusterSubnet": {
            "type": "string"
        },
        "CreatedAt": {
            "type": "string"
        },
        "Endpoint": {
            "type": "string"
        },
        "Ipv4Address": {
            "type": "string"
        },
        "KubeConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "ClientCertificate": {
                        "type": "string"
                    },
                    "ClientKey": {
                        "type": "string"
                    },
                    "ClusterCaCertificate": {
                        "type": "string"
                    },
                    "ExpiresAt": {
                        "type": "string"
                    },
                    "Host": {
                        "type": "string"
                    },
                    "RawConfig": {
                        "type": "string"
                    },
                    "Token": {
                        "type": "string"
                    }
                }
            }
        },
        "Name": {
            "type": "string",
            "description": "A name for the node pool.\n- `size` - (Required) The slug identifier for the type of Droplet to be used as workers in the node pool.\n- `node_count` - (Optional) The number of Droplet instances in the node pool. If auto-scaling is enabled, this should only be set if the desired result is to explicitly reset the number of nodes to this value. If auto-scaling is enabled, and the node count is outside of the given min/max range, it will use the min nodes value.\n- `auto_scale` - (Optional) Enable auto-scaling of the number of nodes in the node pool within the given min/max range.\n- `min_nodes` - (Optional) If auto-scaling is enabled, this represents the minimum number of nodes that the node pool can be scaled down to.\n- `max_nodes` - (Optional) If auto-scaling is enabled, this represents the maximum number of nodes that the node pool can be scaled up to.\n- `tags` - (Optional) A list of tag names to be applied to the Kubernetes cluster.\n- `labels` - (Optional) A map of key/value pairs to apply to nodes in the pool. The labels are exposed in the Kubernetes API as labels in the metadata of the corresponding [Node resources](https://kubernetes.io/docs/concepts/architecture/nodes/)."
        },
        "Region": {
            "type": "string",
            "description": "The slug identifier for the region where the Kubernetes cluster will be created."
        },
        "ServiceSubnet": {
            "type": "string"
        },
        "Status": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "A list of tag names to be applied to the Kubernetes cluster."
        },
        "UpdatedAt": {
            "type": "string"
        },
        "Version": {
            "type": "string",
            "description": "The slug identifier for the version of Kubernetes used for the cluster. Use [doctl](https://github.com/digitalocean/doctl) to find the available versions `doctl kubernetes options versions`. (**Note:** A cluster may only be upgraded to newer versions in-place. If the version is decreased, a new resource will be created.)."
        },
        "NodePool": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/NodePool"
            },
            "maxItems": 1,
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "Region",
        "Version"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/ClusterSubnet",
        "/properties/CreatedAt",
        "/properties/Endpoint",
        "/properties/Ipv4Address",
        "/properties/KubeConfig",
        "/properties/ServiceSubnet",
        "/properties/Status",
        "/properties/UpdatedAt"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}