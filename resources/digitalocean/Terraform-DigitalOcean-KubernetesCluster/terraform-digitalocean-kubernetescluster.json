{
    "typeName": "Terraform::DigitalOcean::KubernetesCluster",
    "description": "CloudFormation equivalent of digitalocean_kubernetes_cluster",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "NodePool": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AutoScale": {
                    "type": "boolean"
                },
                "Labels": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "properties": {
                            "Key": {
                                "type": "string"
                            },
                            "Value": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "Key",
                            "Value"
                        ]
                    }
                },
                "MaxNodes": {
                    "type": "number"
                },
                "MinNodes": {
                    "type": "number"
                },
                "Name": {
                    "type": "string"
                },
                "NodeCount": {
                    "type": "number"
                },
                "Size": {
                    "type": "string"
                },
                "Tags": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                }
            },
            "required": [
                "Name",
                "Size"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ClusterSubnet": {
            "type": "string"
        },
        "CreatedAt": {
            "type": "string"
        },
        "Endpoint": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Ipv4Address": {
            "type": "string"
        },
        "KubeConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "ClientCertificate": {
                        "type": "string"
                    },
                    "ClientKey": {
                        "type": "string"
                    },
                    "ClusterCaCertificate": {
                        "type": "string"
                    },
                    "ExpiresAt": {
                        "type": "string"
                    },
                    "Host": {
                        "type": "string"
                    },
                    "RawConfig": {
                        "type": "string"
                    },
                    "Token": {
                        "type": "string"
                    }
                }
            }
        },
        "Name": {
            "type": "string"
        },
        "Region": {
            "type": "string"
        },
        "ServiceSubnet": {
            "type": "string"
        },
        "Status": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "UpdatedAt": {
            "type": "string"
        },
        "Version": {
            "type": "string"
        },
        "NodePool": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/NodePool"
            },
            "maxItems": 1,
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "Region",
        "Version"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/ClusterSubnet",
        "/properties/CreatedAt",
        "/properties/Endpoint",
        "/properties/Ipv4Address",
        "/properties/KubeConfig",
        "/properties/ServiceSubnet",
        "/properties/Status",
        "/properties/UpdatedAt"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}