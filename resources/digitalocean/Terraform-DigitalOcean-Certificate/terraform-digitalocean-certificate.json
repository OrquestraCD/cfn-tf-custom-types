{
    "typeName": "Terraform::DigitalOcean::Certificate",
    "description": "CloudFormation equivalent of digitalocean_certificate",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CertificateChain": {
            "type": "string"
        },
        "Domains": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "LeafCertificate": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "NotAfter": {
            "type": "string"
        },
        "PrivateKey": {
            "type": "string"
        },
        "Sha1Fingerprint": {
            "type": "string"
        },
        "State": {
            "type": "string"
        },
        "Type": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/NotAfter",
        "/properties/Sha1Fingerprint",
        "/properties/State"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}