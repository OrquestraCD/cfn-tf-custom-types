{
    "typeName": "Terraform::Dyn::Record",
    "description": "Provides a Dyn DNS record resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Fqdn": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name of the record."
        },
        "Ttl": {
            "type": "string",
            "description": "The TTL of the record. Default uses the zone default."
        },
        "Type": {
            "type": "string",
            "description": "The type of the record."
        },
        "Value": {
            "type": "string",
            "description": "The value of the record."
        },
        "Zone": {
            "type": "string",
            "description": "The DNS zone to add the record to."
        }
    },
    "additionalProperties": false,
    "required": [
        "Type",
        "Value",
        "Zone"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Fqdn",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}