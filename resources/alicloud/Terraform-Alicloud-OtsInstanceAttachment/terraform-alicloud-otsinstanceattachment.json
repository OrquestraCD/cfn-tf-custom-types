{
    "typeName": "Terraform::Alicloud::OtsInstanceAttachment",
    "description": "This resource will help you to bind a VPC to an OTS instance.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "InstanceName": {
            "type": "string",
            "description": "The name of the OTS instance."
        },
        "VpcId": {
            "type": "string"
        },
        "VpcName": {
            "type": "string",
            "description": "The name of attaching VPC to instance."
        },
        "VswitchId": {
            "type": "string",
            "description": "The ID of attaching VSwitch to instance."
        }
    },
    "additionalProperties": false,
    "required": [
        "InstanceName",
        "VpcName",
        "VswitchId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/VpcId"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}