{
    "typeName": "Terraform::Alicloud::AlikafkaInstance",
    "description": "CloudFormation equivalent of alicloud_alikafka_instance",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "DeployType": {
            "type": "number"
        },
        "DiskSize": {
            "type": "number"
        },
        "DiskType": {
            "type": "number"
        },
        "EipMax": {
            "type": "number"
        },
        "Id": {
            "type": "string"
        },
        "IoMax": {
            "type": "number"
        },
        "Name": {
            "type": "string"
        },
        "PaidType": {
            "type": "string"
        },
        "SpecType": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "TopicQuota": {
            "type": "number"
        },
        "VpcId": {
            "type": "string"
        },
        "VswitchId": {
            "type": "string"
        },
        "ZoneId": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "DeployType",
        "DiskSize",
        "DiskType",
        "IoMax",
        "TopicQuota",
        "VswitchId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/VpcId",
        "/properties/ZoneId"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}