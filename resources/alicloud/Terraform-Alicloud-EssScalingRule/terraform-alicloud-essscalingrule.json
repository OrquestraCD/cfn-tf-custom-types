            "type": "number"
        },
        "Id": {
            "type": "string"
        },
        "MetricName": {
            "type": "string"
        },
        "ScalingGroupId": {
            "type": "string"
        },
        "ScalingRuleName": {
            "type": "string"
        },
        "ScalingRuleType": {
            "type": "string"
        },
        "TargetValue": {
            "type": "number"
        },
        "StepAdjustment": {
            "type": "array",
