{
    "typeName": "Terraform::Alicloud::PvtzZoneAttachment",
    "description": "Provides vpcs bound to Alicloud Private Zone resource.\n\n-> **NOTE:** Terraform will auto bind vpc to a Private Zone while it uses `alicloud_pvtz_zone_attachment` to build a Private Zone and VPC binding resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Vpcs": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "RegionId": {
                    "type": "string"
                },
                "VpcId": {
                    "type": "string"
                }
            },
            "required": [
                "VpcId"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Lang": {
            "type": "string",
            "description": "The language of code."
        },
        "UserClientIp": {
            "type": "string",
            "description": "The user custom IP address."
        },
        "VpcIds": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "The id List of the VPC with the same region, for example:[\"vpc-1\",\"vpc-2\"]."
        },
        "ZoneId": {
            "type": "string",
            "description": "The name of the Private Zone Record."
        },
        "Vpcs": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Vpcs"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "ZoneId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}