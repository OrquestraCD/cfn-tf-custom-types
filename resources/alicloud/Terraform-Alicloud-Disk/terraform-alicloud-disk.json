        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AvailabilityZone": {
            "type": "string"
        },
        "Category": {
            "type": "string"
        },
        "DeleteAutoSnapshot": {
            "type": "boolean"
        },
        "DeleteWithInstance": {
            "type": "boolean"
        },
        "Description": {
            "type": "string"
        },
        "EnableAutoSnapshot": {
            "type": "boolean"
        },
        "Encrypted": {
            "type": "boolean"
        },
        "Name": {
            "type": "string"
        },
        "ResourceGroupId": {
            "type": "string"
        },
        "Size": {
            "type": "number"
        },
        "SnapshotId": {
            "type": "string"
        },
        "Status": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
