{
    "typeName": "Terraform::Alicloud::PolardbAccount",
    "description": "CloudFormation equivalent of alicloud_polardb_account",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AccountDescription": {
            "type": "string"
        },
        "AccountName": {
            "type": "string"
        },
        "AccountPassword": {
            "type": "string"
        },
        "AccountType": {
            "type": "string"
        },
        "DbClusterId": {
            "type": "string"
        },
        "KmsEncryptedPassword": {
            "type": "string"
        },
        "KmsEncryptionContext": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "AccountName",
        "AccountPassword",
        "DbClusterId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}