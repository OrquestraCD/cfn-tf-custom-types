{
    "typeName": "Terraform::Alicloud::OtsTable",
    "description": "CloudFormation equivalent of alicloud_ots_table",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "PrimaryKey": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Name": {
                    "type": "string"
                },
                "Type": {
                    "type": "string"
                }
            },
            "required": [
                "Name",
                "Type"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "DeviationCellVersionInSec": {
            "type": "string"
        },
        "InstanceName": {
            "type": "string"
        },
        "MaxVersion": {
            "type": "number"
        },
        "TableName": {
            "type": "string"
        },
        "TimeToLive": {
            "type": "number"
        },
        "PrimaryKey": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/PrimaryKey"
            },
            "maxItems": 4,
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "InstanceName",
        "MaxVersion",
        "TableName",
        "TimeToLive"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}