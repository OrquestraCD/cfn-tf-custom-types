{
    "typeName": "Terraform::Alicloud::CdnDomainNew",
    "description": "CloudFormation equivalent of alicloud_cdn_domain_new",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "CertificateConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "CertName": {
                    "type": "string"
                },
                "CertType": {
                    "type": "string"
                },
                "ForceSet": {
                    "type": "string"
                },
                "PrivateKey": {
                    "type": "string"
                },
                "ServerCertificate": {
                    "type": "string"
                },
                "ServerCertificateStatus": {
                    "type": "string"
                }
            },
            "required": []
        },
        "Sources": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Content": {
                    "type": "string"
                },
                "Port": {
                    "type": "number"
                },
                "Priority": {
                    "type": "number"
                },
                "Type": {
                    "type": "string"
                },
                "Weight": {
                    "type": "number"
                }
            },
            "required": [
                "Content",
                "Type"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CdnType": {
            "type": "string"
        },
        "DomainName": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "ResourceGroupId": {
            "type": "string"
        },
        "Scope": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "CertificateConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/CertificateConfig"
            },
            "maxItems": 1
        },
        "Sources": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Sources"
            },
            "maxItems": 1,
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "CdnType",
        "DomainName"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
