{
    "typeName": "Terraform::Alicloud::Havip",
    "description": "Provides a HaVip resource.\n\n-> **NOTE:** Terraform will auto build havip instance  while it uses `alicloud_havip` to build a havip resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "The description of the HaVip instance."
        },
        "Id": {
            "type": "string"
        },
        "IpAddress": {
            "type": "string",
            "description": "The ip address of the HaVip. If not filled, the default will be assigned one from the vswitch."
        },
        "VswitchId": {
            "type": "string",
            "description": "The vswitch_id of the HaVip, the field can't be changed."
        }
    },
    "additionalProperties": false,
    "required": [
        "VswitchId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}