{
    "typeName": "Terraform::Alicloud::AdbBackupPolicy",
    "description": "CloudFormation equivalent of alicloud_adb_backup_policy",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "BackupRetentionPeriod": {
            "type": "string"
        },
        "DbClusterId": {
            "type": "string"
        },
        "PreferredBackupPeriod": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "PreferredBackupTime": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "DbClusterId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/BackupRetentionPeriod"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}