{
    "typeName": "Terraform::Alicloud::PolardbAccountPrivilege",
    "description": "Provides a PolarDB account privilege resource and used to grant several database some access privilege. A database can be granted by multiple account.\n\n-> **NOTE:** Available in v1.67.0+.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AccountName": {
            "type": "string",
            "description": "A specified account name."
        },
        "AccountPrivilege": {
            "type": "string",
            "description": "The privilege of one account access database. Valid values: [\"ReadOnly\", \"ReadWrite\"]. Default to \"ReadOnly\"."
        },
        "DbClusterId": {
            "type": "string",
            "description": "The Id of cluster in which account belongs."
        },
        "DbNames": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "List of specified database name."
        }
    },
    "additionalProperties": false,
    "required": [
        "AccountName",
        "DbClusterId",
        "DbNames"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}