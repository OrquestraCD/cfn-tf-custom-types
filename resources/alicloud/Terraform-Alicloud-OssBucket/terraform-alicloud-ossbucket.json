{
    "typeName": "Terraform::Alicloud::OssBucket",
    "description": "Provides a resource to create a oss bucket and set its attribution.\n\n-> **NOTE:** The bucket namespace is shared by all users of the OSS system. Please set bucket name as unique as possible.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "CorsRule": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AllowedHeaders": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                },
                "AllowedMethods": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                },
                "AllowedOrigins": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                },
                "ExposeHeaders": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                },
                "MaxAgeSeconds": {
                    "type": "number"
                }
            },
            "required": [
                "AllowedMethods",
                "AllowedOrigins"
            ]
        },
        "LifecycleRule": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Enabled": {
                    "type": "boolean"
                },
                "Id": {
                    "type": "string"
                },
                "Prefix": {
                    "type": "string"
                },
                "Expiration": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "$ref": "#/definitions/Expiration"
                    }
                },
                "Transitions": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "$ref": "#/definitions/Transitions"
                    }
                }
            },
            "required": [
                "Enabled",
                "Prefix"
            ]
        },
        "Logging": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "TargetBucket": {
                    "type": "string"
                },
                "TargetPrefix": {
                    "type": "string"
                }
            },
            "required": [
                "TargetBucket"
            ]
        },
        "RefererConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AllowEmpty": {
                    "type": "boolean"
                },
                "Referers": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                }
            },
            "required": [
                "Referers"
            ]
        },
        "ServerSideEncryptionRule": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "SseAlgorithm": {
                    "type": "string"
                }
            },
            "required": [
                "SseAlgorithm"
            ]
        },
        "Versioning": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Status": {
                    "type": "string"
                }
            },
            "required": [
                "Status"
            ]
        },
        "Website": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "ErrorDocument": {
                    "type": "string"
                },
                "IndexDocument": {
                    "type": "string"
                }
            },
            "required": [
                "IndexDocument"
            ]
        },
        "Expiration": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Date": {
                    "type": "string"
                },
                "Days": {
                    "type": "number"
                }
            },
            "required": []
        },
        "Transitions": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "CreatedBeforeDate": {
                    "type": "string"
                },
                "Days": {
                    "type": "number"
                },
                "StorageClass": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Acl": {
            "type": "string",
            "description": "The [canned ACL](https://www.alibabacloud.com/help/doc-detail/31898.htm) to apply. Defaults to \"private\"."
        },
        "Bucket": {
            "type": "string",
            "description": "The name of the bucket. If omitted, Terraform will assign a random and unique name."
        },
        "CreationDate": {
            "type": "string"
        },
        "ExtranetEndpoint": {
            "type": "string"
        },
        "ForceDestroy": {
            "type": "boolean",
            "description": "A boolean that indicates all objects should be deleted from the bucket so that the bucket can be destroyed without error. These objects are not recoverable. Defaults to \"false\"."
        },
        "Id": {
            "type": "string"
        },
        "IntranetEndpoint": {
            "type": "string"
        },
        "Location": {
            "type": "string"
        },
        "LoggingIsenable": {
            "type": "boolean",
            "description": "The flag of using logging enable container. Defaults true."
        },
        "Owner": {
            "type": "string"
        },
        "Policy": {
            "type": "string",
            "description": "Json format text of bucket policy [bucket policy management](https://www.alibabacloud.com/help/doc-detail/100680.htm) (documented below)."
        },
        "StorageClass": {
            "type": "string",
            "description": "The [storage class](https://www.alibabacloud.com/help/doc-detail/51374.htm) to apply. Can be \"Standard\", \"IA\" and \"Archive\". Defaults to \"Standard\"."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "A mapping of tags to assign to the bucket. The items are no more than 10 for a bucket."
        },
        "CorsRule": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/CorsRule"
            },
            "maxItems": 10
        },
        "LifecycleRule": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/LifecycleRule"
            },
            "maxItems": 1000
        },
        "Logging": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Logging"
            },
            "maxItems": 1
        },
        "RefererConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/RefererConfig"
            },
            "maxItems": 1
        },
        "ServerSideEncryptionRule": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/ServerSideEncryptionRule"
            },
            "maxItems": 1
        },
        "Versioning": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Versioning"
            },
            "maxItems": 1
        },
        "Website": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Website"
            },
            "maxItems": 1
        },
        "Expiration": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Expiration"
            }
        },
        "Transitions": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Transitions"
            }
        }
    },
    "additionalProperties": false,
    "required": [],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/CreationDate",
        "/properties/ExtranetEndpoint",
        "/properties/Id",
        "/properties/IntranetEndpoint",
        "/properties/Location",
        "/properties/Owner"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}