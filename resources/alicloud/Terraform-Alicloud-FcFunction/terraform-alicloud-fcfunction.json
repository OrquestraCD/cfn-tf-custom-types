{
    "typeName": "Terraform::Alicloud::FcFunction",
    "description": "CloudFormation equivalent of alicloud_fc_function",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CodeChecksum": {
            "type": "string"
        },
        "Description": {
            "type": "string"
        },
        "EnvironmentVariables": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "Filename": {
            "type": "string"
        },
        "FunctionId": {
            "type": "string"
        },
        "Handler": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "LastModified": {
            "type": "string"
        },
        "MemorySize": {
            "type": "number"
        },
        "Name": {
            "type": "string"
        },
        "NamePrefix": {
            "type": "string"
        },
        "OssBucket": {
            "type": "string"
        },
        "OssKey": {
            "type": "string"
        },
        "Runtime": {
            "type": "string"
        },
        "Service": {
            "type": "string"
        },
        "Timeout": {
            "type": "number"
        }
    },
    "additionalProperties": false,
    "required": [
        "Handler",
        "Runtime",
        "Service"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/FunctionId",
        "/properties/LastModified"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}