{
    "typeName": "Terraform::Alicloud::KeyPair",
    "description": "Provides a key pair resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "FingerPrint": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "KeyFile": {
            "type": "string",
            "description": "The name of file to save your new key pair's private key. Strongly suggest you to specified it when you creating key pair, otherwise, you wouldn't get its private key ever."
        },
        "KeyName": {
            "type": "string",
            "description": "The key pair's name. It is the only in one Alicloud account."
        },
        "KeyNamePrefix": {
            "type": "string",
            "description": "The key pair name's prefix. It is conflict with `key_name`. If it is specified, terraform will using it to build the only key name."
        },
        "PublicKey": {
            "type": "string",
            "description": "You can import an existing public key and using Alicloud key pair to manage it."
        },
        "ResourceGroupId": {
            "type": "string",
            "description": "The Id of resource group which the key pair belongs."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "A mapping of tags to assign to the resource.\n-> **NOTE:** If `key_name` and `key_name_prefix` are not set, terraform will produce a specified ID to replace."
        }
    },
    "additionalProperties": false,
    "required": [],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/FingerPrint",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}