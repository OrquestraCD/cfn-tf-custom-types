{
    "typeName": "Terraform::Cloudflare::SpectrumApplication",
    "description": "CloudFormation equivalent of cloudflare_spectrum_application",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Dns": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Name": {
                    "type": "string"
                },
                "Type": {
                    "type": "string"
                }
            },
            "required": [
                "Name",
                "Type"
            ]
        },
        "OriginDns": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Name": {
                    "type": "string"
                }
            },
            "required": [
                "Name"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "IpFirewall": {
            "type": "boolean"
        },
        "OriginDirect": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "OriginPort": {
            "type": "number"
        },
        "Protocol": {
            "type": "string"
        },
        "ProxyProtocol": {
            "type": "string"
        },
        "Tls": {
            "type": "string"
        },
        "TrafficType": {
            "type": "string"
        },
        "ZoneId": {
            "type": "string"
        },
        "Dns": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Dns"
            },
            "maxItems": 1,
            "minItems": 1
        },
        "OriginDns": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/OriginDns"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Protocol",
        "ZoneId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}