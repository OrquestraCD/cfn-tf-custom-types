{
    "typeName": "Terraform::Cloudflare::OriginCaCertificate",
    "description": "CloudFormation equivalent of cloudflare_origin_ca_certificate",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Certificate": {
            "type": "string"
        },
        "Csr": {
            "type": "string"
        },
        "ExpiresOn": {
            "type": "string"
        },
        "Hostnames": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Id": {
            "type": "string"
        },
        "RequestType": {
            "type": "string"
        },
        "RequestedValidity": {
            "type": "number"
        }
    },
    "additionalProperties": false,
    "required": [
        "Csr",
        "Hostnames",
        "RequestType"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Certificate",
        "/properties/ExpiresOn"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}