{
    "typeName": "Terraform::Cloudflare::Zone",
    "description": "CloudFormation equivalent of cloudflare_zone",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "JumpStart": {
            "type": "boolean"
        },
        "Meta": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "NameServers": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "Paused": {
            "type": "boolean"
        },
        "Plan": {
            "type": "string"
        },
        "Status": {
            "type": "string"
        },
        "Type": {
            "type": "string"
        },
        "VanityNameServers": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "VerificationKey": {
            "type": "string"
        },
        "Zone": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "Zone"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Meta",
        "/properties/NameServers",
        "/properties/Status",
        "/properties/VanityNameServers",
        "/properties/VerificationKey"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}