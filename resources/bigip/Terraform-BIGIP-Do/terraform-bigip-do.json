{
    "typeName": "Terraform::BIGIP::Do",
    "description": "`bigip_do` provides details about bigip do resource\n\nThis resource is helpful to configure do declarative JSON on BIG-IP.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "DoJson": {
            "type": "string",
            "description": "Name of the of the Declarative DO JSON file."
        },
        "Id": {
            "type": "string"
        },
        "TenantName": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "DoJson",
        "TenantName"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}