{
    "typeName": "Terraform::OCI::DatabaseExadataIormConfig",
    "description": "This resource provides the Exadata Iorm Config resource in Oracle Cloud Infrastructure Database service.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "DbPlans": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "DbName": {
                    "type": "string"
                },
                "Share": {
                    "type": "number"
                }
            },
            "required": [
                "DbName",
                "Share"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "DbSystemId": {
            "type": "string",
            "description": "The DB system [OCID](https://docs.cloud.oracle.com/iaas/Content/General/Concepts/identifiers.htm)."
        },
        "Id": {
            "type": "string"
        },
        "LifecycleDetails": {
            "type": "string"
        },
        "Objective": {
            "type": "string",
            "description": "Value for the IORM objective Default is \"Auto\"."
        },
        "State": {
            "type": "string"
        },
        "DbPlans": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/DbPlans"
            },
            "minItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "DbSystemId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/LifecycleDetails",
        "/properties/State"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}