{
    "typeName": "Terraform::OCI::CoreInstancePool",
    "description": "CloudFormation equivalent of oci_core_instance_pool",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "LoadBalancers": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "BackendSetName": {
                    "type": "string"
                },
                "LoadBalancerId": {
                    "type": "string"
                },
                "Port": {
                    "type": "number"
                },
                "VnicSelection": {
                    "type": "string"
                }
            },
            "required": [
                "BackendSetName",
                "LoadBalancerId",
                "Port",
                "VnicSelection"
            ]
        },
        "PlacementConfigurations": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AvailabilityDomain": {
                    "type": "string"
                },
                "FaultDomains": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                },
                "PrimarySubnetId": {
                    "type": "string"
                },
                "SecondaryVnicSubnets": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/SecondaryVnicSubnets"
                    }
                }
            },
            "required": [
                "AvailabilityDomain",
                "PrimarySubnetId"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        },
        "SecondaryVnicSubnets": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "DisplayName": {
                    "type": "string"
                },
                "SubnetId": {
                    "type": "string"
                }
            },
            "required": [
                "SubnetId"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ActualSize": {
            "type": "number"
        },
        "CompartmentId": {
            "type": "string"
        },
        "DefinedTags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "DisplayName": {
            "type": "string"
        },
        "FreeformTags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "InstanceConfigurationId": {
            "type": "string"
        },
        "Size": {
            "type": "number"
        },
        "State": {
            "type": "string"
        },
        "TimeCreated": {
            "type": "string"
        },
        "LoadBalancers": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/LoadBalancers"
            }
        },
        "PlacementConfigurations": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/PlacementConfigurations"
            },
            "minItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        },
        "SecondaryVnicSubnets": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/SecondaryVnicSubnets"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "CompartmentId",
        "InstanceConfigurationId",
        "Size"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/ActualSize",
        "/properties/TimeCreated"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}