{
    "typeName": "Terraform::OCI::DatabaseVmCluster",
    "description": "This resource provides the Vm Cluster resource in Oracle Cloud Infrastructure Database service.\n\nCreates a VM cluster.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CompartmentId": {
            "type": "string",
            "description": "(Updatable) The [OCID](https://docs.cloud.oracle.com/iaas/Content/General/Concepts/identifiers.htm) of the compartment."
        },
        "CpuCoreCount": {
            "type": "number",
            "description": "(Updatable) The number of CPU cores to enable for the VM cluster."
        },
        "CpusEnabled": {
            "type": "number"
        },
        "DataStorageSizeInTbs": {
            "type": "number"
        },
        "DefinedTags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "(Updatable) Defined tags for this resource. Each key is predefined and scoped to a namespace. For more information, see [Resource Tags](https://docs.cloud.oracle.com/iaas/Content/General/Concepts/resourcetags.htm)."
        },
        "DisplayName": {
            "type": "string",
            "description": "The user-friendly name for the VM cluster. The name does not need to be unique."
        },
        "ExadataInfrastructureId": {
            "type": "string",
            "description": "The [OCID](https://docs.cloud.oracle.com/iaas/Content/General/Concepts/identifiers.htm) of the Exadata infrastructure."
        },
        "FreeformTags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "(Updatable) Free-form tags for this resource. Each tag is a simple key-value pair with no predefined name, type, or namespace. For more information, see [Resource Tags](https://docs.cloud.oracle.com/iaas/Content/General/Concepts/resourcetags.htm).  Example: `{\"Department\": \"Finance\"}`."
        },
        "GiVersion": {
            "type": "string",
            "description": "The Oracle Grid Infrastructure software version for the VM cluster."
        },
        "Id": {
            "type": "string"
        },
        "IsLocalBackupEnabled": {
            "type": "boolean",
            "description": "If true, database backup on local Exadata storage is configured for the VM cluster. If false, database backup on local Exadata storage is not available in the VM cluster."
        },
        "IsSparseDiskgroupEnabled": {
            "type": "boolean",
            "description": "If true, the sparse disk group is configured for the VM cluster. If false, the sparse disk group is not created."
        },
        "LicenseModel": {
            "type": "string",
            "description": "(Updatable) The Oracle license model that applies to the VM cluster. The default is BRING_YOUR_OWN_LICENSE."
        },
        "LifecycleDetails": {
            "type": "string"
        },
        "Shape": {
            "type": "string"
        },
        "SshPublicKeys": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "(Updatable) The public key portion of one or more key pairs used for SSH access to the VM cluster."
        },
        "State": {
            "type": "string"
        },
        "TimeCreated": {
            "type": "string"
        },
        "TimeZone": {
            "type": "string",
            "description": "The time zone to use for the VM cluster. For details, see [DB System Time Zones](https://docs.cloud.oracle.com/iaas/Content/Database/References/timezones.htm)."
        },
        "VmClusterNetworkId": {
            "type": "string",
            "description": "The [OCID](https://docs.cloud.oracle.com/iaas/Content/General/Concepts/identifiers.htm) of the VM cluster network."
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "CompartmentId",
        "CpuCoreCount",
        "DisplayName",
        "ExadataInfrastructureId",
        "GiVersion",
        "SshPublicKeys",
        "VmClusterNetworkId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/CpusEnabled",
        "/properties/DataStorageSizeInTbs",
        "/properties/Id",
        "/properties/LifecycleDetails",
        "/properties/Shape",
        "/properties/State",
        "/properties/TimeCreated"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}