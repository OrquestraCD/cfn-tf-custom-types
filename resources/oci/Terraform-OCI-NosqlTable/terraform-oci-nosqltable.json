{
    "typeName": "Terraform::OCI::NosqlTable",
    "description": "CloudFormation equivalent of oci_nosql_table",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "TableLimits": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MaxReadUnits": {
                    "type": "number"
                },
                "MaxStorageInGbs": {
                    "type": "number"
                },
                "MaxWriteUnits": {
                    "type": "number"
                }
            },
            "required": [
                "MaxReadUnits",
                "MaxStorageInGbs",
                "MaxWriteUnits"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CompartmentId": {
            "type": "string"
        },
        "DdlStatement": {
            "type": "string"
        },
        "DefinedTags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "FreeformTags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "Id": {
            "type": "string"
        },
        "LifecycleDetails": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "Schema": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Columns": {
                        "type": "array",
                        "insertionOrder": false,
                        "items": {
                            "type": "object",
                            "additionalProperties": false,
                            "properties": {
                                "DefaultValue": {
                                    "type": "string"
                                },
                                "IsNullable": {
                                    "type": "boolean"
                                },
                                "Name": {
                                    "type": "string"
                                },
                                "Type": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "PrimaryKey": {
                        "type": "array",
                        "insertionOrder": false,
                        "items": {
                            "type": "string"
                        }
                    },
                    "ShardKey": {
                        "type": "array",
                        "insertionOrder": false,
                        "items": {
                            "type": "string"
                        }
                    },
                    "Ttl": {
                        "type": "number"
                    }
                }
            }
        },
        "State": {
            "type": "string"
        },
        "TimeCreated": {
            "type": "string"
        },
        "TimeUpdated": {
            "type": "string"
        },
        "TableLimits": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/TableLimits"
            },
            "maxItems": 1,
            "minItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "CompartmentId",
        "DdlStatement",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/LifecycleDetails",
        "/properties/Schema",
        "/properties/State",
        "/properties/TimeCreated",
        "/properties/TimeUpdated"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}