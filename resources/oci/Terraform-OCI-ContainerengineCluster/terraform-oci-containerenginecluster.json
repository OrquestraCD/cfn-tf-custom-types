{
    "typeName": "Terraform::OCI::ContainerengineCluster",
    "description": "This resource provides the Cluster resource in Oracle Cloud Infrastructure Container Engine service.\n\nCreate a new cluster.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/oci/Terraform-OCI-ContainerengineCluster/docs/README.md",
    "definitions": {
        "Options": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "ServiceLbSubnetIds": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                },
                "AddOns": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/AddOns"
                    },
                    "maxItems": 1
                },
                "KubernetesNetworkConfig": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/KubernetesNetworkConfig"
                    },
                    "maxItems": 1
                }
            },
            "required": []
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        },
        "AddOns": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "IsKubernetesDashboardEnabled": {
                    "type": "boolean"
                },
                "IsTillerEnabled": {
                    "type": "boolean"
                }
            },
            "required": []
        },
        "KubernetesNetworkConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "PodsCidr": {
                    "type": "string"
                },
                "ServicesCidr": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AvailableKubernetesUpgrades": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "CompartmentId": {
            "type": "string",
            "description": "The OCID of the compartment in which to create the cluster."
        },
        "Endpoints": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Kubernetes": {
                        "type": "string"
                    }
                }
            }
        },
        "Id": {
            "type": "string"
        },
        "KmsKeyId": {
            "type": "string",
            "description": "The OCID of the KMS key to be used as the master encryption key for Kubernetes secret encryption. When used, `kubernetesVersion` must be at least `v1.13.0`."
        },
        "KubernetesVersion": {
            "type": "string",
            "description": "(Updatable) The version of Kubernetes to install into the cluster masters."
        },
        "LifecycleDetails": {
            "type": "string"
        },
        "Metadata": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "CreatedByUserId": {
                        "type": "string"
                    },
                    "CreatedByWorkRequestId": {
                        "type": "string"
                    },
                    "DeletedByUserId": {
                        "type": "string"
                    },
                    "DeletedByWorkRequestId": {
                        "type": "string"
                    },
                    "TimeCreated": {
                        "type": "string"
                    },
                    "TimeDeleted": {
                        "type": "string"
                    },
                    "TimeUpdated": {
                        "type": "string"
                    },
                    "UpdatedByUserId": {
                        "type": "string"
                    },
                    "UpdatedByWorkRequestId": {
                        "type": "string"
                    }
                }
            }
        },
        "Name": {
            "type": "string",
            "description": "(Updatable) The name of the cluster. Avoid entering confidential information."
        },
        "State": {
            "type": "string"
        },
        "VcnId": {
            "type": "string",
            "description": "The OCID of the virtual cloud network (VCN) in which to create the cluster."
        },
        "Options": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Options"
            },
            "maxItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        },
        "AddOns": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/AddOns"
            },
            "maxItems": 1
        },
        "KubernetesNetworkConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/KubernetesNetworkConfig"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "CompartmentId",
        "KubernetesVersion",
        "Name",
        "VcnId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/AvailableKubernetesUpgrades",
        "/properties/Endpoints",
        "/properties/Id",
        "/properties/LifecycleDetails",
        "/properties/Metadata",
        "/properties/State"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}