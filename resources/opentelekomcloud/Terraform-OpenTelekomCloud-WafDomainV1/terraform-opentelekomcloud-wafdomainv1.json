        },
        "AccessCode": {
            "type": "string"
        },
        "AccessStatus": {
            "type": "number"
        },
        "CertificateId": {
            "type": "string"
        },
        "Cname": {
            "type": "string"
        },
        "Hostname": {
            "type": "string"
        },
        "PolicyId": {
            "type": "string"
        },
        "ProtectStatus": {
            "type": "number"
        },
        "Protocol": {
            "type": "string"
        },
        "Proxy": {
            "type": "boolean"
        },
        "SipHeaderList": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "SipHeaderName": {
            "type": "string"
        },
        "SubDomain": {
            "type": "string"
        },
        "TxtCode": {
            "type": "string"
        },
        "Server": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Server"
            },
            "minItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
