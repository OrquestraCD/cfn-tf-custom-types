                "FrontProtocol": {
                    "type": "string"
                },
                "Port": {
                    "type": "string"
                }
            },
            "required": [
                "Address",
                "BackProtocol",
                "FrontProtocol",
                "Port"
            ]
        },
        "Timeouts": {
            "type": "object",
