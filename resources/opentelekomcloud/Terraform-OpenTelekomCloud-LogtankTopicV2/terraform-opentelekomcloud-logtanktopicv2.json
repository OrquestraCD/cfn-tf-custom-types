{
    "typeName": "Terraform::OpenTelekomCloud::LogtankTopicV2",
    "description": "Manage a log topic resource within OpenTelekomCloud.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "GroupId": {
            "type": "string",
            "description": "Specifies the ID of a created log group.\nChanging this parameter will create a new resource."
        },
        "Id": {
            "type": "string"
        },
        "IndexEnabled": {
            "type": "boolean"
        },
        "TopicName": {
            "type": "string",
            "description": "Specifies the log topic name.\nChanging this parameter will create a new resource."
        }
    },
    "additionalProperties": false,
    "required": [
        "GroupId",
        "TopicName"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/IndexEnabled"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}