{
    "typeName": "Terraform::OpenTelekomCloud::IdentityAgencyV3",
    "description": "Manages an agency resource within Opentelekomcloud.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "ProjectRole": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Project": {
                    "type": "string",
                    "description": "The name of project."
                },
                "Roles": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    },
                    "description": "An array of role names."
                }
            },
            "required": [
                "Project",
                "Roles"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CreateTime": {
            "type": "string"
        },
        "DelegatedDomainName": {
            "type": "string",
            "description": "The name of delegated domain."
        },
        "Description": {
            "type": "string",
            "description": "Provides supplementary information about the\nagency. The value is a string of 0 to 255 characters."
        },
        "DomainRoles": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "An array of role names which stand for the\npermissionis to be granted to agency on domain."
        },
        "Duration": {
            "type": "string"
        },
        "ExpireTime": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name of agency. The name is a string of 1 to 64\ncharacters."
        },
        "ProjectRole": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/ProjectRole"
            }
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "DelegatedDomainName",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/CreateTime",
        "/properties/Duration",
        "/properties/ExpireTime",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}