{
    "typeName": "Terraform::OpenTelekomCloud::KmsKeyV1",
    "description": "Manages a V1 key resource within KMS.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CreationDate": {
            "type": "string"
        },
        "DefaultKeyFlag": {
            "type": "string"
        },
        "DomainId": {
            "type": "string"
        },
        "ExpirationTime": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "IsEnabled": {
            "type": "boolean",
            "description": "Specifies whether the key is enabled. Defaults to true.\nChanging this updates the state of existing key."
        },
        "KeyAlias": {
            "type": "string",
            "description": "The alias in which to create the key. It is required when\nwe create a new key. Changing this updates the alias of key."
        },
        "KeyDescription": {
            "type": "string",
            "description": "The description of the key as viewed in OpenTelekomCloud console.\nChanging this updates the description of key."
        },
        "Origin": {
            "type": "string"
        },
        "PendingDays": {
            "type": "string",
            "description": "Duration in days after which the key is deleted\nafter destruction of the resource, must be between 7 and 1096 days. Defaults to 7.\nIt only be used when delete a key."
        },
        "Realm": {
            "type": "string",
            "description": "Region where a key resides. Changing this creates a new key."
        },
        "ScheduledDeletionDate": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "KeyAlias"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/CreationDate",
        "/properties/DefaultKeyFlag",
        "/properties/DomainId",
        "/properties/ExpirationTime",
        "/properties/Id",
        "/properties/Origin",
        "/properties/ScheduledDeletionDate"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}