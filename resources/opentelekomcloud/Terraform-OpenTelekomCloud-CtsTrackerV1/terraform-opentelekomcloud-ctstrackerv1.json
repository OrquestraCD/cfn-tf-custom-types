{
    "typeName": "Terraform::OpenTelekomCloud::CtsTrackerV1",
    "description": "Allows you to collect, store, and query cloud resource operation records.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "BucketName": {
            "type": "string",
            "description": "The OBS bucket name for a tracker."
        },
        "FilePrefixName": {
            "type": "string",
            "description": "The prefix of a log that needs to be stored in an OBS bucket."
        },
        "IsSendAllKeyOperation": {
            "type": "boolean",
            "description": "When the value is **false**, operations cannot be left empty."
        },
        "IsSupportSmn": {
            "type": "boolean",
            "description": "Specifies whether SMN is supported. When the value is false, topic_id and operations can be left empty."
        },
        "NeedNotifyUserList": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "The users using the login function. When these users log in, notifications will be sent."
        },
        "Operations": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "Trigger conditions for sending a notification."
        },
        "Region": {
            "type": "string"
        },
        "Status": {
            "type": "string",
            "description": "The status of a tracker. The value should be **enabled** when creating a tracker, and when updating the value can be enabled or disabled."
        },
        "TopicId": {
            "type": "string",
            "description": "The theme of the SMN service, Is obtained from SMN and in the format of **urn:smn:([a-z]|[A-Z]|[0-9]|\\-){1,32}:([a-z]|[A-Z]|[0-9]){32}:([a-z]|[A-Z]|[0-9]|\\-|\\_){1,256}**."
        },
        "TrackerName": {
            "type": "string"
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "BucketName",
        "IsSendAllKeyOperation",
        "IsSupportSmn",
        "Operations",
        "TopicId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/TrackerName"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}