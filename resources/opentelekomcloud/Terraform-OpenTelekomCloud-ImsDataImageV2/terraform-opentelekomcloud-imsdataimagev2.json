{
    "typeName": "Terraform::OpenTelekomCloud::ImsDataImageV2",
    "description": "Manages a V2 Data Image resource within OpenTelekomCloud IMS.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/opentelekomcloud/Terraform-OpenTelekomCloud-ImsDataImageV2/docs/README.md",
    "definitions": {
        "Timeouts": {
            "type": "object",
