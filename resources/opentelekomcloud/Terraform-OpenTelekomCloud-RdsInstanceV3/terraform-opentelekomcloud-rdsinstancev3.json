{
    "typeName": "Terraform::OpenTelekomCloud::RdsInstanceV3",
    "description": "instance management",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "BackupStrategy": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "KeepDays": {
                    "type": "number"
                },
                "StartTime": {
                    "type": "string"
                }
            },
            "required": [
                "StartTime"
            ]
        },
        "Db": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Password": {
                    "type": "string"
                },
                "Port": {
                    "type": "number"
                },
                "Type": {
                    "type": "string"
                },
                "Version": {
                    "type": "string"
                }
            },
            "required": [
                "Password",
                "Type",
                "Version"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        },
        "Volume": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "DiskEncryptionId": {
                    "type": "string"
                },
                "Size": {
                    "type": "number"
                },
                "Type": {
                    "type": "string"
                }
            },
            "required": [
                "Size",
                "Type"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AvailabilityZone": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "Created": {
            "type": "string"
        },
        "Flavor": {
            "type": "string"
        },
        "HaReplicationMode": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "Nodes": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "AvailabilityZone": {
                        "type": "string"
                    },
                    "Id": {
                        "type": "string"
                    },
                    "Name": {
                        "type": "string"
                    },
                    "Role": {
                        "type": "string"
                    },
                    "Status": {
                        "type": "string"
                    }
                }
            }
        },
        "ParamGroupId": {
            "type": "string"
        },
        "PrivateIps": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "PublicIps": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "SecurityGroupId": {
            "type": "string"
        },
        "SubnetId": {
            "type": "string"
        },
        "Tag": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "Tags key/value pairs to associate with the instance."
        },
        "VpcId": {
            "type": "string"
        },
        "BackupStrategy": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/BackupStrategy"
            },
            "maxItems": 1
        },
        "Db": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Db"
            },
            "maxItems": 1,
            "minItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        },
        "Volume": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Volume"
            },
            "maxItems": 1,
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "AvailabilityZone",
        "Flavor",
        "Name",
        "SecurityGroupId",
        "SubnetId",
        "VpcId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Created",
        "/properties/Id",
        "/properties/Nodes",
        "/properties/PrivateIps",
        "/properties/PublicIps"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}