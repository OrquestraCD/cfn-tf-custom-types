{
    "typeName": "Terraform::OpenTelekomCloud::NetworkingPortV2",
    "description": "Manages a V2 port resource within OpenTelekomCloud.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "AllowedAddressPairs": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "IpAddress": {
                    "type": "string",
                    "description": "The additional IP address."
                },
                "MacAddress": {
                    "type": "string",
                    "description": "The additional MAC address."
                }
            },
            "required": [
                "IpAddress"
            ]
        },
        "FixedIp": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "IpAddress": {
                    "type": "string",
                    "description": "IP address desired in the subnet for this port. If\nyou don't specify `ip_address`, an available IP address from the specified\nsubnet will be allocated to this port."
                },
                "SubnetId": {
                    "type": "string",
                    "description": "Subnet in which to allocate IP address for\nthis port."
                }
            },
            "required": [
                "SubnetId"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AdminStateUp": {
            "type": "boolean",
            "description": "Administrative up/down status for the port\n(must be \"true\" or \"false\" if provided). Changing this updates the\n`admin_state_up` of an existing port."
        },
        "AllFixedIps": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "DeviceId": {
            "type": "string",
            "description": "The ID of the device attached to the port. Changing this\ncreates a new port."
        },
        "DeviceOwner": {
            "type": "string",
            "description": "The device owner of the Port. Changing this creates\na new port."
        },
        "Id": {
            "type": "string"
        },
        "MacAddress": {
            "type": "string",
            "description": "Specify a specific MAC address for the port. Changing\nthis creates a new port."
        },
        "Name": {
            "type": "string",
            "description": "A unique name for the port. Changing this\nupdates the `name` of an existing port."
        },
        "NetworkId": {
            "type": "string",
            "description": "The ID of the network to attach the port to. Changing\nthis creates a new port."
        },
        "NoSecurityGroups": {
            "type": "boolean"
        },
        "Region": {
            "type": "string"
        },
        "SecurityGroupIds": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "A list of security group IDs to apply to the\nport. The security groups must be specified by ID and not name (as opposed\nto how they are configured with the Compute Instance)."
        },
        "TenantId": {
            "type": "string",
            "description": "The owner of the Port. Required if admin wants\nto create a port for another tenant. Changing this creates a new port."
        },
        "ValueSpecs": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "Map of additional options."
        },
        "AllowedAddressPairs": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/AllowedAddressPairs"
            }
        },
        "FixedIp": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/FixedIp"
            }
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "NetworkId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/AllFixedIps",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}