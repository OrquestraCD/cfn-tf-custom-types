{
    "typeName": "Terraform::OpenTelekomCloud::NetworkingVipAssociateV2",
    "description": "Manages a V2 vip associate resource within OpenTelekomCloud.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "PortIds": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "An array of one or more IDs of the ports to attach the vip to.\nChanging this creates a new vip associate."
        },
        "VipId": {
            "type": "string",
            "description": "The ID of vip to attach the port to.\nChanging this creates a new vip associate."
        },
        "VipIpAddress": {
            "type": "string"
        },
        "VipSubnetId": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "PortIds",
        "VipId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/VipIpAddress",
        "/properties/VipSubnetId"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}