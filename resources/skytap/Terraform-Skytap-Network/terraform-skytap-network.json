{
    "typeName": "Terraform::Skytap::Network",
    "description": "Provides a Skytap Network resource. Networks are not top-level elements of the Skytap API.\nRather, they are elements properly contained within an environment.\nOperations on them are implicitly on the containing environment.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/skytap/Terraform-Skytap-Network/docs/README.md",
    "definitions": {
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Domain": {
            "type": "string",
            "description": "Domain name for the Skytap network. Limited to 64 characters."
        },
        "EnvironmentId": {
            "type": "string",
            "description": "ID of the environment you want to attach the network to. If updating with a new one then the network will be recreated."
        },
        "Gateway": {
            "type": "string",
            "description": "Gateway IP address."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "User-defined name of the network. Limited to 255 characters. UTF-8 character type."
        },
        "Subnet": {
            "type": "string",
            "description": "Defines the subnet address and subnet mask size in CIDR format (for example, 10.0.0.0/24). IP addresses for the VMs are assigned from this subnet and standard network services (DNS resolution, CIFS share, routes to Internet) are defined appropriately for it."
        },
        "Tunnelable": {
            "type": "boolean",
            "description": "If true, this network can be connected to other networks."
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
