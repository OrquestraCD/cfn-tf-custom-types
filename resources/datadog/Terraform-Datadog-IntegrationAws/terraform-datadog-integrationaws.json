{
    "typeName": "Terraform::Datadog::IntegrationAws",
    "description": "CloudFormation equivalent of datadog_integration_aws",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AccountId": {
            "type": "string"
        },
        "AccountSpecificNamespaceRules": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "boolean"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "ExternalId": {
            "type": "string"
        },
        "FilterTags": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "HostTags": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "RoleName": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "AccountId",
        "RoleName"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/ExternalId"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}