{
    "typeName": "Terraform::Datadog::IntegrationPagerduty",
    "description": "Provides a Datadog - PagerDuty resource. This can be used to create and manage Datadog - PagerDuty integration.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Services": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "ServiceKey": {
                    "type": "string"
                },
                "ServiceName": {
                    "type": "string"
                }
            },
            "required": [
                "ServiceKey",
                "ServiceName"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ApiToken": {
            "type": "string",
            "description": "Your PagerDuty API token."
        },
        "IndividualServices": {
            "type": "boolean",
            "description": "Boolean to specify whether or not individual service objects specified by [datadog_integration_pagerduty_service_object](/docs/providers/datadog/r/integration_pagerduty_service_object.html) resource are to be used. Mutually exclusive with `services` key."
        },
        "Schedules": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "Array of your schedule URLs."
        },
        "Subdomain": {
            "type": "string",
            "description": "Your PagerDuty account\u2019s personalized subdomain name."
        },
        "Services": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Services"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Subdomain"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}