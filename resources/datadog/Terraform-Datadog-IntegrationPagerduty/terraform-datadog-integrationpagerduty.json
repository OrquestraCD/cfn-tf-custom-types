{
    "typeName": "Terraform::Datadog::IntegrationPagerduty",
    "description": "CloudFormation equivalent of datadog_integration_pagerduty",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Services": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "ServiceKey": {
                    "type": "string"
                },
                "ServiceName": {
                    "type": "string"
                }
            },
            "required": [
                "ServiceKey",
                "ServiceName"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ApiToken": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "IndividualServices": {
            "type": "boolean"
        },
        "Schedules": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "Subdomain": {
            "type": "string"
        },
        "Services": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Services"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Subdomain"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}