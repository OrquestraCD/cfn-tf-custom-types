{
    "typeName": "Terraform::Datadog::ServiceLevelObjective",
    "description": "Provides a Datadog service level objective resource. This can be used to create and manage Datadog service level objectives.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Query": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Denominator": {
                    "type": "string"
                },
                "Numerator": {
                    "type": "string"
                }
            },
            "required": [
                "Denominator",
                "Numerator"
            ]
        },
        "Thresholds": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Target": {
                    "type": "number"
                },
                "TargetDisplay": {
                    "type": "string"
                },
                "Timeframe": {
                    "type": "string"
                },
                "Warning": {
                    "type": "number"
                },
                "WarningDisplay": {
                    "type": "string"
                }
            },
            "required": [
                "Target",
                "Timeframe"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "A description of this service level objective."
        },
        "Groups": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Id": {
            "type": "string"
        },
        "MonitorIds": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "number"
            }
        },
        "MonitorSearch": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "Name of Datadog service level objective."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Type": {
            "type": "string",
            "description": "The type of the service level objective. The mapping from these types to the types found in the Datadog Web UI can be found in the Datadog API [documentation](https://docs.datadoghq.com/api/?lang=python#create-a-service-level-objective) page. Available options to choose from are:\n* `metric`\n* `monitor`."
        },
        "Query": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Query"
            },
            "maxItems": 1
        },
        "Thresholds": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Thresholds"
            },
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "Type"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
