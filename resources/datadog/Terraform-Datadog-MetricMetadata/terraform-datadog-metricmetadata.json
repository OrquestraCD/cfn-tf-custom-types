{
    "typeName": "Terraform::Datadog::MetricMetadata",
    "description": "Provides a Datadog metric_metadata resource. This can be used to manage a metric's metadata.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "A description of the metric."
        },
        "Metric": {
            "type": "string",
            "description": "The name of the metric."
        },
        "PerUnit": {
            "type": "string",
            "description": "'Per' unit of the metric such as 'second' in 'bytes per second'."
        },
        "ShortName": {
            "type": "string",
            "description": "A short name of the metric."
        },
        "StatsdInterval": {
            "type": "number",
            "description": "If applicable, stasd flush interval in seconds for the metric."
        },
        "Type": {
            "type": "string"
        },
        "Unit": {
            "type": "string",
            "description": "Primary unit of the metric such as 'byte' or 'operation'."
        }
    },
    "additionalProperties": false,
    "required": [
        "Metric"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}