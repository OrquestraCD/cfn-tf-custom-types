{
    "typeName": "Terraform::Grafana::Dashboard",
    "description": "The dashboard resource allows a dashboard to be created on a Grafana server.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ConfigJson": {
            "type": "string",
            "description": "The JSON configuration for the dashboard."
        },
        "Folder": {
            "type": "number"
        },
        "Id": {
            "type": "string"
        },
        "Slug": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "ConfigJson"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/Slug"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}