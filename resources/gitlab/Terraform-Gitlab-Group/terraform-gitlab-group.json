{
    "typeName": "Terraform::Gitlab::Group",
    "description": "This resource allows you to create and manage GitLab groups.\nNote your provider will need to be configured with admin-level access for this resource to work.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "The description of the group."
        },
        "FullName": {
            "type": "string"
        },
        "FullPath": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "LfsEnabled": {
            "type": "boolean",
            "description": "Boolean, defaults to true.  Whether to enable LFS\nsupport for projects in this group."
        },
        "Name": {
            "type": "string",
            "description": "The name of this group."
        },
        "ParentId": {
            "type": "number",
            "description": "Integer, id of the parent group (creates a nested group)."
        },
        "Path": {
            "type": "string",
            "description": "The path of the group."
        },
        "RequestAccessEnabled": {
            "type": "boolean",
            "description": "Boolean, defaults to false.  Whether to\nenable users to request access to the group."
        },
        "RunnersToken": {
            "type": "string"
        },
        "VisibilityLevel": {
            "type": "string",
            "description": "Set to `public` to create a public group.\nValid values are `private`, `internal`, `public`.\nGroups are created as private by default."
        },
        "WebUrl": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "Path"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/FullName",
        "/properties/FullPath",
        "/properties/Id",
        "/properties/RunnersToken",
        "/properties/WebUrl"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}