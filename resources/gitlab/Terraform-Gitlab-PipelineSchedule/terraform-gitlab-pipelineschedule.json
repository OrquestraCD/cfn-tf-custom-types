{
    "typeName": "Terraform::Gitlab::PipelineSchedule",
    "description": "This resource allows you to create and manage pipeline schedules.\nFor further information on clusters, consult the [gitlab\ndocumentation](https://docs.gitlab.com/ce/user/project/pipelines/schedules.html).",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Active": {
            "type": "boolean",
            "description": "The activation of pipeline schedule. If false is set, the pipeline schedule will deactivated initially."
        },
        "Cron": {
            "type": "string",
            "description": "The cron (e.g. `0 1 * * *`)."
        },
        "CronTimezone": {
            "type": "string",
            "description": "The timezone."
        },
        "Description": {
            "type": "string",
            "description": "The description of the pipeline schedule."
        },
        "Id": {
            "type": "string"
        },
        "Project": {
            "type": "string",
            "description": "The name or id of the project to add the schedule to."
        },
        "Ref": {
            "type": "string",
            "description": "The branch/tag name to be triggered."
        }
    },
    "additionalProperties": false,
    "required": [
        "Cron",
        "Description",
        "Project",
        "Ref"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}