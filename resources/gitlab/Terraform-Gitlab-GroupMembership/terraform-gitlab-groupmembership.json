{
    "typeName": "Terraform::Gitlab::GroupMembership",
    "description": "This resource allows you to add a user to an existing group.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AccessLevel": {
            "type": "string",
            "description": "Acceptable values are: guest, reporter, developer, maintainer, owner."
        },
        "ExpiresAt": {
            "type": "string",
            "description": "Expiration date for the group membership. Format: `YYYY-MM-DD`."
        },
        "GroupId": {
            "type": "string",
            "description": "The id of the group."
        },
        "Id": {
            "type": "string"
        },
        "UserId": {
            "type": "number",
            "description": "The id of the user."
        }
    },
    "additionalProperties": false,
    "required": [
        "AccessLevel",
        "GroupId",
        "UserId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
