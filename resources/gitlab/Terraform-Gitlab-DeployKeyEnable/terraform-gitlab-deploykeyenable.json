{
    "typeName": "Terraform::Gitlab::DeployKeyEnable",
    "description": "This resource allows you to enable pre-existing deploy keys for your GitLab projects.\n\n**the GITLAB KEY_ID for the deploy key must be known**",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CanPush": {
            "type": "boolean"
        },
        "Key": {
            "type": "string"
        },
        "KeyId": {
            "type": "string",
            "description": "The Gitlab key id for the pre-existing deploy key."
        },
        "Project": {
            "type": "string",
            "description": "The name or id of the project to add the deploy key to."
        },
        "Title": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
