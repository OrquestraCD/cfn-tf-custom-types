{
    "typeName": "Terraform::RabbitMQ::Permissions",
    "description": "The ``rabbitmq_permissions`` resource creates and manages a user's set of\npermissions.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/rabbitmq/Terraform-RabbitMQ-Permissions/docs/README.md",
    "definitions": {
        "Permissions": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Configure": {
                    "type": "string",
                    "description": "The \"configure\" ACL."
                },
                "Read": {
                    "type": "string",
                    "description": "The \"read\" ACL."
                },
                "Write": {
                    "type": "string",
                    "description": "The \"write\" ACL."
                }
            },
            "required": [
                "Configure",
                "Read",
                "Write"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "User": {
            "type": "string",
            "description": "The user to apply the permissions to."
        },
        "Vhost": {
            "type": "string",
            "description": "The vhost to create the resource in."
        },
        "Permissions": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Permissions"
            },
            "maxItems": 1,
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "User"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}