{
    "typeName": "Terraform::RabbitMQ::TopicPermissions",
    "description": "CloudFormation equivalent of rabbitmq_topic_permissions",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Permissions": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Exchange": {
                    "type": "string"
                },
                "Read": {
                    "type": "string"
                },
                "Write": {
                    "type": "string"
                }
            },
            "required": [
                "Exchange",
                "Read",
                "Write"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "User": {
            "type": "string"
        },
        "Vhost": {
            "type": "string"
        },
        "Permissions": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Permissions"
            },
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "User"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}