{
    "typeName": "Terraform::RabbitMQ::Policy",
    "description": "The ``rabbitmq_policy`` resource creates and manages policies for exchanges\nand queues.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Policy": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "ApplyTo": {
                    "type": "string",
                    "description": "Can either be \"exchanges\", \"queues\", or \"all\"."
                },
                "Definition": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "properties": {
                            "Key": {
                                "type": "string"
                            },
                            "Value": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "Key",
                            "Value"
                        ]
                    },
                    "description": "Key/value pairs of the policy definition. See the\nRabbitMQ documentation for definition references and examples."
                },
                "Pattern": {
                    "type": "string",
                    "description": "A pattern to match an exchange or queue name."
                },
                "Priority": {
                    "type": "number",
                    "description": "The policy with the greater priority is applied first."
                }
            },
            "required": [
                "ApplyTo",
                "Definition",
                "Pattern",
                "Priority"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name of the policy."
        },
        "Vhost": {
            "type": "string",
            "description": "The vhost to create the resource in."
        },
        "Policy": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Policy"
            },
            "maxItems": 1,
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "Vhost"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}