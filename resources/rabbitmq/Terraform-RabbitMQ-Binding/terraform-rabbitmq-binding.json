{
    "typeName": "Terraform::RabbitMQ::Binding",
    "description": "The ``rabbitmq_binding`` resource creates and manages a binding relationship\nbetween a queue an exchange.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Arguments": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "Additional key/value arguments for the binding."
        },
        "Destination": {
            "type": "string",
            "description": "The destination queue or exchange."
        },
        "DestinationType": {
            "type": "string",
            "description": "The type of destination (queue or exchange)."
        },
        "Id": {
            "type": "string"
        },
        "PropertiesKey": {
            "type": "string"
        },
        "RoutingKey": {
            "type": "string",
            "description": "A routing key for the binding."
        },
        "Source": {
            "type": "string",
            "description": "The source exchange."
        },
        "Vhost": {
            "type": "string",
            "description": "The vhost to create the resource in."
        }
    },
    "additionalProperties": false,
    "required": [
        "Destination",
        "DestinationType",
        "Source",
        "Vhost"
    ],
    "readOnlyProperties": [
