{
    "typeName": "Terraform::RabbitMQ::Binding",
    "description": "CloudFormation equivalent of rabbitmq_binding",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Arguments": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "Destination": {
            "type": "string"
        },
        "DestinationType": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "PropertiesKey": {
            "type": "string"
        },
        "RoutingKey": {
            "type": "string"
        },
        "Source": {
            "type": "string"
        },
        "Vhost": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "Destination",
        "DestinationType",
        "Source",
        "Vhost"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/PropertiesKey"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}