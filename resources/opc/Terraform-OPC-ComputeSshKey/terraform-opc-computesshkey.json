{
    "typeName": "Terraform::OPC::ComputeSshKey",
    "description": "The ``opc_compute_ssh_key`` resource creates and manages an SSH key in an Oracle Cloud Infrastructure Compute Classic identity domain.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Enabled": {
            "type": "boolean",
            "description": "Whether or not the key is enabled. This is useful if you want to temporarily disable an SSH key,\nwithout removing it entirely from your Terraform resource definition. Defaults to `true`."
        },
        "Id": {
            "type": "string"
        },
        "Key": {
            "type": "string",
            "description": "The SSH key itself."
        },
        "Name": {
            "type": "string",
            "description": "The unique (within this identity domain) name of the SSH key."
        }
    },
    "additionalProperties": false,
    "required": [
        "Key",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}