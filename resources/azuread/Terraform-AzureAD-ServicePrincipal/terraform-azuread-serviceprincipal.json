{
    "typeName": "Terraform::AzureAD::ServicePrincipal",
    "description": "CloudFormation equivalent of azuread_service_principal",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AppRoleAssignmentRequired": {
            "type": "boolean"
        },
        "ApplicationId": {
            "type": "string"
        },
        "DisplayName": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "ObjectId": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "ApplicationId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/DisplayName",
        "/properties/ObjectId"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}