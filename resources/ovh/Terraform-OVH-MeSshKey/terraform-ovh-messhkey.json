{
    "typeName": "Terraform::OVH::MeSshKey",
    "description": "Creates an SSH Key.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Default": {
            "type": "boolean",
            "description": "True when this public SSH key is used for rescue mode and reinstallations."
        },
        "Id": {
            "type": "string"
        },
        "Key": {
            "type": "string",
            "description": "The content of the public key in the form \"ssh-algo content\", e.g. \"ssh-ed25519 AAAAC3...\"."
        },
        "KeyName": {
            "type": "string",
            "description": "The friendly name of this SSH key."
        }
    },
    "additionalProperties": false,
    "required": [
        "Key",
        "KeyName"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}