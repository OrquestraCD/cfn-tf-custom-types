{
    "typeName": "Terraform::OVH::CloudNetworkPrivate",
    "description": "Creates a private network in a public cloud project.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name of the network."
        },
        "ProjectId": {
            "type": "string",
            "description": "The id of the public cloud project. If omitted,\nthe `OVH_PROJECT_ID` environment variable is used."
        },
        "Regions": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "an array of valid OVH public cloud region ID in which the network\nwill be available. Ex.: \"GRA1\". Defaults to all public cloud regions."
        },
        "RegionsStatus": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Region": {
                        "type": "string"
                    },
                    "Status": {
                        "type": "string"
                    }
                }
            }
        },
        "Status": {
            "type": "string"
        },
        "Type": {
            "type": "string"
        },
        "VlanId": {
            "type": "number",
            "description": "a vlan id to associate with the network.\nChanging this value recreates the resource. Defaults to 0."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "ProjectId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/RegionsStatus",
        "/properties/Status",
        "/properties/Type"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}