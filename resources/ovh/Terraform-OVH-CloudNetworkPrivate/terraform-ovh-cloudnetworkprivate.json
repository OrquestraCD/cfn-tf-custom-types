{
    "typeName": "Terraform::OVH::CloudNetworkPrivate",
    "description": "CloudFormation equivalent of ovh_cloud_network_private",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "ProjectId": {
            "type": "string"
        },
        "Regions": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
