{
    "typeName": "Terraform::UCloud::VpnConnection",
    "description": "Provides a IPSec VPN Gateway Connection resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/ucloud/Terraform-UCloud-VpnConnection/docs/README.md",
    "definitions": {
        "IkeConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AuthenticationAlgorithm": {
                    "type": "string"
                },
                "DhGroup": {
                    "type": "string"
                },
                "EncryptionAlgorithm": {
                    "type": "string"
                },
                "ExchangeMode": {
                    "type": "string"
                },
                "IkeVersion": {
                    "type": "string"
                },
                "LocalId": {
                    "type": "string"
                },
                "PreSharedKey": {
                    "type": "string"
                },
                "RemoteId": {
                    "type": "string"
                },
                "SaLifeTime": {
                    "type": "number"
                }
            },
            "required": [
                "PreSharedKey"
            ]
        },
        "IpsecConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AuthenticationAlgorithm": {
                    "type": "string"
                },
                "EncryptionAlgorithm": {
                    "type": "string"
                },
                "LocalSubnetIds": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                },
                "PfsDhGroup": {
                    "type": "string"
                },
                "Protocol": {
                    "type": "string"
                },
                "RemoteSubnets": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                },
                "SaLifeTime": {
                    "type": "number"
                },
                "SaLifeTimeBytes": {
                    "type": "number"
                }
            },
            "required": [
                "LocalSubnetIds",
                "RemoteSubnets"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CreateTime": {
            "type": "string"
        },
        "CustomerGatewayId": {
            "type": "string",
            "description": "The grade of the VPN Gateway."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name of the VPN Gateway Connection which contains 1-63 characters and only support Chinese, English, numbers and special characters: `-_.`. If not specified, terraform will auto-generate a name beginning with `tf-vpn-connection-`."
        },
        "Remark": {
            "type": "string",
            "description": "The remarks of the VPN Gateway Connection. (Default: `\"\"`)."
        },
        "Tag": {
            "type": "string",
            "description": "A tag assigned to VPN Gateway Connection, which contains at most 63 characters and only support Chinese, English, numbers, '-', '_', and '.'. If it is not filled in or a empty string is filled in, then default tag will be assigned. (Default: `Default`)."
        },
        "VpcId": {
            "type": "string",
            "description": "The ID of VPC linked to the VPN Gateway Connection."
        },
        "VpnGatewayId": {
            "type": "string",
            "description": "The ID of  the VPN Customer Gateway."
        },
        "IkeConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/IkeConfig"
            },
            "maxItems": 1,
            "minItems": 1
        },
        "IpsecConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/IpsecConfig"
            },
            "maxItems": 1,
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "CustomerGatewayId",
        "VpcId",
        "VpnGatewayId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/CreateTime",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}