        },
        "BackendIds": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Domain": {
            "type": "string"
        },
        "ListenerId": {
            "type": "string"
        },
        "LoadBalancerId": {
            "type": "string"
        },
        "Path": {
            "type": "string"
        }
    },
    "additionalProperties": false,
