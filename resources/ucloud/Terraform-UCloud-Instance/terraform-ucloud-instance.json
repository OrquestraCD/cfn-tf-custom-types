{
    "typeName": "Terraform::UCloud::Instance",
    "description": "CloudFormation equivalent of ucloud_instance",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AllowStoppingForUpdate": {
            "type": "boolean"
        },
        "AutoRenew": {
            "type": "boolean"
        },
        "AvailabilityZone": {
            "type": "string"
        },
        "BootDiskSize": {
            "type": "number"
        },
        "BootDiskType": {
            "type": "string"
        },
        "ChargeType": {
            "type": "string"
        },
        "Cpu": {
            "type": "number"
        },
        "CreateTime": {
            "type": "string"
        },
        "DataDiskSize": {
            "type": "number"
        },
        "DataDiskType": {
            "type": "string"
        },
        "DiskSet": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Id": {
                        "type": "string"
                    },
                    "IsBoot": {
                        "type": "boolean"
                    },
                    "Size": {
                        "type": "number"
                    },
                    "Type": {
                        "type": "string"
                    }
                }
            }
        },
        "Duration": {
            "type": "number"
        },
        "ExpireTime": {
            "type": "string"
        },
        "ImageId": {
            "type": "string"
        },
        "InstanceType": {
            "type": "string"
        },
        "IpSet": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "InternetType": {
                        "type": "string"
                    },
                    "Ip": {
                        "type": "string"
                    }
                }
            }
        },
        "IsolationGroup": {
            "type": "string"
        },
        "Memory": {
            "type": "number"
        },
        "Name": {
            "type": "string"
        },
        "PrivateIp": {
            "type": "string"
        },
        "Remark": {
            "type": "string"
        },
        "RootPassword": {
            "type": "string"
        },
        "SecurityGroup": {
            "type": "string"
        },
        "Status": {
            "type": "string"
        },
        "SubnetId": {
            "type": "string"
        },
        "Tag": {
            "type": "string"
        },
        "VpcId": {
            "type": "string"
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "AvailabilityZone",
        "ImageId",
        "InstanceType"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/AutoRenew",
        "/properties/Cpu",
        "/properties/CreateTime",
        "/properties/DiskSet",
        "/properties/ExpireTime",
        "/properties/IpSet",
        "/properties/Memory",
        "/properties/Status"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
