{
    "typeName": "Terraform::UCloud::DbInstance",
    "description": "CloudFormation equivalent of ucloud_db_instance",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AvailabilityZone": {
            "type": "string"
        },
        "BackupBeginTime": {
            "type": "number"
        },
        "BackupBlackList": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "BackupCount": {
            "type": "number"
        },
        "BackupDate": {
            "type": "string"
        },
        "ChargeType": {
            "type": "string"
        },
        "CreateTime": {
            "type": "string"
        },
        "Duration": {
            "type": "number"
        },
        "Engine": {
            "type": "string"
        },
        "EngineVersion": {
            "type": "string"
        },
        "ExpireTime": {
            "type": "string"
        },
        "InstanceStorage": {
            "type": "number"
        },
        "InstanceType": {
            "type": "string"
        },
        "ModifyTime": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "Password": {
            "type": "string"
        },
        "Port": {
            "type": "number"
        },
        "PrivateIp": {
            "type": "string"
        },
        "StandbyZone": {
            "type": "string"
        },
        "Status": {
            "type": "string"
        },
        "SubnetId": {
            "type": "string"
        },
        "Tag": {
            "type": "string"
        },
        "VpcId": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "AvailabilityZone",
        "Engine",
        "EngineVersion",
        "InstanceStorage",
        "InstanceType"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/CreateTime",
        "/properties/ExpireTime",
        "/properties/ModifyTime",
        "/properties/PrivateIp",
        "/properties/Status"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}