{
    "typeName": "Terraform::UCloud::Lb",
    "description": "Provides a Load Balancer resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ChargeType": {
            "type": "string",
            "description": "**Deprecated**, argument `charge_type` is deprecated for optimizing parameters."
        },
        "CreateTime": {
            "type": "string"
        },
        "ExpireTime": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Internal": {
            "type": "boolean",
            "description": "Indicate whether the load balancer is intranet mode.(Default: `\"false\"`)."
        },
        "IpSet": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "InternetType": {
                        "type": "string"
                    },
                    "Ip": {
                        "type": "string"
                    }
                }
            }
        },
        "Name": {
            "type": "string",
            "description": "The name of the load balancer. If not specified, terraform will auto-generate a name beginning with `tf-lb`."
        },
        "PrivateIp": {
            "type": "string"
        },
        "Remark": {
            "type": "string",
            "description": "The remarks of the load balancer. (Default: `\"\"`)."
        },
        "SubnetId": {
            "type": "string",
            "description": "The ID of subnet that intranet load balancer belongs to. This argument is not required if default subnet."
        },
        "Tag": {
            "type": "string",
            "description": "A tag assigned to load balancer, which contains at most 63 characters and only support Chinese, English, numbers, '-', '_', and '.'. If it is not filled in or a empty string is filled in, then default tag will be assigned. (Default: `Default`)."
        },
        "VpcId": {
            "type": "string",
            "description": "The ID of the VPC linked to the Load balancer, This argument is not required if default VPC."
        }
    },
    "additionalProperties": false,
    "required": [],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/CreateTime",
        "/properties/ExpireTime",
        "/properties/Id",
        "/properties/IpSet",
        "/properties/PrivateIp"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}