{
    "typeName": "Terraform::ACME::Registration",
    "description": "CloudFormation equivalent of acme_registration",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AccountKeyPem": {
            "type": "string"
        },
        "EmailAddress": {
            "type": "string"
        },
        "RegistrationUrl": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "AccountKeyPem",
        "EmailAddress"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/RegistrationUrl"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}