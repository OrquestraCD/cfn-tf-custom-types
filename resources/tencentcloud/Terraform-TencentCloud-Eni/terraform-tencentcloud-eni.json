            "type": "string"
        },
        "CreateTime": {
            "type": "string"
        },
        "Description": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Ipv4Count": {
            "type": "number"
        },
        "Ipv4Info": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Description": {
                        "type": "string"
                    },
                    "Ip": {
                        "type": "string"
                    },
                    "Primary": {
                        "type": "boolean"
                    }
                }
            }
        },
        "Mac": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "Primary": {
            "type": "boolean"
        },
        "SecurityGroups": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "State": {
            "type": "string"
        },
        "SubnetId": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
