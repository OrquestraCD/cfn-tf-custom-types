                    "Key",
                    "Value"
                ]
            }
        },
        "VpcId": {
            "type": "string"
        },
        "Ipv4s": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Ipv4s"
            },
            "maxItems": 30
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "SubnetId",
        "VpcId"
