{
    "typeName": "Terraform::TencentCloud::Eni",
    "description": "CloudFormation equivalent of tencentcloud_eni",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Ipv4s": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Description": {
                    "type": "string"
