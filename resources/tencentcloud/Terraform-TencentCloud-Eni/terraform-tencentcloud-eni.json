    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/CreateTime",
        "/properties/Ipv4Info",
        "/properties/Mac",
        "/properties/Primary",
        "/properties/State"
    ],
    "primaryIdentifier": [
