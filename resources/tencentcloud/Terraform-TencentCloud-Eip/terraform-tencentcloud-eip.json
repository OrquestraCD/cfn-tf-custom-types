{
    "typeName": "Terraform::TencentCloud::Eip",
    "description": "Provides an EIP resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AnycastZone": {
            "type": "string",
            "description": "The zone of anycast, and available values include `ANYCAST_ZONE_GLOBAL` and `ANYCAST_ZONE_OVERSEAS`."
        },
        "ApplicableForClb": {
            "type": "boolean",
            "description": "It has been deprecated from version 1.27.0. Indicates whether the anycast eip can be associated to a CLB."
        },
        "Id": {
            "type": "string"
        },
        "InternetChargeType": {
            "type": "string",
            "description": "The charge type of eip, and available values include `BANDWIDTH_PACKAGE`, `BANDWIDTH_POSTPAID_BY_HOUR` and `TRAFFIC_POSTPAID_BY_HOUR`."
        },
        "InternetMaxBandwidthOut": {
            "type": "number",
            "description": "The bandwidth limit of eip, unit is Mbps, and the range is 1-1000."
        },
        "InternetServiceProvider": {
            "type": "string",
            "description": "Internet service provider of eip, and available values include `BGP`, `CMCC`, `CTCC` and `CUCC`."
        },
        "Name": {
            "type": "string",
            "description": "The name of eip."
        },
        "PublicIp": {
            "type": "string"
        },
        "Status": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "The tags of eip."
        },
        "Type": {
            "type": "string",
            "description": "The type of eip, and available values include `EIP` and `AnycastEIP`. Default is `EIP`."
        }
    },
    "additionalProperties": false,
    "required": [],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/PublicIp",
        "/properties/Status"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
