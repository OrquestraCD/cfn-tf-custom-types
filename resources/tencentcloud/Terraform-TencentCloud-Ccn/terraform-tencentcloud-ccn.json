{
    "typeName": "Terraform::TencentCloud::Ccn",
    "description": "Provides a resource to create a CCN instance.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CreateTime": {
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "Description of CCN, and maximum length does not exceed 100 bytes."
        },
        "Id": {
            "type": "string"
        },
        "InstanceCount": {
            "type": "number"
        },
        "Name": {
            "type": "string",
            "description": "Name of the CCN to be queried, and maximum length does not exceed 60 bytes."
        },
        "Qos": {
            "type": "string",
            "description": "Service quality of CCN, and the available value include 'PT', 'AU', 'AG'. The default is 'AU'."
        },
        "State": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/CreateTime",
        "/properties/Id",
        "/properties/InstanceCount",
        "/properties/State"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}