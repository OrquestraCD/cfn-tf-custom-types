{
    "typeName": "Terraform::TencentCloud::TcaplusApplication",
    "description": "Use this resource to create tcaplus application\n\n~> **NOTE:** tcaplus now only supports the following regions:ap-shanghai,ap-hongkong,na-siliconvalley,ap-singapore,ap-seoul,ap-tokyo,eu-frankfurt",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ApiAccessId": {
            "type": "string"
        },
        "ApiAccessIp": {
            "type": "string"
        },
        "ApiAccessPort": {
            "type": "number"
        },
        "AppName": {
            "type": "string",
            "description": "Name of the tcapplus application. length should between 1 and 30."
        },
        "CreateTime": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "IdlType": {
            "type": "string",
            "description": "Idl type of the tcapplus application.Valid values are PROTO,TDR,MIX."
        },
        "NetworkType": {
            "type": "string"
        },
        "OldPasswordExpireLast": {
            "type": "number",
            "description": "Old password expected expiration seconds after change password,must >= 300."
        },
        "OldPasswordExpireTime": {
            "type": "string"
        },
        "Password": {
            "type": "string",
            "description": "Password of the tcapplus application. length should between 12 and 16,a-z and 0-9 and A-Z must contain."
        },
        "PasswordStatus": {
            "type": "string"
        },
        "SubnetId": {
            "type": "string",
            "description": "Subnet id of the tcapplus application."
        },
        "VpcId": {
            "type": "string",
            "description": "VPC id of the tcapplus application."
        }
    },
    "additionalProperties": false,
    "required": [
        "AppName",
        "IdlType",
        "Password",
        "SubnetId",
        "VpcId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/ApiAccessId",
        "/properties/ApiAccessIp",
        "/properties/ApiAccessPort",
        "/properties/CreateTime",
        "/properties/Id",
        "/properties/NetworkType",
        "/properties/OldPasswordExpireTime",
        "/properties/PasswordStatus"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}