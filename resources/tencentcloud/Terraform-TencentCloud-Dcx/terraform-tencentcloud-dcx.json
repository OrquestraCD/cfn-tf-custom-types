{
    "typeName": "Terraform::TencentCloud::Dcx",
    "description": "CloudFormation equivalent of tencentcloud_dcx",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Bandwidth": {
            "type": "number"
        },
        "BgpAsn": {
            "type": "number"
        },
        "BgpAuthKey": {
            "type": "string"
        },
        "CreateTime": {
            "type": "string"
        },
        "CustomerAddress": {
            "type": "string"
        },
        "DcId": {
            "type": "string"
        },
        "DcgId": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "NetworkType": {
            "type": "string"
        },
        "RouteFilterPrefixes": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "RouteType": {
            "type": "string"
        },
        "State": {
            "type": "string"
        },
        "TencentAddress": {
            "type": "string"
        },
        "Vlan": {
            "type": "number"
        },
        "VpcId": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "DcId",
        "DcgId",
        "Name",
        "VpcId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/CreateTime",
        "/properties/State"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}