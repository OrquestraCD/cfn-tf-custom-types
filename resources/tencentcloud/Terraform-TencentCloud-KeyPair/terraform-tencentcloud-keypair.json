{
    "typeName": "Terraform::TencentCloud::KeyPair",
    "description": "Provides a key pair resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "KeyName": {
            "type": "string",
            "description": "The key pair's name. It is the only in one TencentCloud account."
        },
        "ProjectId": {
            "type": "number",
            "description": "Specifys to which project the key pair belongs."
        },
        "PublicKey": {
            "type": "string",
            "description": "You can import an existing public key and using TencentCloud key pair to manage it."
        }
    },
    "additionalProperties": false,
    "required": [
        "KeyName",
        "PublicKey"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}