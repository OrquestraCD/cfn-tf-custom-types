{
    "typeName": "Terraform::TencentCloud::ClbAttachment",
    "description": "Provides a resource to create a CLB attachment.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Targets": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "InstanceId": {
                    "type": "string",
                    "description": "Id of the backend server."
                },
                "Port": {
                    "type": "number",
                    "description": "Port of the backend server."
                },
                "Weight": {
                    "type": "number",
                    "description": "Forwarding weight of the backend service, the range of [0, 100], defaults to 10."
                }
            },
            "required": [
                "InstanceId",
                "Port"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ClbId": {
            "type": "string",
            "description": "Id of the CLB."
        },
        "Id": {
            "type": "string"
        },
        "ListenerId": {
            "type": "string",
            "description": "Id of the CLB listener."
        },
        "ProtocolType": {
            "type": "string"
        },
        "RuleId": {
            "type": "string",
            "description": "Id of the CLB listener rule. Only supports listeners of 'HTTPS' and 'HTTP' protocol."
        },
        "Targets": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Targets"
            },
            "maxItems": 100,
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "ClbId",
        "ListenerId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/ProtocolType"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}