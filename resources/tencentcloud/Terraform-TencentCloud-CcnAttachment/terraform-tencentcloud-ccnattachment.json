{
    "typeName": "Terraform::TencentCloud::CcnAttachment",
    "description": "Provides a CCN attaching resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AttachedTime": {
            "type": "string"
        },
        "CcnId": {
            "type": "string",
            "description": "ID of the CCN."
        },
        "CidrBlock": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "Id": {
            "type": "string"
        },
        "InstanceId": {
            "type": "string",
            "description": "ID of instance is attached."
        },
        "InstanceRegion": {
            "type": "string",
            "description": "The region that the instance locates at."
        },
        "InstanceType": {
            "type": "string",
            "description": "Type of attached instance network, and available values include VPC, DIRECTCONNECT and BMVPC."
        },
        "State": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "CcnId",
        "InstanceId",
        "InstanceRegion",
        "InstanceType"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/AttachedTime",
        "/properties/CidrBlock",
        "/properties/Id",
        "/properties/State"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}