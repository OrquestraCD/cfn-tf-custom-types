{
    "typeName": "Terraform::TencentCloud::VpnConnection",
    "description": "CloudFormation equivalent of tencentcloud_vpn_connection",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "SecurityGroupPolicy": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "LocalCidrBlock": {
                    "type": "string"
                },
                "RemoteCidrBlock": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                }
            },
            "required": [
                "LocalCidrBlock",
                "RemoteCidrBlock"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CreateTime": {
            "type": "string"
        },
        "CustomerGatewayId": {
            "type": "string"
        },
        "EncryptProto": {
            "type": "string"
        },
        "IkeDhGroupName": {
            "type": "string"
        },
        "IkeExchangeMode": {
            "type": "string"
        },
        "IkeLocalAddress": {
            "type": "string"
        },
        "IkeLocalFqdnName": {
            "type": "string"
        },
        "IkeLocalIdentity": {
            "type": "string"
        },
        "IkeProtoAuthenAlgorithm": {
            "type": "string"
        },
        "IkeProtoEncryAlgorithm": {
            "type": "string"
        },
        "IkeRemoteAddress": {
            "type": "string"
        },
        "IkeRemoteFqdnName": {
            "type": "string"
        },
        "IkeRemoteIdentity": {
            "type": "string"
        },
        "IkeSaLifetimeSeconds": {
            "type": "number"
        },
        "IkeVersion": {
            "type": "string"
        },
        "IpsecEncryptAlgorithm": {
            "type": "string"
        },
        "IpsecIntegrityAlgorithm": {
            "type": "string"
        },
        "IpsecPfsDhGroup": {
            "type": "string"
        },
        "IpsecSaLifetimeSeconds": {
            "type": "number"
        },
        "IpsecSaLifetimeTraffic": {
            "type": "number"
        },
        "Name": {
            "type": "string"
        },
        "NetStatus": {
            "type": "string"
        },
        "PreShareKey": {
            "type": "string"
        },
        "RouteType": {
            "type": "string"
        },
        "State": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "VpcId": {
            "type": "string"
        },
        "VpnGatewayId": {
            "type": "string"
        },
        "VpnProto": {
            "type": "string"
        },
        "SecurityGroupPolicy": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/SecurityGroupPolicy"
            },
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "CustomerGatewayId",
        "Name",
        "PreShareKey",
        "VpcId",
        "VpnGatewayId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/CreateTime",
        "/properties/EncryptProto",
        "/properties/NetStatus",
        "/properties/RouteType",
        "/properties/State",
        "/properties/VpnProto"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
