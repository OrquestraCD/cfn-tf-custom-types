{
    "typeName": "Terraform::TencentCloud::CamUser",
    "description": "Provides a resource to manage CAM user.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/tencentcloud/Terraform-TencentCloud-CamUser/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ConsoleLogin": {
            "type": "boolean",
            "description": "Indicate whether the CAM user can login to the web console or not."
        },
        "CountryCode": {
            "type": "string",
            "description": "Country code of the phone number, for example: '86'."
        },
        "Email": {
            "type": "string",
            "description": "Email of the CAM user."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "Name of the CAM user."
        },
        "NeedResetPassword": {
            "type": "boolean",
            "description": "Indicate whether the CAM user need to reset the password when first logins."
        },
        "Password": {
            "type": "string",
            "description": "The password of the CAM user. Password should be at least 8 characters and no more than 32 characters, includes uppercase letters, lowercase letters, numbers and special characters. Only required when `console_login` is true. If not set, a random password will be automatically generated."
        },
        "PhoneNum": {
            "type": "string",
            "description": "Phone number of the CAM user."
        },
        "Remark": {
            "type": "string",
            "description": "Remark of the CAM user."
        },
        "SecretId": {
            "type": "string"
        },
        "SecretKey": {
            "type": "string"
        },
        "Uid": {
            "type": "number"
        },
        "Uin": {
            "type": "number"
        },
        "UseApi": {
            "type": "boolean",
            "description": "Indicate whether to generate the API secret key or not."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/SecretId",
        "/properties/SecretKey",
        "/properties/Uid",
        "/properties/Uin"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}