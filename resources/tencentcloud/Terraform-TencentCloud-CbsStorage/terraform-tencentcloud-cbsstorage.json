{
    "typeName": "Terraform::TencentCloud::CbsStorage",
    "description": "CloudFormation equivalent of tencentcloud_cbs_storage",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Attached": {
            "type": "boolean"
        },
        "AvailabilityZone": {
            "type": "string"
        },
        "Encrypt": {
            "type": "boolean"
        },
        "Id": {
            "type": "string"
        },
        "Period": {
            "type": "number"
        },
        "ProjectId": {
            "type": "number"
        },
        "SnapshotId": {
            "type": "string"
        },
        "StorageName": {
            "type": "string"
        },
        "StorageSize": {
            "type": "number"
        },
        "StorageStatus": {
            "type": "string"
        },
        "StorageType": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "AvailabilityZone",
        "StorageName",
        "StorageSize",
        "StorageType"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Attached",
        "/properties/StorageStatus"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}