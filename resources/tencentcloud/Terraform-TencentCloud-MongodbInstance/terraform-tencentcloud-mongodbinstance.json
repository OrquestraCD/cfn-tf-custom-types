{
    "typeName": "Terraform::TencentCloud::MongodbInstance",
    "description": "Provide a resource to create a Mongodb instance.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AvailableZone": {
            "type": "string",
            "description": "The available zone of the Mongodb."
        },
        "CreateTime": {
            "type": "string"
        },
        "EngineVersion": {
            "type": "string",
            "description": "Version of the Mongodb, and available values include MONGO_3_WT, MONGO_3_ROCKS and MONGO_36_WT."
        },
        "Id": {
            "type": "string"
        },
        "InstanceName": {
            "type": "string",
            "description": "Name of the Mongodb instance."
        },
        "MachineType": {
            "type": "string",
            "description": "Type of Mongodb instance, and available values include GIO and TGIO."
        },
        "Memory": {
            "type": "number",
            "description": "Memory size. The minimum value is 2, and unit is GB."
        },
        "Password": {
            "type": "string",
            "description": "Password of this Mongodb account."
        },
        "ProjectId": {
            "type": "number",
            "description": "ID of the project which the instance belongs."
        },
        "SecurityGroups": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "ID of the security group."
        },
        "Status": {
            "type": "number"
        },
        "SubnetId": {
            "type": "string",
            "description": "ID of the subnet within this VPC. The vaule is required if VpcId is set."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "The tags of the Mongodb."
        },
        "Vip": {
            "type": "string"
        },
        "Volume": {
            "type": "number",
            "description": "Disk size. The minimum value is 25, and unit is GB."
        },
        "VpcId": {
            "type": "string",
            "description": "ID of the VPC."
        },
        "Vport": {
            "type": "number"
        }
    },
    "additionalProperties": false,
    "required": [
        "AvailableZone",
        "EngineVersion",
        "InstanceName",
        "MachineType",
        "Memory",
        "Password",
        "Volume"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/CreateTime",
        "/properties/Id",
        "/properties/Status",
        "/properties/Vip",
        "/properties/Vport"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}