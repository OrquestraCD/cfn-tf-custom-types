{
    "typeName": "Terraform::Panos::Vlan",
    "description": "This resource allows you to add/update/delete VLANs.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Interfaces": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "List of layer2 interfaces.  You can also leave\nthis blank and also use [panos_vlan_entry](./vlan_entry.html) for more control."
        },
        "Name": {
            "type": "string",
            "description": "The object's name."
        },
        "VlanInterface": {
            "type": "string",
            "description": "The VLAN interface."
        },
        "Vsys": {
            "type": "string",
            "description": "The vsys to put the object into (default: `vsys1`)."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}