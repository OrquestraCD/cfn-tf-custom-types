{
    "typeName": "Terraform::Panos::Licensing",
    "description": "CloudFormation equivalent of panos_licensing",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AuthCodes": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "Delicense": {
            "type": "boolean"
        },
        "Id": {
            "type": "string"
        },
        "Licenses": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "AuthCode": {
                        "type": "string"
                    },
                    "Description": {
                        "type": "string"
                    },
                    "Expired": {
                        "type": "string"
                    },
                    "Expires": {
                        "type": "string"
                    },
                    "Feature": {
                        "type": "string"
                    },
                    "Issued": {
                        "type": "string"
                    },
                    "Serial": {
                        "type": "string"
                    }
                }
            }
        },
        "Mode": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "AuthCodes"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Licenses"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}