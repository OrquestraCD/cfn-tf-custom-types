{
    "typeName": "Terraform::Panos::ManagementProfile",
    "description": "This resource allows you to add/update/delete interface management profiles.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Http": {
            "type": "boolean",
            "description": "Allow HTTP."
        },
        "HttpOcsp": {
            "type": "boolean",
            "description": "Allow HTTP OCSP."
        },
        "Https": {
            "type": "boolean",
            "description": "Allow HTTPS."
        },
        "Name": {
            "type": "string",
            "description": "The management profile's name."
        },
        "PermittedIps": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "The list of permitted IP addresses or address\nranges for this management profile."
        },
        "Ping": {
            "type": "boolean",
            "description": "Allow ping."
        },
        "ResponsePages": {
            "type": "boolean",
            "description": "Allow response pages."
        },
        "Snmp": {
            "type": "boolean",
            "description": "Allow SNMP."
        },
        "Ssh": {
            "type": "boolean",
            "description": "Allow SSH."
        },
        "Telnet": {
            "type": "boolean",
            "description": "Allow telnet."
        },
        "UseridService": {
            "type": "boolean",
            "description": "Allow User ID service."
        },
        "UseridSyslogListenerSsl": {
            "type": "boolean",
            "description": "Allow User ID syslog listener\nfor SSL."
        },
        "UseridSyslogListenerUdp": {
            "type": "boolean",
            "description": "Allow User ID syslog listener\nfor UDP."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}