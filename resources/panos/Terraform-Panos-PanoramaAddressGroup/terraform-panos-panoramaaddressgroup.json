{
    "typeName": "Terraform::Panos::PanoramaAddressGroup",
    "description": "This resource allows you to add/update/delete Panorama address groups.\n\nAddress groups are either statically defined or dynamically defined, so only\n`static_addresses` or `dynamic_match` should be defined within a given address\ngroup.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/panos/Terraform-Panos-PanoramaAddressGroup/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "The address group's description."
        },
        "DeviceGroup": {
            "type": "string",
            "description": "The device group to put the address group into\n(default: `shared`)."
        },
        "DynamicMatch": {
            "type": "string",
            "description": "The IP tags to include in this DAG."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The address group's name."
        },
        "StaticAddresses": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "The address objects to include in this\nstatically defined address group."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "List of administrative tags."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}