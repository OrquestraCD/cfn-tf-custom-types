{
    "typeName": "Terraform::Panos::PanoramaServiceObject",
    "description": "This resource allows you to add/update/delete Panorama service objects.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "The service object's description."
        },
        "DestinationPort": {
            "type": "string",
            "description": "The destination port.  This can be a single\nport number, range (1-65535), or comma separated (80,8080,443)."
        },
        "DeviceGroup": {
            "type": "string",
            "description": "The device group to put the service object into\n(default: `shared`)."
        },
        "Name": {
            "type": "string",
            "description": "The service object's name."
        },
        "OverrideHalfClosedTimeout": {
            "type": "number",
            "description": "The overridden\nTCP half closed timeout."
        },
        "OverrideSessionTimeout": {
            "type": "boolean",
            "description": "Set to `true` to\noverride the default application timeouts."
        },
        "OverrideTimeWaitTimeout": {
            "type": "number",
            "description": "The overridden\nTCP wait time."
        },
        "OverrideTimeout": {
            "type": "number",
            "description": "The overridden TCP timeout."
        },
        "Protocol": {
            "type": "string",
            "description": "The service's protocol.  This should be `tcp`,\n`udp`, or `sctp` (PAN-OS 8.1+)."
        },
        "SourcePort": {
            "type": "string",
            "description": "The source port.  This can be a single port\nnumber, range (1-65535), or comma separated (80,8080,443)."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "List of administrative tags."
        }
    },
    "additionalProperties": false,
    "required": [
        "DestinationPort",
        "Name",
        "Protocol"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}