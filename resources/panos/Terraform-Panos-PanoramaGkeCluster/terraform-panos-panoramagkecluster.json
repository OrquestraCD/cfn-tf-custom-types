{
    "typeName": "Terraform::Panos::PanoramaGkeCluster",
    "description": "This resource allows you to add/update/delete a GKE cluster in a cluster group.\n\nThis resource requires that the GCP plugin be installed.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ClusterCredential": {
            "type": "string",
            "description": "The GKE account to use."
        },
        "GcpZone": {
            "type": "string",
            "description": "The GCP zone."
        },
        "GkeClusterGroup": {
            "type": "string",
            "description": "The cluster group name."
        },
        "Name": {
            "type": "string",
            "description": "The cluster group's name."
        }
    },
    "additionalProperties": false,
    "required": [
        "ClusterCredential",
        "GcpZone",
        "GkeClusterGroup",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}