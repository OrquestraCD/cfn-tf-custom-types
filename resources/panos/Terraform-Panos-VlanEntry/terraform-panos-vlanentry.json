{
    "typeName": "Terraform::Panos::VlanEntry",
    "description": "This resource allows you to add/update/delete an interface in a VLAN.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Interface": {
            "type": "string",
            "description": "The interface's name."
        },
        "LiveMacAddresses": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "MacAddresses": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "List of MAC addresses that should go with this entry."
        },
        "Vlan": {
            "type": "string",
            "description": "The VLAN's name."
        }
    },
    "additionalProperties": false,
    "required": [
        "Interface",
        "Vlan"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/LiveMacAddresses"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}