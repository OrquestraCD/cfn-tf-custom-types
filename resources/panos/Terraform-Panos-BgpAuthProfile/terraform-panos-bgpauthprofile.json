{
    "typeName": "Terraform::Panos::BgpAuthProfile",
    "description": "This resource allows you to add/update/delete a BGP auth profile.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name."
        },
        "Secret": {
            "type": "string",
            "description": "Shared secret for the TCP MD5 authentication."
        },
        "SecretEnc": {
            "type": "string"
        },
        "VirtualRouter": {
            "type": "string",
            "description": "The virtual router to add this BGP\nauth profile to."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "VirtualRouter"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/SecretEnc"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}