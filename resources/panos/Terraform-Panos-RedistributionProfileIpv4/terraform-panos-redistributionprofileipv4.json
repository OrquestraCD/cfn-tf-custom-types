{
    "typeName": "Terraform::Panos::RedistributionProfileIpv4",
    "description": "This resource allows you to add/update/delete IPv4 redistribution profiles\non a virtual router.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Action": {
            "type": "string",
            "description": "The action.  Valid values are `redist` (default) or\n`no-redist`."
        },
        "BgpCommunities": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "BGP communities."
        },
        "BgpExtendedCommunities": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "BGP extended communities."
        },
        "Destinations": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "Specify candidate routes' next-hop addresses\n(subnet match)."
        },
        "Interfaces": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "Specify candidate routes."
        },
        "Name": {
            "type": "string",
            "description": "The redistribution profile's name."
        },
        "NextHops": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "Specify candidate routes' next-hop addresses\n(subnet match)."
        },
        "OspfAreas": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "OSPF areas."
        },
        "OspfPathTypes": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "OSPF path types.  Valid values are\n`intra-area`, `inter-area`, `ext-1`, and `ext-2`."
        },
        "OspfTags": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "OSPF tags."
        },
        "Priority": {
            "type": "number",
            "description": "The priority, integer from 1 to 255."
        },
        "Types": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "The source types.  Valid values are `bgp`, `connect`,\n`ospf`, `rip`, and `static`."
        },
        "VirtualRouter": {
            "type": "string",
            "description": "The virtual router to add the\nredistribution profile to."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "Priority",
        "VirtualRouter"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}