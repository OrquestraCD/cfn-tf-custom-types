{
    "typeName": "Terraform::Panos::MonitorProfile",
    "description": "This resource allows you to add/update/delete monitor profiles.\n\n**Minimum PAN-OS version**: 7.1",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Action": {
            "type": "string",
            "description": "Action triggered when tunnel's status changes.  Valid values\nare `wait-recover` (default) or `fail-over`."
        },
        "Id": {
            "type": "string"
        },
        "Interval": {
            "type": "number",
            "description": "The probing interval in seconds."
        },
        "Name": {
            "type": "string",
            "description": "The monitor profile name."
        },
        "Threshold": {
            "type": "number",
            "description": "The number of failed probes to determine that\nthe tunnel is down."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}