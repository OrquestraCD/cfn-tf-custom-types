            "type": "string"
        },
        "OnlyPassing": {
            "type": "boolean"
        },
        "Service": {
            "type": "string"
        },
        "Session": {
            "type": "string"
        },
        "StoredToken": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
