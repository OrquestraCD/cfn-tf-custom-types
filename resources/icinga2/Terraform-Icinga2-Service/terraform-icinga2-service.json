{
    "typeName": "Terraform::Icinga2::Service",
    "description": "Configures an Icinga2 service resource. This allows service to be configured, updated,\nand deleted.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CheckCommand": {
            "type": "string",
            "description": "The name of an existing Icinga2 CheckCommand object that is used to determine if the service is available on the host."
        },
        "Hostname": {
            "type": "string",
            "description": "The host to check the service's status on."
        },
        "Name": {
            "type": "string",
            "description": "The name of the Service object."
        },
        "Vars": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            },
            "description": "A mapping of variables to assign to the service."
        }
    },
    "additionalProperties": false,
    "required": [
        "CheckCommand",
        "Hostname",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}