{
    "typeName": "Terraform::Netlify::Site",
    "description": "Primary settings for a Netlify site - should contain the bulk of your configuration. Allows configuration of most aspects of your Netlify site.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Repo": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Command": {
                    "type": "string"
                },
                "DeployKeyId": {
                    "type": "string"
                },
                "Dir": {
                    "type": "string"
                },
                "Provider": {
                    "type": "string"
                },
                "RepoBranch": {
                    "type": "string"
                },
                "RepoPath": {
                    "type": "string"
                }
            },
            "required": [
                "Provider",
                "RepoBranch",
                "RepoPath"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AccountName": {
            "type": "string"
        },
        "AccountSlug": {
            "type": "string"
        },
        "CustomDomain": {
            "type": "string",
            "description": "- Custom domain of the site, must be configured using a CNAME in accordance with [Netlify's docs](https://www.netlify.com/docs/custom-domains). (e.g. `www.example.com`)."
        },
        "DeployUrl": {
            "type": "string",
            "description": null
        },
        "Name": {
            "type": "string",
            "description": "- Name of your site on Netlify (e.g. **mysite**.netlify.com)."
        },
        "Repo": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Repo"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/AccountName",
        "/properties/DeployUrl"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}