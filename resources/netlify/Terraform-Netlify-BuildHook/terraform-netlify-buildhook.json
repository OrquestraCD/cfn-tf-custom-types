{
    "typeName": "Terraform::Netlify::BuildHook",
    "description": "Manages build hooks, also known as [incoming webhooks]\n(https://www.netlify.com/docs/webhooks/#outgoing-webhooks). These can,\nat the time of writing, only be used to trigger new builds of the site.\nTo create one, provide your site id along with the name of the hook, and\nthe branch to be built when the hook is triggered.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Branch": {
            "type": "string",
            "description": "branch to be built when the hook is triggered."
        },
        "Id": {
            "type": "string"
        },
        "SiteId": {
            "type": "string",
            "description": "Your netlify site's unique id."
        },
        "Title": {
            "type": "string",
            "description": "name of the webhook - this is purely for organization and\ncan be any name you want."
        },
        "Url": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "Branch",
        "SiteId",
        "Title"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/Url"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}