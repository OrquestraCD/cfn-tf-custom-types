                "InstanceType"
            ]
        },
        "KerberosAttributes": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AdDomainJoinPassword": {
                    "type": "string"
                },
                "AdDomainJoinUser": {
                    "type": "string"
                },
                "CrossRealmTrustPrincipalPassword": {
                    "type": "string"
                },
                "KdcAdminPassword": {
                    "type": "string"
                },
                "Realm": {
                    "type": "string"
                }
            },
