                    "type": "string"
                },
                "AdditionalSlaveSecurityGroups": {
                    "type": "string"
                },
                "EmrManagedMasterSecurityGroup": {
                    "type": "string"
                },
                "EmrManagedSlaveSecurityGroup": {
                    "type": "string"
                },
                "InstanceProfile": {
                    "type": "string"
                },
                "KeyName": {
                    "type": "string"
                },
                "ServiceAccessSecurityGroup": {
                    "type": "string"
                },
                "SubnetId": {
                    "type": "string"
                }
            },
            "required": [
                "InstanceProfile"
            ]
        },
        "InstanceGroup": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AutoscalingPolicy": {
                    "type": "string"
