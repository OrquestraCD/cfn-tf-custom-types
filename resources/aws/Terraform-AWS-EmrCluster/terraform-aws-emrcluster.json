                },
                "InstanceCount": {
                    "type": "number"
                },
                "InstanceRole": {
                    "type": "string"
                },
                "InstanceType": {
                    "type": "string"
                },
                "Name": {
                    "type": "string"
                },
                "EbsConfig": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "$ref": "#/definitions/EbsConfig"
                    }
                }
            },
            "required": [
