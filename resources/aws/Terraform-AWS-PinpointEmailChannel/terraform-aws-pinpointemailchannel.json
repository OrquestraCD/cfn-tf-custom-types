{
    "typeName": "Terraform::AWS::PinpointEmailChannel",
    "description": "Provides a Pinpoint SMS Channel resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ApplicationId": {
            "type": "string",
            "description": "The application ID."
        },
        "Enabled": {
            "type": "boolean",
            "description": "Whether the channel is enabled or disabled. Defaults to `true`."
        },
        "FromAddress": {
            "type": "string",
            "description": "The email address used to send emails from."
        },
        "Id": {
            "type": "string"
        },
        "Identity": {
            "type": "string",
            "description": "The ARN of an identity verified with SES."
        },
        "MessagesPerSecond": {
            "type": "number"
        },
        "RoleArn": {
            "type": "string",
            "description": "The ARN of an IAM Role used to submit events to Mobile Analytics' event ingestion service."
        }
    },
    "additionalProperties": false,
    "required": [
        "ApplicationId",
        "FromAddress",
        "Identity",
        "RoleArn"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/MessagesPerSecond"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}