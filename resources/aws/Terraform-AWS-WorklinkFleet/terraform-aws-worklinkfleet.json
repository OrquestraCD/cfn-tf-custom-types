        },
        "AuditStreamArn": {
            "type": "string"
        },
        "CompanyCode": {
            "type": "string"
        },
        "CreatedTime": {
            "type": "string"
        },
        "DeviceCaCertificate": {
            "type": "string"
        },
        "DisplayName": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "LastUpdatedTime": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
