{
    "typeName": "Terraform::AWS::LambdaAlias",
    "description": "CloudFormation equivalent of aws_lambda_alias",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "RoutingConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AdditionalVersionWeights": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "properties": {
                            "Key": {
                                "type": "string"
                            },
                            "Value": {
                                "type": "number"
                            }
                        },
                        "required": [
                            "Key",
                            "Value"
                        ]
                    }
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Arn": {
            "type": "string"
        },
        "Description": {
            "type": "string"
        },
        "FunctionName": {
            "type": "string"
        },
        "FunctionVersion": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "InvokeArn": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "RoutingConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/RoutingConfig"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "FunctionName",
        "FunctionVersion",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Arn",
        "/properties/InvokeArn"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}