{
    "typeName": "Terraform::AWS::S3BucketInventory",
    "description": "Provides a S3 bucket [inventory configuration](https://docs.aws.amazon.com/AmazonS3/latest/dev/storage-inventory.html) resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Destination": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Bucket": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/Bucket"
                    },
                    "maxItems": 1,
                    "minItems": 1
                }
            },
            "required": []
        },
        "Filter": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Prefix": {
                    "type": "string",
                    "description": "The prefix that an object must have to be included in the inventory results."
                }
            },
            "required": []
        },
        "Schedule": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Frequency": {
                    "type": "string",
                    "description": "Specifies how frequently inventory results are produced. Valid values: `Daily`, `Weekly`."
                }
            },
            "required": [
                "Frequency"
            ]
        },
        "Bucket": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AccountId": {
                    "type": "string",
                    "description": "The ID of the account that owns the destination bucket. Recommended to be set to prevent problems if the destination bucket ownership changes."
                },
                "BucketArn": {
                    "type": "string",
                    "description": "The Amazon S3 bucket ARN of the destination."
                },
                "Format": {
                    "type": "string",
                    "description": "Specifies the output format of the inventory results. Can be `CSV`, [`ORC`](https://orc.apache.org/) or [`Parquet`](https://parquet.apache.org/)."
                },
                "Prefix": {
                    "type": "string",
                    "description": "The prefix that is prepended to all inventory results."
                },
                "Encryption": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/Encryption"
                    },
                    "maxItems": 1
                }
            },
            "required": [
                "BucketArn",
                "Format"
            ]
        },
        "Encryption": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "SseKms": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/SseKms"
                    },
                    "maxItems": 1
                },
                "SseS3": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/SseS3"
                    },
                    "maxItems": 1
                }
            },
            "required": []
        },
        "SseKms": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "KeyId": {
                    "type": "string",
                    "description": "The ARN of the KMS customer master key (CMK) used to encrypt the inventory file."
                }
            },
            "required": [
                "KeyId"
            ]
        },
        "SseS3": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "IsPropertyDefined": {
                    "type": "boolean"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Bucket": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Bucket"
            },
            "maxItems": 1,
            "minItems": 1
        },
        "Enabled": {
            "type": "boolean",
            "description": "Specifies whether the inventory is enabled or disabled."
        },
        "Id": {
            "type": "string"
        },
        "IncludedObjectVersions": {
            "type": "string",
            "description": "Object versions to include in the inventory list. Valid values: `All`, `Current`."
        },
        "Name": {
            "type": "string",
            "description": "Unique identifier of the inventory configuration for the bucket."
        },
        "OptionalFields": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "List of optional fields that are included in the inventory results.\nValid values: `Size`, `LastModifiedDate`, `StorageClass`, `ETag`, `IsMultipartUploaded`, `ReplicationStatus`, `EncryptionStatus`, `ObjectLockRetainUntilDate`, `ObjectLockMode`, `ObjectLockLegalHoldStatus`, `IntelligentTieringAccessTier`."
        },
        "Destination": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Destination"
            },
            "maxItems": 1,
            "minItems": 1
        },
        "Filter": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Filter"
            },
            "maxItems": 1
        },
        "Schedule": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Schedule"
            },
            "maxItems": 1,
            "minItems": 1
        },
        "Encryption": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Encryption"
            },
            "maxItems": 1
        },
        "SseKms": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/SseKms"
            },
            "maxItems": 1
        },
        "SseS3": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/SseS3"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Bucket",
        "IncludedObjectVersions",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}