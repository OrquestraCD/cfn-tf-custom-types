{
    "typeName": "Terraform::AWS::DirectoryServiceDirectory",
    "description": "CloudFormation equivalent of aws_directory_service_directory",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "ConnectSettings": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "CustomerDnsIps": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                },
                "CustomerUsername": {
                    "type": "string"
                },
                "SubnetIds": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                },
                "VpcId": {
                    "type": "string"
                }
            },
            "required": [
                "CustomerDnsIps",
                "CustomerUsername",
                "SubnetIds",
                "VpcId"
            ]
        },
        "VpcSettings": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "SubnetIds": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                },
                "VpcId": {
                    "type": "string"
                }
            },
            "required": [
                "SubnetIds",
                "VpcId"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AccessUrl": {
            "type": "string"
        },
        "Alias": {
            "type": "string"
        },
        "Description": {
            "type": "string"
        },
        "DnsIpAddresses": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Edition": {
            "type": "string"
        },
        "EnableSso": {
            "type": "boolean"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "Password": {
            "type": "string"
        },
        "SecurityGroupId": {
            "type": "string"
        },
        "ShortName": {
            "type": "string"
        },
        "Size": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            }
        },
        "Type": {
            "type": "string"
        },
        "ConnectSettings": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/ConnectSettings"
            },
            "maxItems": 1
        },
        "VpcSettings": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/VpcSettings"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "Password"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/AccessUrl",
        "/properties/DnsIpAddresses",
        "/properties/Id",
        "/properties/SecurityGroupId"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}