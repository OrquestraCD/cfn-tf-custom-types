{
    "typeName": "Terraform::AWS::WorkspacesDirectory",
    "description": "Provides a directory registration in AWS WorkSpaces Service",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "SelfServicePermissions": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "ChangeComputeType": {
                    "type": "boolean"
                },
                "IncreaseVolumeSize": {
                    "type": "boolean"
                },
                "RebuildWorkspace": {
                    "type": "boolean"
                },
                "RestartWorkspace": {
                    "type": "boolean"
                },
                "SwitchRunningMode": {
                    "type": "boolean"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "DirectoryId": {
            "type": "string",
            "description": "The directory identifier for registration in WorkSpaces service."
        },
        "Id": {
            "type": "string"
        },
        "SubnetIds": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "The identifiers of the subnets where the directory resides."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            }
        },
        "SelfServicePermissions": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/SelfServicePermissions"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "DirectoryId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}