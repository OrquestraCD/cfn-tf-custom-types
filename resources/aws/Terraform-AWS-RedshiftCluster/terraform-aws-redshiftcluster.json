{
    "typeName": "Terraform::AWS::RedshiftCluster",
    "description": "CloudFormation equivalent of aws_redshift_cluster",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Logging": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "BucketName": {
                    "type": "string"
                },
                "Enable": {
                    "type": "boolean"
                },
                "S3KeyPrefix": {
                    "type": "string"
                }
            },
            "required": [
                "Enable"
            ]
        },
        "SnapshotCopy": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "DestinationRegion": {
                    "type": "string"
                },
                "GrantName": {
                    "type": "string"
                },
                "RetentionPeriod": {
                    "type": "number"
                }
            },
            "required": [
                "DestinationRegion"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AllowVersionUpgrade": {
            "type": "boolean"
        },
        "Arn": {
            "type": "string"
        },
        "AutomatedSnapshotRetentionPeriod": {
            "type": "number"
        },
        "AvailabilityZone": {
            "type": "string"
        },
        "BucketName": {
            "type": "string"
        },
        "ClusterIdentifier": {
            "type": "string"
        },
        "ClusterParameterGroupName": {
            "type": "string"
        },
        "ClusterPublicKey": {
            "type": "string"
        },
        "ClusterRevisionNumber": {
            "type": "string"
        },
        "ClusterSecurityGroups": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "ClusterSubnetGroupName": {
            "type": "string"
        },
        "ClusterType": {
            "type": "string"
        },
        "ClusterVersion": {
            "type": "string"
        },
        "DatabaseName": {
            "type": "string"
        },
        "DnsName": {
            "type": "string"
        },
        "ElasticIp": {
            "type": "string"
        },
        "EnableLogging": {
            "type": "boolean"
        },
        "Encrypted": {
            "type": "boolean"
        },
        "Endpoint": {
            "type": "string"
        },
        "EnhancedVpcRouting": {
            "type": "boolean"
        },
        "FinalSnapshotIdentifier": {
            "type": "string"
        },
        "IamRoles": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Id": {
            "type": "string"
        },
        "KmsKeyId": {
            "type": "string"
        },
        "MasterPassword": {
            "type": "string"
        },
        "MasterUsername": {
            "type": "string"
        },
        "NodeType": {
            "type": "string"
        },
        "NumberOfNodes": {
            "type": "number"
        },
        "OwnerAccount": {
            "type": "string"
        },
        "Port": {
            "type": "number"
        },
        "PreferredMaintenanceWindow": {
            "type": "string"
        },
        "PubliclyAccessible": {
            "type": "boolean"
        },
        "S3KeyPrefix": {
            "type": "string"
        },
        "SkipFinalSnapshot": {
            "type": "boolean"
        },
        "SnapshotClusterIdentifier": {
            "type": "string"
        },
        "SnapshotIdentifier": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "VpcSecurityGroupIds": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Logging": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Logging"
            },
            "maxItems": 1
        },
        "SnapshotCopy": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/SnapshotCopy"
            },
            "maxItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "ClusterIdentifier",
        "NodeType"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Arn",
        "/properties/DnsName"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}