{
    "typeName": "Terraform::AWS::NetworkInterface",
    "description": "Provides an Elastic network interface (ENI) resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Attachment": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "DeviceIndex": {
                    "type": "number",
                    "description": "Integer to define the devices index."
                },
                "Instance": {
                    "type": "string",
                    "description": "ID of the instance to attach to."
                }
            },
            "required": [
                "DeviceIndex",
                "Instance"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "A description for the network interface."
        },
        "Id": {
            "type": "string"
        },
        "MacAddress": {
            "type": "string"
        },
        "PrivateDnsName": {
            "type": "string"
        },
        "PrivateIp": {
            "type": "string"
        },
        "PrivateIps": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "List of private IPs to assign to the ENI."
        },
        "PrivateIpsCount": {
            "type": "number",
            "description": "Number of secondary private IPs to assign to the ENI. The total number of private IPs will be 1 + private_ips_count, as a primary private IP will be assiged to an ENI by default."
        },
        "SecurityGroups": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "List of security group IDs to assign to the ENI."
        },
        "SourceDestCheck": {
            "type": "boolean",
            "description": "Whether to enable source destination checking for the ENI. Default true."
        },
        "SubnetId": {
            "type": "string",
            "description": "Subnet ID to create the ENI in."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "A mapping of tags to assign to the resource."
        },
        "Attachment": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Attachment"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "SubnetId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/MacAddress",
        "/properties/PrivateDnsName"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}