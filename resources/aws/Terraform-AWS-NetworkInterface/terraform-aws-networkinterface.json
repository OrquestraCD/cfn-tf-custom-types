{
    "typeName": "Terraform::AWS::NetworkInterface",
    "description": "CloudFormation equivalent of aws_network_interface",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Attachment": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "DeviceIndex": {
                    "type": "number"
                },
                "Instance": {
                    "type": "string"
                }
            },
            "required": [
                "DeviceIndex",
                "Instance"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "MacAddress": {
            "type": "string"
        },
        "PrivateDnsName": {
            "type": "string"
        },
        "PrivateIp": {
            "type": "string"
        },
        "PrivateIps": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "PrivateIpsCount": {
            "type": "number"
        },
        "SecurityGroups": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "SourceDestCheck": {
            "type": "boolean"
        },
        "SubnetId": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            }
        },
        "Attachment": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Attachment"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "SubnetId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/MacAddress",
        "/properties/PrivateDnsName"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}