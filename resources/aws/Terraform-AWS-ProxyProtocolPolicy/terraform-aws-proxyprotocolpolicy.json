{
    "typeName": "Terraform::AWS::ProxyProtocolPolicy",
    "description": "Provides a proxy protocol policy, which allows an ELB to carry a client connection information to a backend.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "InstancePorts": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "List of instance ports to which the policy\nshould be applied. This can be specified if the protocol is SSL or TCP."
        },
        "LoadBalancer": {
            "type": "string",
            "description": "The load balancer to which the policy\nshould be attached."
        }
    },
    "additionalProperties": false,
    "required": [
        "InstancePorts",
        "LoadBalancer"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}