{
    "typeName": "Terraform::AWS::RdsClusterEndpoint",
    "description": "Manages a RDS Aurora Cluster Endpoint.\nYou can refer to the [User Guide][1].",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Arn": {
            "type": "string"
        },
        "ClusterEndpointIdentifier": {
            "type": "string",
            "description": "The identifier to use for the new endpoint. This parameter is stored as a lowercase string."
        },
        "ClusterIdentifier": {
            "type": "string",
            "description": "The cluster identifier."
        },
        "CustomEndpointType": {
            "type": "string",
            "description": "The type of the endpoint. One of: READER , ANY ."
        },
        "Endpoint": {
            "type": "string"
        },
        "ExcludedMembers": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "List of DB instance identifiers that aren't part of the custom endpoint group. All other eligible instances are reachable through the custom endpoint. Only relevant if the list of static members is empty. Conflicts with `static_members`."
        },
        "Id": {
            "type": "string"
        },
        "StaticMembers": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "List of DB instance identifiers that are part of the custom endpoint group. Conflicts with `excluded_members`."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "Key-value mapping of resource tags."
        }
    },
    "additionalProperties": false,
    "required": [
        "ClusterEndpointIdentifier",
        "ClusterIdentifier",
        "CustomEndpointType"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Arn",
        "/properties/Endpoint",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}