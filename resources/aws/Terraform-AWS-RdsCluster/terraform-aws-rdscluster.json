            "type": "string"
        },
        "Engine": {
            "type": "string"
        },
        "EngineMode": {
            "type": "string"
        },
        "EngineVersion": {
            "type": "string"
        },
        "FinalSnapshotIdentifier": {
            "type": "string"
        },
