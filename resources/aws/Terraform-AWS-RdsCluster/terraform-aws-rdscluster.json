        "DbClusterParameterGroupName": {
            "type": "string"
        },
        "DbSubnetGroupName": {
            "type": "string"
        },
        "DeletionProtection": {
            "type": "boolean"
        },
        "EnableHttpEndpoint": {
            "type": "boolean"
