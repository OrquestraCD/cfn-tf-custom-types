        "IamRoles": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "KmsKeyId": {
            "type": "string"
        },
        "MasterPassword": {
            "type": "string"
        },
        "MasterUsername": {
            "type": "string"
