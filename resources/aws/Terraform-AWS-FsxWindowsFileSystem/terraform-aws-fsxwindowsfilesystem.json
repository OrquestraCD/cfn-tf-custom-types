{
    "typeName": "Terraform::AWS::FsxWindowsFileSystem",
    "description": "CloudFormation equivalent of aws_fsx_windows_file_system",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "SelfManagedActiveDirectory": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "DnsIps": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                },
                "DomainName": {
                    "type": "string"
                },
                "FileSystemAdministratorsGroup": {
                    "type": "string"
                },
                "OrganizationalUnitDistinguishedName": {
                    "type": "string"
                },
                "Password": {
                    "type": "string"
                },
                "Username": {
                    "type": "string"
                }
            },
            "required": [
                "DnsIps",
                "DomainName",
                "Password",
                "Username"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ActiveDirectoryId": {
            "type": "string"
        },
        "Arn": {
            "type": "string"
        },
        "AutomaticBackupRetentionDays": {
            "type": "number"
        },
        "CopyTagsToBackups": {
            "type": "boolean"
        },
        "DailyAutomaticBackupStartTime": {
            "type": "string"
        },
        "DnsName": {
            "type": "string"
        },
        "KmsKeyId": {
            "type": "string"
        },
        "NetworkInterfaceIds": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "OwnerId": {
            "type": "string"
        },
        "SecurityGroupIds": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "SkipFinalBackup": {
            "type": "boolean"
        },
        "StorageCapacity": {
            "type": "number"
        },
        "SubnetIds": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "ThroughputCapacity": {
            "type": "number"
        },
        "VpcId": {
            "type": "string"
        },
        "WeeklyMaintenanceStartTime": {
            "type": "string"
        },
        "SelfManagedActiveDirectory": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/SelfManagedActiveDirectory"
            },
            "maxItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "StorageCapacity",
        "SubnetIds",
        "ThroughputCapacity"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Arn",
        "/properties/DnsName",
        "/properties/NetworkInterfaceIds",
        "/properties/OwnerId",
        "/properties/VpcId"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}