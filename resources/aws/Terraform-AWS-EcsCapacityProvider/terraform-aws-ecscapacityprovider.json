{
    "typeName": "Terraform::AWS::EcsCapacityProvider",
    "description": "CloudFormation equivalent of aws_ecs_capacity_provider",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "AutoScalingGroupProvider": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AutoScalingGroupArn": {
                    "type": "string"
                },
                "ManagedTerminationProtection": {
                    "type": "string"
                },
                "ManagedScaling": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/ManagedScaling"
                    }
                }
            },
            "required": [
                "AutoScalingGroupArn"
            ]
        },
        "ManagedScaling": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MaximumScalingStepSize": {
                    "type": "number"
                },
                "MinimumScalingStepSize": {
                    "type": "number"
                },
                "Status": {
                    "type": "string"
                },
                "TargetCapacity": {
                    "type": "number"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Arn": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "AutoScalingGroupProvider": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/AutoScalingGroupProvider"
            },
            "maxItems": 1,
            "minItems": 1
        },
        "ManagedScaling": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/ManagedScaling"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Arn"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}