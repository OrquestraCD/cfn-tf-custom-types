{
    "typeName": "Terraform::AWS::LoadBalancerBackendServerPolicy",
    "description": "Attaches a load balancer policy to an ELB backend server.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "InstancePort": {
            "type": "number",
            "description": "The instance port to apply the policy to."
        },
        "LoadBalancerName": {
            "type": "string",
            "description": "The load balancer to attach the policy to."
        },
        "PolicyNames": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "List of Policy Names to apply to the backend server."
        }
    },
    "additionalProperties": false,
