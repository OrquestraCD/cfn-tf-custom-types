{
    "typeName": "Terraform::AWS::DlmLifecyclePolicy",
    "description": "Provides a [Data Lifecycle Manager (DLM) lifecycle policy](https://docs.aws.amazon.com/AWSEC2/latest/UserGuide/snapshot-lifecycle.html) for managing snapshots.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "PolicyDetails": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "ResourceTypes": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                },
                "TargetTags": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "properties": {
                            "MapKey": {
                                "type": "string"
                            },
                            "MapValue": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "MapKey",
                            "MapValue"
                        ]
                    }
                },
                "Schedule": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/Schedule"
                    },
                    "minItems": 1
                }
            },
            "required": [
                "ResourceTypes",
                "TargetTags"
            ]
        },
        "Schedule": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "CopyTags": {
                    "type": "boolean"
                },
                "Name": {
                    "type": "string"
                },
                "TagsToAdd": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "properties": {
                            "MapKey": {
                                "type": "string"
                            },
                            "MapValue": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "MapKey",
                            "MapValue"
                        ]
                    }
                },
                "CreateRule": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/CreateRule"
                    },
                    "maxItems": 1,
                    "minItems": 1
                },
                "RetainRule": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/RetainRule"
                    },
                    "maxItems": 1,
                    "minItems": 1
                }
            },
            "required": [
                "Name"
            ]
        },
        "CreateRule": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Interval": {
                    "type": "number"
                },
                "IntervalUnit": {
                    "type": "string"
                },
                "Times": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                }
            },
            "required": [
                "Interval"
            ]
        },
        "RetainRule": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Count": {
                    "type": "number"
                }
            },
            "required": [
                "Count"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Arn": {
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "A description for the DLM lifecycle policy."
        },
        "ExecutionRoleArn": {
            "type": "string",
            "description": "The ARN of an IAM role that is able to be assumed by the DLM service."
        },
        "Id": {
            "type": "string"
        },
        "State": {
            "type": "string",
            "description": "Whether the lifecycle policy should be enabled or disabled. `ENABLED` or `DISABLED` are valid values. Defaults to `ENABLED`."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "Key-value mapping of resource tags."
        },
        "PolicyDetails": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/PolicyDetails"
            },
            "maxItems": 1,
            "minItems": 1
        },
        "Schedule": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Schedule"
            },
            "minItems": 1
        },
        "CreateRule": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/CreateRule"
            },
            "maxItems": 1,
            "minItems": 1
        },
        "RetainRule": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/RetainRule"
            },
            "maxItems": 1,
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Description",
        "ExecutionRoleArn"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Arn",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}