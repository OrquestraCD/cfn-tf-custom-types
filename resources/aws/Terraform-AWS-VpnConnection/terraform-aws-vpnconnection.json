{
    "typeName": "Terraform::AWS::VpnConnection",
    "description": "Manages an EC2 VPN connection. These objects can be connected to customer gateways, and allow you to establish tunnels between your network and Amazon.\n\n~> **Note:** All arguments including `tunnel1_preshared_key` and `tunnel2_preshared_key` will be stored in the raw state as plain-text.\n[Read more about sensitive data in state](/docs/state/sensitive-data.html).\n\n~> **Note:** The CIDR blocks in the arguments `tunnel1_inside_cidr` and `tunnel2_inside_cidr` must have a prefix of /30 and be a part of a specific range.\n[Read more about this in the AWS documentation](https://docs.aws.amazon.com/AWSEC2/latest/APIReference/API_VpnTunnelOptionsSpecification.html).",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/aws/Terraform-AWS-VpnConnection/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CustomerGatewayConfiguration": {
            "type": "string"
        },
        "CustomerGatewayId": {
            "type": "string",
            "description": "The ID of the customer gateway."
        },
        "Id": {
            "type": "string"
        },
        "Routes": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "DestinationCidrBlock": {
                        "type": "string"
                    },
                    "Source": {
                        "type": "string"
                    },
                    "State": {
                        "type": "string"
                    }
                }
            }
        },
        "StaticRoutesOnly": {
            "type": "boolean",
            "description": "Whether the VPN connection uses static routes exclusively. Static routes must be used for devices that don't support BGP."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "Tags to apply to the connection."
        },
        "TransitGatewayAttachmentId": {
            "type": "string"
        },
        "TransitGatewayId": {
            "type": "string",
            "description": "The ID of the EC2 Transit Gateway."
        },
        "Tunnel1Address": {
            "type": "string"
        },
        "Tunnel1BgpAsn": {
            "type": "string"
        },
        "Tunnel1BgpHoldtime": {
            "type": "number"
        },
        "Tunnel1CgwInsideAddress": {
            "type": "string"
        },
        "Tunnel1InsideCidr": {
            "type": "string",
            "description": "The CIDR block of the inside IP addresses for the first VPN tunnel."
        },
        "Tunnel1PresharedKey": {
            "type": "string",
            "description": "The preshared key of the first VPN tunnel."
        },
        "Tunnel1VgwInsideAddress": {
            "type": "string"
        },
        "Tunnel2Address": {
            "type": "string"
        },
        "Tunnel2BgpAsn": {
            "type": "string"
        },
        "Tunnel2BgpHoldtime": {
            "type": "number"
        },
        "Tunnel2CgwInsideAddress": {
            "type": "string"
        },
        "Tunnel2InsideCidr": {
            "type": "string",
            "description": "The CIDR block of the inside IP addresses for the second VPN tunnel."
        },
        "Tunnel2PresharedKey": {
            "type": "string",
            "description": "The preshared key of the second VPN tunnel."
        },
        "Tunnel2VgwInsideAddress": {
            "type": "string"
        },
        "Type": {
            "type": "string",
            "description": "The type of VPN connection. The only type AWS supports at this time is \"ipsec.1\"."
        },
        "VgwTelemetry": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "AcceptedRouteCount": {
                        "type": "number"
                    },
                    "LastStatusChange": {
                        "type": "string"
                    },
                    "OutsideIpAddress": {
                        "type": "string"
                    },
                    "Status": {
                        "type": "string"
                    },
                    "StatusMessage": {
                        "type": "string"
                    }
                }
            }
        },
        "VpnGatewayId": {
            "type": "string",
            "description": "The ID of the Virtual Private Gateway."
        }
    },
    "additionalProperties": false,
    "required": [
        "CustomerGatewayId",
        "Type"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/CustomerGatewayConfiguration",
        "/properties/Id",
        "/properties/Routes",
        "/properties/TransitGatewayAttachmentId",
        "/properties/Tunnel1Address",
        "/properties/Tunnel1BgpAsn",
        "/properties/Tunnel1BgpHoldtime",
        "/properties/Tunnel1CgwInsideAddress",
        "/properties/Tunnel1VgwInsideAddress",
        "/properties/Tunnel2Address",
        "/properties/Tunnel2BgpAsn",
        "/properties/Tunnel2BgpHoldtime",
        "/properties/Tunnel2CgwInsideAddress",
        "/properties/Tunnel2VgwInsideAddress",
        "/properties/VgwTelemetry"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    },
    "writeOnlyProperties": [
        "/properties/Tunnel1PresharedKey",
        "/properties/Tunnel2PresharedKey"
    ]
}