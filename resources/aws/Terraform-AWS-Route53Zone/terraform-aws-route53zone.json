{
    "typeName": "Terraform::AWS::Route53Zone",
    "description": "Manages a Route53 Hosted Zone.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Vpc": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "VpcId": {
                    "type": "string"
                },
                "VpcRegion": {
                    "type": "string"
                }
            },
            "required": [
                "VpcId"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Comment": {
            "type": "string",
            "description": "A comment for the hosted zone. Defaults to 'Managed by Terraform'."
        },
        "DelegationSetId": {
            "type": "string",
            "description": "The ID of the reusable delegation set whose NS records you want to assign to the hosted zone. Conflicts with `vpc` as delegation sets can only be used for public zones."
        },
        "ForceDestroy": {
            "type": "boolean",
            "description": "Whether to destroy all records (possibly managed outside of Terraform) in the zone when destroying the zone."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "This is the name of the hosted zone."
        },
        "NameServers": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "A mapping of tags to assign to the zone."
        },
        "VpcId": {
            "type": "string"
        },
        "VpcRegion": {
            "type": "string"
        },
        "ZoneId": {
            "type": "string"
        },
        "Vpc": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Vpc"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/NameServers",
        "/properties/ZoneId"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}