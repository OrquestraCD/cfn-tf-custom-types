{
    "typeName": "Terraform::AWS::EfsMountTarget",
    "description": "Provides an Elastic File System (EFS) mount target.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "DnsName": {
            "type": "string"
        },
        "FileSystemArn": {
            "type": "string"
        },
        "FileSystemId": {
            "type": "string",
            "description": "The ID of the file system for which the mount target is intended."
        },
        "Id": {
            "type": "string"
        },
        "IpAddress": {
            "type": "string",
            "description": "The address (within the address range of the specified subnet) at\nwhich the file system may be mounted via the mount target."
        },
        "NetworkInterfaceId": {
            "type": "string"
        },
        "SecurityGroups": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "A list of up to 5 VPC security group IDs (that must\nbe for the same VPC as subnet specified) in effect for the mount target."
        },
        "SubnetId": {
            "type": "string",
            "description": "The ID of the subnet to add the mount target in."
        }
    },
    "additionalProperties": false,
    "required": [
        "FileSystemId",
        "SubnetId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/DnsName",
        "/properties/FileSystemArn",
        "/properties/Id",
        "/properties/NetworkInterfaceId"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}