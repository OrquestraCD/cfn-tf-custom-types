{
    "typeName": "Terraform::AWS::DatasyncLocationS3",
    "description": "CloudFormation equivalent of aws_datasync_location_s3",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "S3Config": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "BucketAccessRoleArn": {
                    "type": "string"
                }
            },
            "required": [
                "BucketAccessRoleArn"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Arn": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "S3BucketArn": {
            "type": "string"
        },
        "Subdirectory": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "Uri": {
            "type": "string"
        },
        "S3Config": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/S3Config"
            },
            "maxItems": 1,
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "S3BucketArn",
        "Subdirectory"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Arn",
        "/properties/Uri"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}