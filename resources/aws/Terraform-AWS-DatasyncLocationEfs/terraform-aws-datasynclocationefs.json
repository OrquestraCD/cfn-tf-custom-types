{
    "typeName": "Terraform::AWS::DatasyncLocationEfs",
    "description": "Manages an AWS DataSync EFS Location.\n\n~> **NOTE:** The EFS File System must have a mounted EFS Mount Target before creating this resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Ec2Config": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "SecurityGroupArns": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                },
                "SubnetArn": {
                    "type": "string"
                }
            },
            "required": [
                "SecurityGroupArns",
                "SubnetArn"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Arn": {
            "type": "string"
        },
        "EfsFileSystemArn": {
            "type": "string",
            "description": "Amazon Resource Name (ARN) of EFS File System."
        },
        "Id": {
            "type": "string"
        },
        "Subdirectory": {
            "type": "string",
            "description": "Subdirectory to perform actions as source or destination. Default `/`."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "Key-value pairs of resource tags to assign to the DataSync Location."
        },
        "Uri": {
            "type": "string"
        },
        "Ec2Config": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Ec2Config"
            },
            "maxItems": 1,
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "EfsFileSystemArn"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Arn",
        "/properties/Id",
        "/properties/Uri"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}