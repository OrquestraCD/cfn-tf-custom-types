{
    "typeName": "Terraform::AWS::SesDomainDkim",
    "description": "Provides an SES domain DKIM generation resource.\n\nDomain ownership needs to be confirmed first using [ses_domain_identity Resource](/docs/providers/aws/r/ses_domain_identity.html)",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "DkimTokens": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "Domain": {
            "type": "string",
            "description": "Verified domain name to generate DKIM tokens for."
        },
        "Id": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "Domain"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/DkimTokens",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}