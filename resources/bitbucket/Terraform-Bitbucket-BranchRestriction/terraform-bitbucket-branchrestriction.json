{
    "typeName": "Terraform::Bitbucket::BranchRestriction",
    "description": "Provides a Bitbucket branch restriction resource.\n\nThis allows you for setting up branch restrictions for your repository.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Groups": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Owner": {
                    "type": "string"
                },
                "Slug": {
                    "type": "string"
                }
            },
            "required": [
                "Owner",
                "Slug"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Kind": {
            "type": "string",
            "description": "The type of restriction that is being applied. List of possible stages is [here](https://developer.atlassian.com/bitbucket/api/2/reference/resource/repositories/%7Busername%7D/%7Brepo_slug%7D/branch-restrictions)."
        },
        "Owner": {
            "type": "string",
            "description": "The owner of this repository. Can be you or any team you\nhave write access to."
        },
        "Pattern": {
            "type": "string",
            "description": "The pattern to determine which branches will be restricted."
        },
        "Repository": {
            "type": "string",
            "description": "The name of the repository."
        },
        "Users": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "A list of users to use."
        },
        "Value": {
            "type": "number"
        },
        "Groups": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Groups"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Kind",
        "Owner",
        "Pattern",
        "Repository"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}