        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CloneHttps": {
            "type": "string"
        },
        "CloneSsh": {
            "type": "string"
        },
        "Description": {
            "type": "string"
        },
        "ForkPolicy": {
            "type": "string"
        },
        "HasIssues": {
            "type": "boolean"
        },
        "HasWiki": {
            "type": "boolean"
        },
        "Id": {
            "type": "string"
        },
        "IsPrivate": {
            "type": "boolean"
        },
        "Language": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "Owner": {
            "type": "string"
        },
        "PipelinesEnabled": {
            "type": "boolean"
        },
        "ProjectKey": {
            "type": "string"
        },
        "Scm": {
            "type": "string"
        },
