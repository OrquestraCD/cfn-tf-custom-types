{
    "typeName": "Terraform::Bitbucket::Hook",
    "description": "CloudFormation equivalent of bitbucket_hook",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Active": {
            "type": "boolean"
        },
        "Description": {
            "type": "string"
        },
        "Events": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Owner": {
            "type": "string"
        },
        "Repository": {
            "type": "string"
        },
        "SkipCertVerification": {
            "type": "boolean"
        },
        "Url": {
            "type": "string"
        },
        "Uuid": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "Description",
        "Events",
        "Owner",
        "Repository",
        "Url"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Uuid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}