{
    "typeName": "Terraform::PostgreSQL::Extension",
    "description": "The ``postgresql_extension`` resource creates and manages an extension on a PostgreSQL\nserver.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Database": {
            "type": "string",
            "description": "Which database to create the extension on. Defaults to provider database."
        },
        "Name": {
            "type": "string",
            "description": "The name of the extension."
        },
        "Schema": {
            "type": "string",
            "description": "Sets the schema of an extension."
        },
        "Version": {
            "type": "string",
            "description": "Sets the version number of the extension."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}