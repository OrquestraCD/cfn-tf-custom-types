{
    "typeName": "Terraform::PostgreSQL::Grant",
    "description": "The ``postgresql_grant`` resource creates and manages privileges given to a user for a database schema.\n\n~> **Note:** This resource needs Postgresql version 9 or above.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Database": {
            "type": "string",
            "description": "The database to grant privileges on for this role."
        },
        "Id": {
            "type": "string"
        },
        "ObjectType": {
            "type": "string",
            "description": "The PostgreSQL object type to grant the privileges on (one of: table, sequence)."
        },
        "Privileges": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "The list of privileges to grant."
        },
        "Role": {
            "type": "string",
            "description": "The name of the role to grant privileges on."
        },
        "Schema": {
            "type": "string",
            "description": "The database schema to grant privileges on for this role."
        }
    },
    "additionalProperties": false,
    "required": [
        "Database",
        "ObjectType",
        "Privileges",
        "Role",
        "Schema"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}