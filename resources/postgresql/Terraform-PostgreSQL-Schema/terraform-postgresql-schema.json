{
    "typeName": "Terraform::PostgreSQL::Schema",
    "description": "CloudFormation equivalent of postgresql_schema",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Policy": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "boolean"
                },
                "CreateWithGrant": {
                    "type": "boolean"
                },
                "Role": {
                    "type": "string"
                },
                "Usage": {
                    "type": "boolean"
                },
                "UsageWithGrant": {
                    "type": "boolean"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Database": {
            "type": "string"
        },
        "DropCascade": {
            "type": "boolean"
        },
        "Id": {
            "type": "string"
        },
        "IfNotExists": {
            "type": "boolean"
        },
        "Name": {
            "type": "string"
        },
        "Owner": {
            "type": "string"
        },
        "Policy": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Policy"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}