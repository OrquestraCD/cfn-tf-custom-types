{
    "typeName": "Terraform::Google::FilestoreInstance",
    "description": "CloudFormation equivalent of google_filestore_instance",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "FileShares": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "CapacityGb": {
                    "type": "number"
                },
                "Name": {
                    "type": "string"
                }
            },
            "required": [
                "CapacityGb",
                "Name"
            ]
        },
        "Networks": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Modes": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                },
                "Network": {
                    "type": "string"
                },
                "ReservedIpRange": {
                    "type": "string"
                }
            },
            "required": [
                "Modes",
                "Network"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CreateTime": {
            "type": "string"
        },
        "Description": {
            "type": "string"
        },
        "Etag": {
            "type": "string"
        },
        "Labels": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "Name": {
            "type": "string"
        },
        "Project": {
            "type": "string"
        },
        "Tier": {
            "type": "string"
        },
        "Zone": {
            "type": "string"
        },
        "FileShares": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/FileShares"
            },
            "maxItems": 1,
            "minItems": 1
        },
        "Networks": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Networks"
            },
            "minItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "Tier",
        "Zone"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/CreateTime",
        "/properties/Etag"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}