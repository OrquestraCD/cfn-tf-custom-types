{
    "typeName": "Terraform::Google::BigtableGcPolicy",
    "description": "Creates a Google Cloud Bigtable GC Policy inside a family. For more information see\n[the official documentation](https://cloud.google.com/bigtable/) and\n[API](https://cloud.google.com/bigtable/docs/go/reference).",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "MaxAge": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Days": {
                    "type": "number",
                    "description": "Number of days before applying GC policy."
                }
            },
            "required": [
                "Days"
            ]
        },
        "MaxVersion": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Number": {
                    "type": "number",
                    "description": "Number of version before applying the GC policy."
                }
            },
            "required": [
                "Number"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ColumnFamily": {
            "type": "string",
            "description": "The name of the column family."
        },
        "Id": {
            "type": "string"
        },
        "InstanceName": {
            "type": "string",
            "description": "The name of the Bigtable instance."
        },
        "Mode": {
            "type": "string",
            "description": "If multiple policies are set, you should choose between `UNION` OR `INTERSECTION`."
        },
        "Project": {
            "type": "string",
            "description": "The ID of the project in which the resource belongs. If it is not provided, the provider project is used."
        },
        "Table": {
            "type": "string",
            "description": "The name of the table."
        },
        "MaxAge": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/MaxAge"
            }
        },
        "MaxVersion": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/MaxVersion"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "ColumnFamily",
        "InstanceName",
        "Table"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}