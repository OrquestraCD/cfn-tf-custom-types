{
    "typeName": "Terraform::Google::StorageNotification",
    "description": "CloudFormation equivalent of google_storage_notification",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Bucket": {
            "type": "string"
        },
        "CustomAttributes": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "EventTypes": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "NotificationId": {
            "type": "string"
        },
        "ObjectNamePrefix": {
            "type": "string"
        },
        "PayloadFormat": {
            "type": "string"
        },
        "SelfLink": {
            "type": "string"
        },
        "Topic": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "Bucket",
        "PayloadFormat",
        "Topic"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/NotificationId",
        "/properties/SelfLink"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}