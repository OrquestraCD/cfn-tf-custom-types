{
    "typeName": "Terraform::Google::ComputeSharedVpcServiceProject",
    "description": "Enables the Google Compute Engine\n[Shared VPC](https://cloud.google.com/compute/docs/shared-vpc)\nfeature for a project, assigning it as a Shared VPC service project associated\nwith a given host project.\n\nFor more information, see,\n[the Project API documentation](https://cloud.google.com/compute/docs/reference/latest/projects),\nwhere the Shared VPC feature is referred to by its former name \"XPN\".",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/google/Terraform-Google-ComputeSharedVpcServiceProject/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "HostProject": {
            "type": "string",
            "description": "The ID of a host project to associate."
        },
        "Id": {
            "type": "string"
        },
        "ServiceProject": {
            "type": "string",
            "description": "The ID of the project that will serve as a Shared VPC service project."
        }
    },
    "additionalProperties": false,
    "required": [
        "HostProject",
        "ServiceProject"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}