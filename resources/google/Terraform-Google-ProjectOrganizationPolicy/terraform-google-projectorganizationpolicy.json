{
    "typeName": "Terraform::Google::ProjectOrganizationPolicy",
    "description": "Allows management of Organization policies for a Google Project. For more information see\n[the official\ndocumentation](https://cloud.google.com/resource-manager/docs/organization-policy/overview) and\n[API](https://cloud.google.com/resource-manager/reference/rest/v1/projects/setOrgPolicy).",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "BooleanPolicy": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Enforced": {
                    "type": "boolean",
                    "description": "If true, then the Policy is enforced. If false, then any configuration is acceptable."
                }
            },
            "required": [
                "Enforced"
            ]
        },
        "ListPolicy": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "InheritFromParent": {
                    "type": "boolean",
                    "description": "If set to true, the values from the effective Policy of the parent resource\nare inherited, meaning the values set in this Policy are added to the values inherited up the hierarchy."
                },
                "SuggestedValue": {
                    "type": "string",
                    "description": "The Google Cloud Console will try to default to a configuration that matches the value specified in this field."
                },
                "Allow": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/Allow"
                    },
                    "maxItems": 1
                },
                "Deny": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/Deny"
                    },
                    "maxItems": 1
                }
            },
            "required": []
        },
        "RestorePolicy": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Default": {
                    "type": "boolean",
                    "description": "May only be set to true. If set, then the default Policy is restored."
                }
            },
            "required": [
                "Default"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Read": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        },
        "Allow": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "All": {
                    "type": "boolean",
                    "description": "The policy allows or denies all values."
                },
                "Values": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    },
                    "description": "The policy can define specific values that are allowed or denied."
                }
            },
            "required": []
        },
        "Deny": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "All": {
                    "type": "boolean"
                },
                "Values": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Constraint": {
            "type": "string",
            "description": "The name of the Constraint the Policy is configuring, for example, `serviceuser.services`. Check out the [complete list of available constraints](https://cloud.google.com/resource-manager/docs/organization-policy/understanding-constraints#available_constraints)."
        },
        "Etag": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Project": {
            "type": "string",
            "description": "The project id of the project to set the policy for."
        },
        "UpdateTime": {
            "type": "string"
        },
        "Version": {
            "type": "number",
            "description": "Version of the Policy. Default version is 0."
        },
        "BooleanPolicy": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/BooleanPolicy"
            },
            "maxItems": 1
        },
        "ListPolicy": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/ListPolicy"
            },
            "maxItems": 1
        },
        "RestorePolicy": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/RestorePolicy"
            },
            "maxItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        },
        "Allow": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Allow"
            },
            "maxItems": 1
        },
        "Deny": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Deny"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Constraint",
        "Project"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Etag",
        "/properties/Id",
        "/properties/UpdateTime"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}