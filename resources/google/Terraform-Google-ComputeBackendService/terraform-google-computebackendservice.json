                },
                "MaxConnections": {
                    "type": "number"
                },
                "MaxConnectionsPerEndpoint": {
                    "type": "number"
                },
                "MaxConnectionsPerInstance": {
                    "type": "number"
                },
                "MaxRate": {
                    "type": "number"
                },
                "MaxRatePerEndpoint": {
                    "type": "number"
                },
                "MaxRatePerInstance": {
                    "type": "number"
                },
                "MaxUtilization": {
                    "type": "number"
                }
            },
            "required": [
                "Group"
            ]
        },
        "CdnPolicy": {
            "type": "object",
