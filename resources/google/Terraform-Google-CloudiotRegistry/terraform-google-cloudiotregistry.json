{
    "typeName": "Terraform::Google::CloudiotRegistry",
    "description": "CloudFormation equivalent of google_cloudiot_registry",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Credentials": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "PublicKeyCertificate": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "properties": {
                            "Key": {
                                "type": "string"
                            },
                            "Value": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "Key",
                            "Value"
                        ]
                    }
                }
            },
            "required": [
                "PublicKeyCertificate"
            ]
        },
        "EventNotificationConfigs": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "PubsubTopicName": {
                    "type": "string"
                },
                "SubfolderMatches": {
                    "type": "string"
                }
            },
            "required": [
                "PubsubTopicName"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "EventNotificationConfig": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "HttpConfig": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "Id": {
            "type": "string"
        },
        "LogLevel": {
            "type": "string"
        },
        "MqttConfig": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "Name": {
            "type": "string"
        },
        "Project": {
            "type": "string"
        },
        "Region": {
            "type": "string"
        },
        "StateNotificationConfig": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "Credentials": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Credentials"
            },
            "maxItems": 10
        },
        "EventNotificationConfigs": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/EventNotificationConfigs"
            },
            "maxItems": 10
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}