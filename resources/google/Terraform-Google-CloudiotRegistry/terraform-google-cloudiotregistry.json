{
    "typeName": "Terraform::Google::CloudiotRegistry",
    "description": "Creates a device registry in Google's Cloud IoT Core platform. For more information see\n[the official documentation](https://cloud.google.com/iot/docs/) and\n[API](https://cloud.google.com/iot/docs/reference/cloudiot/rest/v1/projects.locations.registries).",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Credentials": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "PublicKeyCertificate": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "properties": {
                            "MapKey": {
                                "type": "string"
                            },
                            "MapValue": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "MapKey",
                            "MapValue"
                        ]
                    },
                    "description": "The certificate format and data."
                }
            },
            "required": [
                "PublicKeyCertificate"
            ]
        },
        "EventNotificationConfigs": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "PubsubTopicName": {
                    "type": "string",
                    "description": "PubSub topic name to publish device events."
                },
                "SubfolderMatches": {
                    "type": "string",
                    "description": "If the subfolder name matches this string\nexactly, this configuration will be used. The string must not include the\nleading '/' character. If empty, all strings are matched. Empty value can\nonly be used for the last `event_notification_configs` item."
                }
            },
            "required": [
                "PubsubTopicName"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "EventNotificationConfig": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            }
        },
        "HttpConfig": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "Activate or deactivate HTTP. Structure is documented below."
        },
        "Id": {
            "type": "string"
        },
        "LogLevel": {
            "type": "string"
        },
        "MqttConfig": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "Activate or deactivate MQTT. Structure is documented below."
        },
        "Name": {
            "type": "string",
            "description": "A unique name for the resource, required by device registry.\nChanging this forces a new resource to be created."
        },
        "Project": {
            "type": "string",
            "description": "The project in which the resource belongs. If it is not provided, the provider project is used."
        },
        "Region": {
            "type": "string",
            "description": "The Region in which the created address should reside. If it is not provided, the provider region is used."
        },
        "StateNotificationConfig": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "A PubSub topic to publish device state updates. Structure is documented below."
        },
        "Credentials": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Credentials"
            },
            "maxItems": 10
        },
        "EventNotificationConfigs": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/EventNotificationConfigs"
            },
            "maxItems": 10
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}