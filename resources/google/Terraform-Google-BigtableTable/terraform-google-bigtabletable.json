{
    "typeName": "Terraform::Google::BigtableTable",
    "description": "CloudFormation equivalent of google_bigtable_table",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "ColumnFamily": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Family": {
                    "type": "string"
                }
            },
            "required": [
                "Family"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "InstanceName": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "Project": {
            "type": "string"
        },
        "SplitKeys": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "ColumnFamily": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/ColumnFamily"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "InstanceName",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}