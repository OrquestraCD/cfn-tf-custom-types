            "additionalProperties": false,
            "properties": {
                "Ttl": {
                    "type": "string"
                }
            },
            "required": [
                "Ttl"
            ]
        },
        "PushConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Attributes": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "properties": {
                            "MapKey": {
                                "type": "string"
                            },
                            "MapValue": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "MapKey",
                            "MapValue"
                        ]
                    }
                },
                "PushEndpoint": {
                    "type": "string"
                },
                "OidcToken": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/OidcToken"
                    },
                    "maxItems": 1
                }
            },
            "required": [
                "PushEndpoint"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
