{
    "typeName": "Terraform::Google::PubsubSubscription",
    "description": "CloudFormation equivalent of google_pubsub_subscription",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "ExpirationPolicy": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Ttl": {
                    "type": "string"
                }
            },
            "required": [
                "Ttl"
            ]
        },
        "PushConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Attributes": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "properties": {
                            "Key": {
                                "type": "string"
                            },
                            "Value": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "Key",
                            "Value"
                        ]
                    }
                },
                "PushEndpoint": {
                    "type": "string"
                },
                "OidcToken": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/OidcToken"
                    }
                }
            },
            "required": [
                "PushEndpoint"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        },
        "OidcToken": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Audience": {
                    "type": "string"
                },
                "ServiceAccountEmail": {
                    "type": "string"
                }
            },
            "required": [
                "ServiceAccountEmail"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AckDeadlineSeconds": {
            "type": "number"
        },
        "Id": {
            "type": "string"
        },
        "Labels": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "MessageRetentionDuration": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "Path": {
            "type": "string"
        },
        "Project": {
            "type": "string"
        },
        "RetainAckedMessages": {
            "type": "boolean"
        },
        "Topic": {
            "type": "string"
        },
        "ExpirationPolicy": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/ExpirationPolicy"
            },
            "maxItems": 1
        },
        "PushConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/PushConfig"
            },
            "maxItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        },
        "OidcToken": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/OidcToken"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "Topic"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Path"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}