{
    "typeName": "Terraform::Google::LoggingFolderSink",
    "description": "CloudFormation equivalent of google_logging_folder_sink",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "BigqueryOptions": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "UsePartitionedTables": {
                    "type": "boolean"
                }
            },
            "required": [
                "UsePartitionedTables"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Destination": {
            "type": "string"
        },
        "Filter": {
            "type": "string"
        },
        "Folder": {
            "type": "string"
        },
        "IncludeChildren": {
            "type": "boolean"
        },
        "Name": {
            "type": "string"
        },
        "WriterIdentity": {
            "type": "string"
        },
        "BigqueryOptions": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/BigqueryOptions"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Destination",
        "Folder",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/WriterIdentity"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
