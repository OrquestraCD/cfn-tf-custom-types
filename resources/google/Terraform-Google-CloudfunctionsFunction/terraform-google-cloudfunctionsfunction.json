{
    "typeName": "Terraform::Google::CloudfunctionsFunction",
    "description": "CloudFormation equivalent of google_cloudfunctions_function",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "EventTrigger": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "EventType": {
                    "type": "string"
                },
                "Resource": {
                    "type": "string"
                },
                "FailurePolicy": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/FailurePolicy"
                    }
                }
            },
            "required": [
                "EventType",
                "Resource"
            ]
        },
        "SourceRepository": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Url": {
                    "type": "string"
                }
            },
            "required": [
                "Url"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Read": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        },
        "FailurePolicy": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Retry": {
                    "type": "boolean"
                }
            },
            "required": [
                "Retry"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AvailableMemoryMb": {
            "type": "number"
        },
        "Description": {
            "type": "string"
        },
        "EntryPoint": {
            "type": "string"
        },
        "EnvironmentVariables": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "HttpsTriggerUrl": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Labels": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "MaxInstances": {
            "type": "number"
        },
        "Name": {
            "type": "string"
        },
        "Project": {
            "type": "string"
        },
        "Region": {
            "type": "string"
        },
        "Runtime": {
            "type": "string"
        },
        "ServiceAccountEmail": {
            "type": "string"
        },
        "SourceArchiveBucket": {
            "type": "string"
        },
        "SourceArchiveObject": {
            "type": "string"
        },
        "Timeout": {
            "type": "number"
        },
        "TriggerHttp": {
            "type": "boolean"
        },
        "VpcConnector": {
            "type": "string"
        },
        "EventTrigger": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/EventTrigger"
            },
            "maxItems": 1
        },
        "SourceRepository": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/SourceRepository"
            },
            "maxItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        },
        "FailurePolicy": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/FailurePolicy"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "Runtime"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
