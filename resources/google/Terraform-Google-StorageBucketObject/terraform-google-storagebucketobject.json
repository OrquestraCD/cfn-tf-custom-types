            "type": "string"
        },
        "Content": {
            "type": "string"
        },
        "ContentDisposition": {
            "type": "string"
        },
        "ContentEncoding": {
            "type": "string"
        },
        "ContentLanguage": {
            "type": "string"
        },
        "ContentType": {
            "type": "string"
        },
        "Crc32c": {
            "type": "string"
        },
        "DetectMd5hash": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Md5hash": {
            "type": "string"
        },
        "Metadata": {
            "type": "array",
