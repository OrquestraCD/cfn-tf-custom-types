{
    "typeName": "Terraform::Google::DataprocAutoscalingPolicy",
    "description": "CloudFormation equivalent of google_dataproc_autoscaling_policy",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "BasicAlgorithm": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "CooldownPeriod": {
                    "type": "string"
                },
                "YarnConfig": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/YarnConfig"
                    }
                }
            },
            "required": []
        },
        "SecondaryWorkerConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MaxInstances": {
                    "type": "number"
                },
                "MinInstances": {
                    "type": "number"
                },
                "Weight": {
                    "type": "number"
                }
            },
            "required": []
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        },
        "WorkerConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MaxInstances": {
                    "type": "number"
                },
                "MinInstances": {
                    "type": "number"
                },
                "Weight": {
                    "type": "number"
                }
            },
            "required": [
                "MaxInstances"
            ]
        },
        "YarnConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "GracefulDecommissionTimeout": {
                    "type": "string"
                },
                "ScaleDownFactor": {
                    "type": "number"
                },
                "ScaleDownMinWorkerFraction": {
                    "type": "number"
                },
                "ScaleUpFactor": {
                    "type": "number"
                },
                "ScaleUpMinWorkerFraction": {
                    "type": "number"
                }
            },
            "required": [
                "GracefulDecommissionTimeout",
                "ScaleDownFactor",
                "ScaleUpFactor"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Location": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "PolicyId": {
            "type": "string"
        },
        "Project": {
            "type": "string"
        },
        "BasicAlgorithm": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/BasicAlgorithm"
            },
            "maxItems": 1
        },
        "SecondaryWorkerConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/SecondaryWorkerConfig"
            },
            "maxItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        },
        "WorkerConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/WorkerConfig"
            },
            "maxItems": 1
        },
        "YarnConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/YarnConfig"
            },
            "maxItems": 1,
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "PolicyId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Name"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}