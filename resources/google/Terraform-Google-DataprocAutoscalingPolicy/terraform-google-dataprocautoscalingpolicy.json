{
    "typeName": "Terraform::Google::DataprocAutoscalingPolicy",
    "description": "Describes an autoscaling policy for Dataproc cluster autoscaler.\n\n\n\n<div class = \"oics-button\" style=\"float: right; margin: 0 0 -15px\">\n  <a href=\"https://console.cloud.google.com/cloudshell/open?cloudshell_git_repo=https%3A%2F%2Fgithub.com%2Fterraform-google-modules%2Fdocs-examples.git&cloudshell_working_dir=dataproc_autoscaling_policy&cloudshell_image=gcr.io%2Fgraphite-cloud-shell-images%2Fterraform%3Alatest&open_in_editor=main.tf&cloudshell_print=.%2Fmotd&cloudshell_tutorial=.%2Ftutorial.md\" target=\"_blank\">\n    <img alt=\"Open in Cloud Shell\" src=\"//gstatic.com/cloudssh/images/open-btn.svg\" style=\"max-height: 44px; margin: 32px auto; max-width: 100%;\">\n  </a>\n</div>",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "BasicAlgorithm": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "CooldownPeriod": {
                    "type": "string"
                },
                "YarnConfig": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/YarnConfig"
                    },
                    "maxItems": 1,
                    "minItems": 1
                }
            },
            "required": []
        },
        "SecondaryWorkerConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MaxInstances": {
                    "type": "number"
                },
                "MinInstances": {
                    "type": "number"
                },
                "Weight": {
                    "type": "number"
                }
            },
            "required": []
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        },
        "WorkerConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MaxInstances": {
                    "type": "number"
                },
                "MinInstances": {
                    "type": "number"
                },
                "Weight": {
                    "type": "number"
                }
            },
            "required": [
                "MaxInstances"
            ]
        },
        "YarnConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "GracefulDecommissionTimeout": {
                    "type": "string"
                },
                "ScaleDownFactor": {
                    "type": "number"
                },
                "ScaleDownMinWorkerFraction": {
                    "type": "number"
                },
                "ScaleUpFactor": {
                    "type": "number"
                },
                "ScaleUpMinWorkerFraction": {
                    "type": "number"
                }
            },
            "required": [
                "GracefulDecommissionTimeout",
                "ScaleDownFactor",
                "ScaleUpFactor"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Location": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "PolicyId": {
            "type": "string"
        },
        "Project": {
            "type": "string",
            "description": "The ID of the project in which the resource belongs.\nIf it is not provided, the provider project is used."
        },
        "BasicAlgorithm": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/BasicAlgorithm"
            },
            "maxItems": 1
        },
        "SecondaryWorkerConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/SecondaryWorkerConfig"
            },
            "maxItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        },
        "WorkerConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/WorkerConfig"
            },
            "maxItems": 1
        },
        "YarnConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/YarnConfig"
            },
            "maxItems": 1,
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "PolicyId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/Name"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}