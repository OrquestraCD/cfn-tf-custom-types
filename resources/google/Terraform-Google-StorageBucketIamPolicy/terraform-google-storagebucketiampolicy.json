{
    "typeName": "Terraform::Google::StorageBucketIamPolicy",
    "description": "CloudFormation equivalent of google_storage_bucket_iam_policy",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Bucket": {
            "type": "string"
        },
        "Etag": {
            "type": "string"
        },
        "PolicyData": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "Bucket",
        "PolicyData"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Etag"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}