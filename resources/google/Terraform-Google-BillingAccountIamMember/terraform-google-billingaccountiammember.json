{
    "typeName": "Terraform::Google::BillingAccountIamMember",
    "description": "Allows creation and management of a single member for a single binding within\nthe IAM policy for an existing Google Cloud Platform Billing Account.\n\n~> **Note:** This resource __must not__ be used in conjunction with\n   `google_billing_account_iam_binding` for the __same role__ or they will fight over\n   what your policy should be.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "BillingAccountId": {
            "type": "string",
            "description": "The billing account id."
        },
        "Etag": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Member": {
            "type": "string",
            "description": "The user that the role should apply to. For more details on format and restrictions see https://cloud.google.com/billing/reference/rest/v1/Policy#Binding."
        },
        "Role": {
            "type": "string",
            "description": "The role that should be applied."
        }
    },
    "additionalProperties": false,
    "required": [
        "BillingAccountId",
        "Member",
        "Role"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Etag",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}