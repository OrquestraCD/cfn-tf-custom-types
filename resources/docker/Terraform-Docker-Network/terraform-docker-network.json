{
    "typeName": "Terraform::Docker::Network",
    "description": "Manages a Docker Network. This can be used alongside\n[docker\\_container](/docs/providers/docker/r/container.html)\nto create virtual networks within the docker environment.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "IpamConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AuxAddress": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "properties": {
                            "Key": {
                                "type": "string"
                            },
                            "Value": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "Key",
                            "Value"
                        ]
                    }
                },
                "Gateway": {
                    "type": "string"
                },
                "IpRange": {
                    "type": "string"
                },
                "Subnet": {
                    "type": "string"
                }
            },
            "required": []
        },
        "Labels": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Label": {
                    "type": "string"
                },
                "Value": {
                    "type": "string"
                }
            },
            "required": [
                "Label",
                "Value"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Attachable": {
            "type": "boolean",
            "description": "Enable manual container attachment to the network.\nDefaults to `false`."
        },
        "CheckDuplicate": {
            "type": "boolean",
            "description": "Requests daemon to check for networks\nwith same name."
        },
        "Driver": {
            "type": "string",
            "description": "Name of the network driver to use. Defaults to\n`bridge` driver."
        },
        "Ingress": {
            "type": "boolean",
            "description": "Create swarm routing-mesh network.\nDefaults to `false`."
        },
        "Internal": {
            "type": "boolean",
            "description": "Restrict external access to the network.\nDefaults to `false`."
        },
        "IpamDriver": {
            "type": "string",
            "description": "Driver used by the custom IP scheme of the\nnetwork."
        },
        "Ipv6": {
            "type": "boolean",
            "description": "Enable IPv6 networking.\nDefaults to `false`."
        },
        "Name": {
            "type": "string",
            "description": "The name of the Docker network."
        },
        "Options": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            },
            "description": "Network specific options to be used by\nthe drivers."
        },
        "Scope": {
            "type": "string"
        },
        "IpamConfig": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/IpamConfig"
            }
        },
        "Labels": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Labels"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Scope"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}