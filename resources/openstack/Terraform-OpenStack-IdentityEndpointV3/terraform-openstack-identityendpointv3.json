{
    "typeName": "Terraform::OpenStack::IdentityEndpointV3",
    "description": "Manages a V3 Endpoint resource within OpenStack Keystone.\n\n~> **Note:** This usually requires admin privileges.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "EndpointRegion": {
            "type": "string",
            "description": "The endpoint region. The `region` and\n`endpoint_region` can be different."
        },
        "Id": {
            "type": "string"
        },
        "Interface": {
            "type": "string",
            "description": "The endpoint interface. Valid values are `public`,\n`internal` and `admin`. Default value is `public`."
        },
        "Name": {
            "type": "string",
            "description": "The endpoint name."
        },
        "Region": {
            "type": "string",
            "description": "The region in which to obtain the V3 Keystone client.\nIf omitted, the `region` argument of the provider is used."
        },
        "ServiceId": {
            "type": "string",
            "description": "The endpoint service ID."
        },
        "ServiceName": {
            "type": "string"
        },
        "ServiceType": {
            "type": "string"
        },
        "Url": {
            "type": "string",
            "description": "The endpoint url."
        }
    },
    "additionalProperties": false,
    "required": [
        "EndpointRegion",
        "ServiceId",
        "Url"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/ServiceName",
        "/properties/ServiceType"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}