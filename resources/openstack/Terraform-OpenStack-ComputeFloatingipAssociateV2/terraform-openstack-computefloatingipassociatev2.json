{
    "typeName": "Terraform::OpenStack::ComputeFloatingipAssociateV2",
    "description": "Associate a floating IP to an instance. This can be used instead of the\n`floating_ip` options in `openstack_compute_instance_v2`.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/openstack/Terraform-OpenStack-ComputeFloatingipAssociateV2/docs/README.md",
    "definitions": {
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "FixedIp": {
            "type": "string",
            "description": "The specific IP address to direct traffic to."
        },
        "FloatingIp": {
            "type": "string",
            "description": "The floating IP to associate."
        },
        "Id": {
            "type": "string"
        },
        "InstanceId": {
            "type": "string",
            "description": "The instance to associte the floating IP with."
        },
        "Region": {
            "type": "string",
            "description": "The region in which to obtain the V2 Compute client.\nKeypairs are associated with accounts, but a Compute client is needed to\ncreate one. If omitted, the `region` argument of the provider is used.\nChanging this creates a new floatingip_associate."
        },
        "WaitUntilAssociated": {
            "type": "boolean",
            "description": "In cases where the OpenStack environment\ndoes not automatically wait until the association has finished, set this\noption to have Terraform poll the instance until the floating IP has been\nassociated. Defaults to false."
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "FloatingIp",
        "InstanceId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
