{
    "typeName": "Terraform::OpenStack::IdentityApplicationCredentialV3",
    "description": "CloudFormation equivalent of openstack_identity_application_credential_v3",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "AccessRules": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Method": {
                    "type": "string"
                },
                "Path": {
                    "type": "string"
                },
                "Service": {
                    "type": "string"
                }
            },
            "required": [
                "Method",
                "Path",
                "Service"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string"
        },
        "ExpiresAt": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "ProjectId": {
            "type": "string"
        },
        "Region": {
            "type": "string"
        },
        "Roles": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Secret": {
            "type": "string"
        },
        "Unrestricted": {
            "type": "boolean"
        },
        "AccessRules": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/AccessRules"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/ProjectId"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}