{
    "typeName": "Terraform::OpenStack::ImagesImageAccessAcceptV2",
    "description": "Manages memberships status for the shared OpenStack Glance V2 Image within the\ndestination project, which has a member proposal.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CreatedAt": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "ImageId": {
            "type": "string",
            "description": "The proposed image ID."
        },
        "MemberId": {
            "type": "string",
            "description": "The member ID, e.g. the target project ID. Optional\nfor admin accounts. Defaults to the current scope project ID."
        },
        "Region": {
            "type": "string",
            "description": "The region in which to obtain the V2 Glance client.\nA Glance client is needed to manage Image memberships. If omitted, the\n`region` argument of the provider is used. Changing this creates a new\nmembership."
        },
        "Schema": {
            "type": "string"
        },
        "Status": {
            "type": "string",
            "description": "The membership proposal status. Can either be\n`accepted`, `rejected` or `pending`."
        },
        "UpdatedAt": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "ImageId",
        "Status"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/CreatedAt",
        "/properties/Id",
        "/properties/Schema",
        "/properties/UpdatedAt"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}