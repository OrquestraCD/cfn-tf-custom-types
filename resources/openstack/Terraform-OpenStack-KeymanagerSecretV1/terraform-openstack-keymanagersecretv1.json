{
    "typeName": "Terraform::OpenStack::KeymanagerSecretV1",
    "description": "Manages a V1 Barbican secret resource within OpenStack.\n\n~> **Important Security Notice** The payload of this resource will be stored\n*unencrypted* in your Terraform state file. **Use of this resource for production\ndeployments is *not* recommended**.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Acl": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Read": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/Read"
                    }
                }
            },
            "required": []
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        },
        "Read": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "ProjectAccess": {
                    "type": "boolean"
                },
                "Users": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Algorithm": {
            "type": "string",
            "description": "Metadata provided by a user or system for informational purposes."
        },
        "AllMetadata": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "BitLength": {
            "type": "number",
            "description": "Metadata provided by a user or system for informational purposes."
        },
        "ContentTypes": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "CreatedAt": {
            "type": "string"
        },
        "CreatorId": {
            "type": "string"
        },
        "Expiration": {
            "type": "string",
            "description": "The expiration time of the secret in the RFC3339 timestamp format (e.g. `2019-03-09T12:58:49Z`). If omitted, a secret will never expire. Changing this creates a new secret."
        },
        "Metadata": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            },
            "description": "Additional Metadata for the secret."
        },
        "Mode": {
            "type": "string",
            "description": "Metadata provided by a user or system for informational purposes."
        },
        "Name": {
            "type": "string",
            "description": "Human-readable name for the Secret. Does not have\nto be unique."
        },
        "Payload": {
            "type": "string",
            "description": "The secret's data to be stored. **payload\\_content\\_type** must also be supplied if **payload** is included."
        },
        "PayloadContentEncoding": {
            "type": "string",
            "description": "(required if **payload** is encoded) The encoding used for the payload to be able to include it in the JSON request. Must be either `base64` or `binary`."
        },
        "PayloadContentType": {
            "type": "string",
            "description": "(required if **payload** is included) The media type for the content of the payload. Must be one of `text/plain`, `text/plain;charset=utf-8`, `text/plain; charset=utf-8`, `application/octet-stream`, `application/pkcs8`."
        },
        "Region": {
            "type": "string",
            "description": "The region in which to obtain the V1 KeyManager client.\nA KeyManager client is needed to create a secret. If omitted, the\n`region` argument of the provider is used. Changing this creates a new\nV1 secret."
        },
        "SecretRef": {
            "type": "string"
        },
        "SecretType": {
            "type": "string",
            "description": "Used to indicate the type of secret being stored. For more information see [Secret types](https://docs.openstack.org/barbican/latest/api/reference/secret_types.html)."
        },
        "Status": {
            "type": "string"
        },
        "UpdatedAt": {
            "type": "string"
        },
        "Acl": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Acl"
            },
            "maxItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        },
        "Read": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Read"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/AllMetadata",
        "/properties/ContentTypes",
        "/properties/CreatedAt",
        "/properties/CreatorId",
        "/properties/SecretRef",
        "/properties/Status",
        "/properties/UpdatedAt"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}