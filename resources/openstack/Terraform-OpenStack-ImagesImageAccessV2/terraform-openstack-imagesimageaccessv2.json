{
    "typeName": "Terraform::OpenStack::ImagesImageAccessV2",
    "description": "Manages members for the shared OpenStack Glance V2 Image within the source\nproject, which owns the Image.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CreatedAt": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "ImageId": {
            "type": "string",
            "description": "The image ID."
        },
        "MemberId": {
            "type": "string",
            "description": "The member ID, e.g. the target project ID."
        },
        "Region": {
            "type": "string",
            "description": "The region in which to obtain the V2 Glance client.\nA Glance client is needed to manage Image members. If omitted, the `region`\nargument of the provider is used. Changing this creates a new resource."
        },
        "Schema": {
            "type": "string"
        },
        "Status": {
            "type": "string",
            "description": "The member proposal status. Optional if admin wants to\nforce the member proposal acceptance. Can either be `accepted`, `rejected` or\n`pending`. Defaults to `pending`. Foridden for non-admin users."
        },
        "UpdatedAt": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "ImageId",
        "MemberId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/CreatedAt",
        "/properties/Id",
        "/properties/Schema",
        "/properties/UpdatedAt"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}