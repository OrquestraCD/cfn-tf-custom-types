                    "items": {
                        "$ref": "#/definitions/Read"
                    },
                    "maxItems": 1
                }
            },
            "required": []
        },
        "SecretRefs": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Name": {
                    "type": "string",
                    "description": "The name of the secret reference. The reference names must correspond the container type, more details are available [here](https://docs.openstack.org/barbican/stein/api/reference/containers.html)."
                },
                "SecretRef": {
                    "type": "string",
                    "description": "The secret reference / where to find the secret, URL."
                }
            },
            "required": [
                "SecretRef"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        },
        "Read": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "ProjectAccess": {
                    "type": "boolean"
                },
                "Users": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Consumers": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Name": {
                        "type": "string"
                    },
                    "Url": {
                        "type": "string"
                    }
                }
            }
        },
        "ContainerRef": {
            "type": "string"
        },
        "CreatedAt": {
            "type": "string"
        },
        "CreatorId": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "Human-readable name for the Container. Does not have\nto be unique."
        },
        "Region": {
            "type": "string",
            "description": "The region in which to obtain the V1 KeyManager client.\nA KeyManager client is needed to create a container. If omitted, the\n`region` argument of the provider is used. Changing this creates a new\nV1 container."
        },
        "Status": {
            "type": "string"
        },
        "Type": {
            "type": "string",
            "description": "Used to indicate the type of container. Must be one of `generic`, `rsa` or `certificate`."
        },
        "UpdatedAt": {
            "type": "string"
        },
        "Acl": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Acl"
            },
            "maxItems": 1
        },
        "SecretRefs": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/SecretRefs"
            }
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
