{
    "typeName": "Terraform::OpenStack::IdentityUserV3",
    "description": "Manages a V3 User resource within OpenStack Keystone.\n\nNote: You _must_ have admin privileges in your OpenStack cloud to use\nthis resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "MultiFactorAuthRule": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Rule": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    },
                    "description": "A list of authentication plugins that the user must\nauthenticate with."
                }
            },
            "required": [
                "Rule"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "DefaultProjectId": {
            "type": "string",
            "description": "The default project this user belongs to."
        },
        "Description": {
            "type": "string",
            "description": "A description of the user."
        },
        "DomainId": {
            "type": "string",
            "description": "The domain this user belongs to."
        },
        "Enabled": {
            "type": "boolean",
            "description": "Whether the user is enabled or disabled. Valid\nvalues are `true` and `false`."
        },
        "Extra": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "Free-form key/value pairs of extra information."
        },
        "Id": {
            "type": "string"
        },
        "IgnoreChangePasswordUponFirstUse": {
            "type": "boolean",
            "description": "User will not have to\nchange their password upon first use. Valid values are `true` and `false`."
        },
        "IgnoreLockoutFailureAttempts": {
            "type": "boolean",
            "description": "User will not have a failure\nlockout placed on their account. Valid values are `true` and `false`."
        },
        "IgnorePasswordExpiry": {
            "type": "boolean",
            "description": "User's password will not expire.\nValid values are `true` and `false`."
        },
        "MultiFactorAuthEnabled": {
            "type": "boolean",
            "description": "Whether to enable multi-factor\nauthentication. Valid values are `true` and `false`."
        },
        "Name": {
            "type": "string",
            "description": "The name of the user."
        },
        "Password": {
            "type": "string",
            "description": "The password for the user."
        },
        "Region": {
            "type": "string",
            "description": "The region in which to obtain the V3 Keystone client.\nIf omitted, the `region` argument of the provider is used. Changing this\ncreates a new User."
        },
        "MultiFactorAuthRule": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/MultiFactorAuthRule"
            }
        }
    },
    "additionalProperties": false,
    "required": [],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}