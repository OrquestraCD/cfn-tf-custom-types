{
    "typeName": "Terraform::OpenStack::IdentityServiceV3",
    "description": "Manages a V3 Service resource within OpenStack Keystone.\n\n~> **Note:** This usually requires admin privileges.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "The service description."
        },
        "Enabled": {
            "type": "boolean",
            "description": "The service status. Defaults to `true`."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The service name."
        },
        "Region": {
            "type": "string",
            "description": "The region in which to obtain the V3 Keystone client.\nIf omitted, the `region` argument of the provider is used."
        },
        "Type": {
            "type": "string",
            "description": "The service type."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "Type"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}