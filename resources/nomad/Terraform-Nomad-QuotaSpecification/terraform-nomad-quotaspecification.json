{
    "typeName": "Terraform::Nomad::QuotaSpecification",
    "description": "Manages a quota specification in a Nomad cluster.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Limits": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Region": {
                    "type": "string"
                },
                "RegionLimit": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "$ref": "#/definitions/RegionLimit"
                    }
                }
            },
            "required": [
                "Region"
            ]
        },
        "RegionLimit": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Cpu": {
                    "type": "number"
                },
                "MemoryMb": {
                    "type": "number"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "Limits": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Limits"
            },
            "minItems": 1
        },
        "RegionLimit": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/RegionLimit"
            },
            "maxItems": 1,
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}