{
    "typeName": "Terraform::NewRelic::SyntheticsSecureCredential",
    "description": "Use this resource to create and manage New Relic Synthetic secure credentials.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CreatedAt": {
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "The secure credential's description."
        },
        "Id": {
            "type": "string"
        },
        "Key": {
            "type": "string",
            "description": "The secure credential's key name.  Regardless of the case used in the configuration, the provider will provide an upcased key to the underlying API.\n* `value` - (Required) The secure credential's value.\n* `description` - (Optional) The secure credential's description."
        },
        "LastUpdated": {
            "type": "string"
        },
        "Value": {
            "type": "string",
            "description": "The secure credential's value.\n* `description` - (Optional) The secure credential's description."
        }
    },
    "additionalProperties": false,
    "required": [
        "Key",
        "Value"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}