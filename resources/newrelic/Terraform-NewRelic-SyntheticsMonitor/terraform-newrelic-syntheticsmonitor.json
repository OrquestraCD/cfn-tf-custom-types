{
    "typeName": "Terraform::NewRelic::SyntheticsMonitor",
    "description": "CloudFormation equivalent of newrelic_synthetics_monitor",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
