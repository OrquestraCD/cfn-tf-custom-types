        },
        "BypassHeadRequest": {
            "type": "boolean"
        },
        "Frequency": {
            "type": "number"
        },
        "Id": {
            "type": "string"
        },
        "Locations": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Name": {
            "type": "string"
        },
        "SlaThreshold": {
            "type": "number"
        },
        "Status": {
            "type": "string"
        },
        "TreatRedirectAsFailure": {
            "type": "boolean"
        },
        "Type": {
            "type": "string"
        },
        "Uri": {
            "type": "string"
        },
        "ValidationString": {
            "type": "string"
        },
        "VerifySsl": {
            "type": "boolean"
        }
    },
    "additionalProperties": false,
    "required": [
        "Frequency",
        "Locations",
        "Name",
        "Status",
        "Type"
    ],
