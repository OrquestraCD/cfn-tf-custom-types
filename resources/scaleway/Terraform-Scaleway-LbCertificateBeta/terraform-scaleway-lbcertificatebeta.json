{
    "typeName": "Terraform::Scaleway::LbCertificateBeta",
    "description": "CloudFormation equivalent of scaleway_lb_certificate_beta",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "CustomCertificate": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "CertificateChain": {
                    "type": "string"
                }
            },
            "required": [
                "CertificateChain"
            ]
        },
        "Letsencrypt": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "CommonName": {
                    "type": "string"
                },
                "SubjectAlternativeName": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                }
            },
            "required": [
                "CommonName"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CommonName": {
            "type": "string"
        },
        "Fingerprint": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "LbId": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "NotValidAfter": {
            "type": "string"
        },
        "NotValidBefore": {
            "type": "string"
        },
        "Status": {
            "type": "string"
        },
        "SubjectAlternativeName": {
            "type": "string"
        },
        "CustomCertificate": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/CustomCertificate"
            },
            "maxItems": 1
        },
        "Letsencrypt": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Letsencrypt"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "LbId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/CommonName",
        "/properties/Fingerprint",
        "/properties/NotValidAfter",
        "/properties/NotValidBefore",
        "/properties/Status",
        "/properties/SubjectAlternativeName"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}