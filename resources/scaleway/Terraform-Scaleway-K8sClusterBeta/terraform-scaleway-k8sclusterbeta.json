{
    "typeName": "Terraform::Scaleway::K8sClusterBeta",
    "description": "Creates and manages Scaleway Kubernetes clusters. For more information, see [the documentation](https://developers.scaleway.com/en/products/k8s/api/).",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "AutoUpgrade": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Enable": {
                    "type": "boolean"
                },
                "MaintenanceWindowDay": {
                    "type": "string"
                },
                "MaintenanceWindowStartHour": {
                    "type": "number"
                }
            },
            "required": [
                "Enable",
                "MaintenanceWindowDay",
                "MaintenanceWindowStartHour"
            ]
        },
        "AutoscalerConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "BalanceSimilarNodeGroups": {
                    "type": "boolean"
                },
                "DisableScaleDown": {
                    "type": "boolean"
                },
                "Estimator": {
                    "type": "string"
                },
                "Expander": {
                    "type": "string"
                },
                "ExpendablePodsPriorityCutoff": {
                    "type": "number"
                },
                "IgnoreDaemonsetsUtilization": {
                    "type": "boolean"
                },
                "ScaleDownDelayAfterAdd": {
                    "type": "string"
                }
            },
            "required": []
        },
        "DefaultPool": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Autohealing": {
                    "type": "boolean"
                },
                "Autoscaling": {
                    "type": "boolean"
                },
                "ContainerRuntime": {
                    "type": "string"
                },
                "MaxSize": {
                    "type": "number"
                },
                "MinSize": {
                    "type": "number"
                },
                "NodeType": {
                    "type": "string"
                },
                "PlacementGroupId": {
                    "type": "string"
                },
                "Size": {
                    "type": "number"
                },
                "Tags": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                },
                "WaitForPoolReady": {
                    "type": "boolean"
                }
            },
            "required": [
                "NodeType",
                "Size"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AdmissionPlugins": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "ApiserverUrl": {
            "type": "string"
        },
        "Cni": {
            "type": "string"
        },
        "CreatedAt": {
            "type": "string"
        },
        "Description": {
            "type": "string"
        },
        "EnableDashboard": {
            "type": "boolean"
        },
        "FeatureGates": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "Id": {
            "type": "string"
        },
        "Ingress": {
            "type": "string"
        },
        "Kubeconfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "ClusterCaCertificate": {
                        "type": "string"
                    },
                    "ConfigFile": {
                        "type": "string"
                    },
                    "Host": {
                        "type": "string"
                    },
                    "Token": {
                        "type": "string"
                    }
                }
            }
        },
        "Name": {
            "type": "string"
        },
        "OrganizationId": {
            "type": "string"
        },
        "Region": {
            "type": "string"
        },
        "Status": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "UpdatedAt": {
            "type": "string"
        },
        "UpgradeAvailable": {
            "type": "boolean"
        },
        "Version": {
            "type": "string"
        },
        "WildcardDns": {
            "type": "string"
        },
        "AutoUpgrade": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/AutoUpgrade"
            },
            "maxItems": 1
        },
        "AutoscalerConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/AutoscalerConfig"
            },
            "maxItems": 1
        },
        "DefaultPool": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/DefaultPool"
            },
            "maxItems": 1,
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Cni",
        "Name",
        "Version"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/ApiserverUrl",
        "/properties/CreatedAt",
        "/properties/Id",
        "/properties/Kubeconfig",
        "/properties/Status",
        "/properties/UpdatedAt",
        "/properties/UpgradeAvailable",
        "/properties/WildcardDns"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}