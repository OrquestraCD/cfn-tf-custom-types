{
    "typeName": "Terraform::Scaleway::Server",
    "description": "CloudFormation equivalent of scaleway_server",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Volume": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "SizeInGb": {
                    "type": "number"
                },
                "Type": {
                    "type": "string"
                }
            },
            "required": [
                "SizeInGb",
                "Type"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "BootType": {
            "type": "string"
        },
        "Bootscript": {
            "type": "string"
        },
        "Cloudinit": {
            "type": "string"
        },
        "DynamicIpRequired": {
            "type": "boolean"
        },
        "EnableIpv6": {
            "type": "boolean"
        },
        "Image": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "PrivateIp": {
            "type": "string"
        },
        "PublicIp": {
            "type": "string"
        },
        "PublicIpv6": {
            "type": "string"
        },
        "SecurityGroup": {
            "type": "string"
        },
        "State": {
            "type": "string"
        },
        "StateDetail": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "Type": {
            "type": "string"
        },
        "Volume": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Volume"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Image",
        "Name",
        "Type"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/PrivateIp",
        "/properties/PublicIpv6",
        "/properties/StateDetail"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}