{
    "typeName": "Terraform::InfluxDB::ContinuousQuery",
    "description": "The continuous_query resource allows a continuous query to be created on an InfluxDB server.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Database": {
            "type": "string",
            "description": "The database for the continuous_query. This must be an existing influxdb database."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name for the continuous_query. This must be unique on the InfluxDB server."
        },
        "Query": {
            "type": "string",
            "description": "The query for the continuous_query."
        }
    },
    "additionalProperties": false,
    "required": [
        "Database",
        "Name",
        "Query"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}