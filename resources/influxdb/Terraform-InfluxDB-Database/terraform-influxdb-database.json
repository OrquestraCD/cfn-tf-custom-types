{
    "typeName": "Terraform::InfluxDB::Database",
    "description": "CloudFormation equivalent of influxdb_database",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "RetentionPolicies": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Default": {
                    "type": "boolean"
                },
                "Duration": {
                    "type": "string"
                },
                "Name": {
                    "type": "string"
                },
                "Replication": {
                    "type": "number"
                },
                "Shardgroupduration": {
                    "type": "string"
                }
            },
            "required": [
                "Duration",
                "Name"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "RetentionPolicies": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/RetentionPolicies"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}