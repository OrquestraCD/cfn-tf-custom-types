{
    "typeName": "Terraform::InfluxDB::Database",
    "description": "The database resource allows a database to be created on an InfluxDB server.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/influxdb/Terraform-InfluxDB-Database/docs/README.md",
    "definitions": {
        "RetentionPolicies": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Default": {
                    "type": "boolean",
                    "description": "Marks current retention policy as default. Default value is false."
                },
                "Duration": {
                    "type": "string",
                    "description": "The duration for retention policy, format of duration can be found at InfluxDB Documentation."
                },
                "Name": {
                    "type": "string",
                    "description": "The name of the retention policy."
                },
                "Replication": {
                    "type": "number",
                    "description": "Determines how many copies of data points are stored in a cluster. Not applicable for single node / Open Source version of InfluxDB. Default value of 1."
                },
                "Shardgroupduration": {
                    "type": "string",
                    "description": "Determines how much time each shard group spans. How and why to modify can be found at InfluxDB Documentation."
                }
            },
            "required": [
                "Duration",
                "Name"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name for the database. This must be unique on the\nInfluxDB server."
        },
        "RetentionPolicies": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/RetentionPolicies"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}