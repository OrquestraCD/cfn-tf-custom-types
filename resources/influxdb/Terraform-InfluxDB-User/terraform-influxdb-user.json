{
    "typeName": "Terraform::InfluxDB::User",
    "description": "The user resource allows a user to be created on an InfluxDB server.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Grant": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Database": {
                    "type": "string",
                    "description": "The name of the database the privilege is associated with."
                },
                "Privilege": {
                    "type": "string",
                    "description": "The privilege to grant (READ|WRITE|ALL)."
                }
            },
            "required": [
                "Database",
                "Privilege"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Admin": {
            "type": "boolean",
            "description": "Mark the user as admin."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name for the user."
        },
        "Password": {
            "type": "string",
            "description": "The password for the user."
        },
        "Grant": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Grant"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "Password"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}