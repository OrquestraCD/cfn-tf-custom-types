        },
        "AutoRecovery": {
            "type": "boolean"
        },
        "AvailabilityZone": {
            "type": "string"
        },
        "ChargingMode": {
            "type": "string"
        },
        "DeleteDisksOnTermination": {
            "type": "boolean"
        },
        "EnterpriseProjectId": {
            "type": "string"
        },
        "Flavor": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "ImageId": {
            "type": "string"
        },
        "KeyName": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "OpSvcUserid": {
            "type": "string"
        },
        "Password": {
            "type": "string"
        },
        "Period": {
            "type": "number"
        },
        "PeriodUnit": {
            "type": "string"
        },
        "SecurityGroups": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "SystemDiskSize": {
            "type": "number"
        },
        "SystemDiskType": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            }
        },
        "UserData": {
            "type": "string"
        },
        "VpcId": {
            "type": "string"
        },
        "DataDisks": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/DataDisks"
            },
            "maxItems": 23
        },
        "Nics": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Nics"
            },
            "maxItems": 12,
            "minItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
