{
    "typeName": "Terraform::HuaweiCloud::NetworkingPortV2",
    "description": "Manages a V2 port resource within HuaweiCloud.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/huaweicloud/Terraform-HuaweiCloud-NetworkingPortV2/docs/README.md",
    "definitions": {
        "AllowedAddressPairs": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "IpAddress": {
                    "type": "string",
                    "description": "The additional IP address."
                },
                "MacAddress": {
                    "type": "string",
                    "description": "The additional MAC address."
                }
            },
            "required": [
                "IpAddress"
            ]
        },
        "ExtraDhcpOption": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "IpVersion": {
                    "type": "number",
                    "description": "IP protocol version. Defaults to 4."
                },
                "Name": {
                    "type": "string",
                    "description": "Name of the DHCP option."
                },
                "Value": {
                    "type": "string",
                    "description": "Value of the DHCP option."
                }
            },
            "required": [
                "Name",
                "Value"
            ]
        },
        "FixedIp": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "IpAddress": {
                    "type": "string",
                    "description": "IP address desired in the subnet for this port. If\nyou don't specify `ip_address`, an available IP address from the specified\nsubnet will be allocated to this port. This field will not be populated if it\nis left blank. To retrieve the assigned IP address, use the `all_fixed_ips`\nattribute."
                },
                "SubnetId": {
                    "type": "string",
                    "description": "Subnet in which to allocate IP address for\nthis port."
                }
            },
            "required": [
                "SubnetId"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AdminStateUp": {
            "type": "boolean",
            "description": "Administrative up/down status for the port\n(must be \"true\" or \"false\" if provided). Changing this updates the\n`admin_state_up` of an existing port."
        },
        "AllFixedIps": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "AllSecurityGroupIds": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "DeviceId": {
            "type": "string",
            "description": "The ID of the device attached to the port. Changing this\ncreates a new port."
        },
        "DeviceOwner": {
            "type": "string",
            "description": "The device owner of the Port. Changing this creates\na new port."
        },
        "Id": {
            "type": "string"
        },
        "MacAddress": {
            "type": "string",
            "description": "Specify a specific MAC address for the port. Changing\nthis creates a new port."
        },
        "Name": {
            "type": "string",
            "description": "A unique name for the port. Changing this\nupdates the `name` of an existing port."
        },
        "NetworkId": {
            "type": "string",
            "description": "The ID of the network to attach the port to. Changing\nthis creates a new port."
        },
        "NoSecurityGroups": {
            "type": "boolean",
            "description": "If set to\n`true`, then no security groups are applied to the port. If set to `false` and\nno `security_group_ids` are specified, then the Port will yield to the default\nbehavior of the Networking service, which is to usually apply the \"default\"\nsecurity group."
        },
        "Region": {
            "type": "string",
            "description": "The region in which to obtain the V2 networking client.\nA networking client is needed to create a port. If omitted, the\n`region` argument of the provider is used. Changing this creates a new\nport."
        },
        "SecurityGroupIds": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "A list\nof security group IDs to apply to the port. The security groups must be\nspecified by ID and not name (as opposed to how they are configured with\nthe Compute Instance)."
        },
        "TenantId": {
            "type": "string",
            "description": "The owner of the Port. Required if admin wants\nto create a port for another tenant. Changing this creates a new port."
        },
        "ValueSpecs": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "Map of additional options."
        },
        "AllowedAddressPairs": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/AllowedAddressPairs"
            }
        },
        "ExtraDhcpOption": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/ExtraDhcpOption"
            }
        },
        "FixedIp": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/FixedIp"
            }
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "NetworkId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/AllFixedIps",
        "/properties/AllSecurityGroupIds",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}