{
    "typeName": "Terraform::HuaweiCloud::DnsRecordsetV2",
    "description": "Manages a DNS record set in the HuaweiCloud DNS Service.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "A description of the record set."
        },
        "Name": {
            "type": "string",
            "description": "The name of the record set. Note the `.` at the end of the name.\nChanging this creates a new DNS record set."
        },
        "Records": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "An array of DNS records."
        },
        "Region": {
            "type": "string",
            "description": "The region in which to obtain the V2 DNS client.\nIf omitted, the `region` argument of the provider is used.\nChanging this creates a new DNS record set."
        },
        "Ttl": {
            "type": "number",
            "description": "The time to live (TTL) of the record set (in seconds). The value\nrange is 300\u20132147483647. The default value is 300."
        },
        "Type": {
            "type": "string",
            "description": "The type of record set. The options include `A`, `AAAA`, `MX`,\n`CNAME`, `TXT`, `NS`, `SRV`, and `PTR`. Changing this creates a new DNS record set."
        },
        "ValueSpecs": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            },
            "description": "Map of additional options. Changing this creates a\nnew record set."
        },
        "ZoneId": {
            "type": "string",
            "description": "The ID of the zone in which to create the record set.\nChanging this creates a new DNS  record set."
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "Records",
        "Type",
        "ZoneId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}