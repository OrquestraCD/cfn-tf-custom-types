{
    "typeName": "Terraform::HuaweiCloud::DmsGroupV1",
    "description": "Manages a DMS group in the huaweicloud DMS Service.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AvailableDeadletters": {
            "type": "number"
        },
        "AvailableMessages": {
            "type": "number"
        },
        "ConsumedMessages": {
            "type": "number"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "Indicates the unique name of a group. A string of 1 to 64\ncharacters that contain a-z, A-Z, 0-9, hyphens (-), and underscores (_).\nThe name cannot be modified once specified."
        },
        "ProducedDeadletters": {
            "type": "number"
        },
        "ProducedMessages": {
            "type": "number"
        },
        "QueueId": {
            "type": "string",
            "description": "Indicates the ID of a specified queue."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "QueueId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/AvailableDeadletters",
        "/properties/AvailableMessages",
        "/properties/ConsumedMessages",
        "/properties/Id",
        "/properties/ProducedDeadletters",
        "/properties/ProducedMessages"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}