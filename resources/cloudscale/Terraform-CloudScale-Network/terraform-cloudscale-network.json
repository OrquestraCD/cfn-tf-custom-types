{
    "typeName": "Terraform::CloudScale::Network",
    "description": "Provides a cloudscale.ch Private Network resource. This can be used to create, modify, and delete networks.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Href": {
            "type": "string"
        },
        "Mtu": {
            "type": "number",
            "description": "`You can specify the MTU size for the network, defaults to 9000."
        },
        "Name": {
            "type": "string",
            "description": "Name of the new network."
        },
        "Subnets": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Cidr": {
                        "type": "string"
                    },
                    "Href": {
                        "type": "string"
                    },
                    "Uuid": {
                        "type": "string"
                    }
                }
            }
        },
        "ZoneSlug": {
            "type": "string",
            "description": "You can specify a zone slug. Options include `lpg1` and `rma1`."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Href",
        "/properties/Subnets"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}